local sv = Instance.new("StringValue",music)

function newTheme(id,pitch,vol,tpos)
	music.SoundId = "rbxassetid://" .. id
	music.PlaybackSpeed = pitch or 1.015
	if bgui:FindFirstChild("UNLEASHED") and Mode["Name"] ~= "FINARE" then music.PlaybackSpeed = music.PlaybackSpeed + .075 end
	music.Volume = (vol or 1)/2
	if tpos then music.TimePosition = tpos end
end

