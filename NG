					Color3 = glowify(Color3.new(1,1,1))
				},{
					Size=Vector3.new(1024,1024,1),
					CFrame=v.CFrame*rang(90,0,0),
					Transparency = 0,
				},{
					Transparency = 1,
				},stats["ASPD"]*30/60,"Sine","In"),3)
				v.Color = Color3.new(0,0,1)
			end)
		end))
	end
	swait(stats["ASPD"]*150)
	-- ~-~ IMPOSSIBLEOVERDRIVE 5 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Back','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-1.3,-0.3)*ang(math.rad(-35),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-0.5,-1.2)*ang(math.rad(-55),math.rad(5),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-0.6,-1.1)*ang(math.rad(-55),math.rad(-5),math.rad(5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.3,-0.1,-1)*ang(math.rad(44.3),math.rad(-10.5),math.rad(-31))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,-0.1,-1)*ang(math.rad(47.2),math.rad(13.7),math.rad(26.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 = cf(0,0.4,-0.8)*ang(math.rad(50),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	marazmify(weirdeff("SpikeBall",{
		Size=Vector3.new(2048,2048,2048),
		CFrame=hb1.CFrame,
		Material = "Neon",
		Transparency = 1,
		Color = Mode["Name"] ~= "MARAZM" and Mode["Colors"][1] or Color3.new(math.random(),math.random(),math.random()),
	},{
		Transparency = 1,
	},{
		Size=Vector3.new(0,0,0),
		Transparency = -1,
		Color = Mode["Name"] ~= "MARAZM" and Mode["Colors"][2] or Color3.new(math.random(),math.random(),math.random()),
	},{
	},stats["ASPD"]*45/60,"Quad","In"),1)
	marazmify(weirdeff("SpikeBall",{
		Size=Vector3.new(2048,2048,2048),
		CFrame=hb2.CFrame,
		Material = "Neon",
		Transparency = 1,
		Color = Mode["Name"] ~= "MARAZM" and Mode["Colors"][1] or Color3.new(math.random(),math.random(),math.random()),
	},{
		Transparency = 1,
	},{
		Size=Vector3.new(100,100,100),
		Transparency = -1,
		Color = Mode["Name"] ~= "MARAZM" and Mode["Colors"][2] or Color3.new(math.random(),math.random(),math.random()),
	},{
	},stats["ASPD"]*45/60,"Quad","In"),2)
	swait(stats["ASPD"]*45)
	BOOOOOM = true
	hb3.Color = Color3.new(1,0,0)
	-- ~-~ IMPOSSIBLEOVERDRIVE 6 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Quint','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-1.1,0)*ang(math.rad(0),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-1.3,-0.9)*ang(math.rad(-90),math.rad(5),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1.3,-0.9)*ang(math.rad(-90),math.rad(-5),math.rad(5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.3,1.4,-0.1)*ang(math.rad(4.5),math.rad(-6.7),math.rad(175.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.2,1.4,0)*ang(math.rad(8.3),math.rad(8.8),math.rad(-176.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.4,-0.8)*ang(math.rad(50),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	hb3.Color = Color3.new(0,0,1)
	swait(stats["ASPD"]*60)
	-- ~-~ IMPOSSIBLEOVERDRIVE FLOP ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*90/60,'Bounce','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-2.6,0.5)*ang(math.rad(85),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.8,-0.6,-0.8)*ang(math.rad(-34.9),math.rad(7.7),math.rad(-8.1))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-0.6,-0.2)*ang(math.rad(-20.2),math.rad(-9.7),math.rad(2.4))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.3,0.9,0.2)*ang(math.rad(-12.2),math.rad(4.6),math.rad(-71.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,0.8,0.2)*ang(math.rad(-9.5),math.rad(-3),math.rad(76.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.4)*ang(math.rad(20),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(stats["ASPD"]*300)
	attack,idlesoff = false,false
	root.Anchored = false
end

function finareZ()
	attack,idlesoff = true,false
	tweenDisable = {RW,LW,RootJoint,Torso.Neck}
	local c0ler = RootJoint.C0:lerp(cf(0,Mode["flying"],-0.1)*ang(math.rad(-15),math.rad(30),math.rad(0))*RootCF,.5)
	local c0le = RootJoint.C0:lerp(cf(-0.1,Mode["flying"],-0.1)*ang(math.rad(6.7),math.rad(-58.5),math.rad(19.4))*RootCF,.5)
	local c0l = RootJoint.C0:lerp(cf(0,Mode["flying"],-0.1)*ang(math.rad(-4),math.rad(50.6),math.rad(-7.8))*RootCF,.5)
	local function PROJECTILE(power,timed,from,to,anglol)
		local e = PART()
		e.Size = Vector3.new(0,0,0)
		e.Color = Mode["Colors"][1]
		e.Material = "Neon"
		e.CFrame = CFrame.new(from.p,to.p)*cf(0,0,-5)
		local m = Instance.new("SpecialMesh",e)
		m.MeshType = "Sphere"
		local tr = TRAIL(e)
		tr.FaceCamera = false
		tr.Color = CS(Mode["Colors"][3])
		tr.Attachment0.Position = Vector3.new(2*power,0,0)
		tr.Attachment1.Position = Vector3.new(2*power,0,0)
		e.Parent = char
		particle(e.CFrame,5,{ZOffset=2,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.35),
			Size=NS({NSK(0,5,0),NSK(.5,3,0),NSK(1,0,0)}),
			Transparency = NS({NSK(0,0,0),NSK(.25,0.5,0),NSK(1,1,0)}),
			LockedToPart=true
		})
		local cfe = e.CFrame*CFrame.new(0,0,-15*power)*rang(0,-anglol,0)
		local cfe2 = cfe*rang(0,anglol or 0,0)
		local g = RTweenAnimate(e,{CFrame = cfe,Transparency=0,Color=Mode["Colors"][3],Size=Vector3.new(3*power,3*power,3*power)},.35,"Quad","Out")
		particle(e,2,{ZOffset=1,
			Texture="rbxassetid://2109052855",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(timed/60),
			Size=NS({NSK(0,50*power,0),NSK(1,0,0)}),
			Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
			LockedToPart=true
		},Vector3.new(0,0,0))
		sound(e,"588738198",1.2,.5)
		swait(45)
		particle(e,3,{ZOffset=2,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(1),
			Size=NS({NSK(0,0,0),NSK(.25,15,0),NSK(1,20,1)}),
			Transparency = NS({NSK(0,0,0),NSK(.25,0.5,0),NSK(1,1,0)}),
			LockedToPart=true
		})
		local BEAM = resources.BEAM:Clone()
		BEAM.Color = Mode["Colors"][1]
		BEAM.Mesh.Scale = Vector3.new(0,0,0)
		BEAM.Mesh.Offset = Vector3.new(0,0,0)
		e.CFrame = cfe*rang(0,90,0)
		BEAM.CFrame = e.CFrame
		BEAM.Parent = e
		RTweenAnimate(BEAM.Mesh,{Offset=Vector3.new(250,0,0),Scale=Vector3.new(500,.1,.1)},20/60,"Quint","In")
		local death = makeHitbox({CFrame=e.CFrame*cf(250,0,0),Size=Vector3.new(500,5*(power^2),5*(power^2)*3)},100*power,false,false,5)
		swait(20)
		death.Color = Color3.new(1,0,0)
		sound(e,"304529688",1.5)
		sound(e,"2613307727",1.2,.75)
		local aaa = BEAM:Clone()
		aaa.Color = Mode["Colors"][3]
		aaa.Transparency = 0.4
		aaa.Mesh.Offset=Vector3.new(250,0,0)
		aaa.Mesh.Scale=Vector3.new(500,.1,.1)
		aaa.Parent = BEAM
		RTweenAnimate(BEAM.Mesh,{Scale=Vector3.new(500,4*(power^2),4*(power^2))},20/60,"Elastic","Out",0,true)
		RTweenAnimate(aaa.Mesh,{Scale=Vector3.new(500,5*(power^2),5*(power^2))},20/60,"Quint","Out",0,true)
		RTweenAnimate(BEAM,{CFrame=cfe2*rang(0,90,0)},45/60,"Sine","Out")
		RTweenAnimate(aaa,{CFrame=cfe2*rang(0,90,0)},45/60,"Sine","Out")
		TweenAnimate(e,{CFrame=cfe2*rang(0,90,0)},45/60,"Sine","Out")
		for i = 1,45 do
			death.CFrame = e.CFrame*cf(250,0,0)
			if i/3 == math.ceil(i/3) then
				local h = rand(1,5)*25
				weirdeff("sphere",{ -- part start properties
					Transparency = -2,
					CFrame = e.CFrame,
					Color=Mode["Colors"][3]:lerp(Color3.new(1,1,1),.5),
					Size = Vector3.new(0,7*(power^2),7*(power^2)),
					Material="Neon"
				},{ 
				},{ -- part end properties
					CFrame = e.CFrame*cf(rand(h/2,h*5),0,0),
					Size = Vector3.new(h,1,1),
					Transparency=1
				},{
				},rand(5,10)/20,"Sine","Out")
				weirdeff("square",{ -- part start properties
					Size=Vector3.new(5,0,5),
					Color=Color3.new(1,1,1),
					Transparency = -1,
					Material="Neon",
					CFrame = e.CFrame*cf(500,0,0)*rang(90,0,90)*randr(10,360,10)
				},{ 
				},{ -- part end properties
					Transparency = 1,
					Size=Vector3.new(10,3.5,10)*(power^2),
					Color=Mode["Colors"][3],
				},{
				},.3,"Quad","Out")
				weirdeff("VBLASTMESH2",{ -- part start properties
					Transparency = .05,
					CFrame = e.CFrame*rang(90,0,90)*cf(0,2.5,0)*randr(0,360,0)
				},{ 
					Scale=Vector3.new(.01,.25,.01),
					VertexColor = glowify(Mode["Colors"][3],Vector3.new)
				},{ -- part end properties
					Transparency = 1,
					CFrame = e.CFrame*rang(90,0,90)*cf(0,5,0)*randr(0,360,0)
				},{
					Scale=Vector3.new(.03,.5,.03)*(power^2)
				},.5,"Sine","Out")
			end
			swait()
		end
		death.Color = Color3.new(0,0,1)
		aaa:Destroy()
		BEAM:Destroy()
		RTweenAnimate(e,{Size=Vector3.new(0,0,0),Transparency=1},30/60,"Back","In")
		swait(120)
		e:Destroy()
	end
	-- ~-~ FINAREZ 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=c0ler},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.3,-0.3,0.3)*ang(math.rad(70.8),math.rad(33.8),math.rad(-10.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(0.4,0.9,-1)*ang(math.rad(-149.6),math.rad(-13.6),math.rad(-108.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0,-0.1)*ang(math.rad(6.1),math.rad(-34.9),math.rad(3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	swait(Alpha*60)
	-- ~-~ FINAREZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 12/60,'Quad','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=c0le},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-0.5,0.5,-1.1)*ang(math.rad(175.9),math.rad(14.3),math.rad(115))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.1,0.7,0.8)*ang(math.rad(-4.6),math.rad(24.6),math.rad(101))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,0.2)*ang(math.rad(-5.4),math.rad(46.2),math.rad(-11.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	slashy(root.CFrame*cf(0,Mode["flying"],0)*rang(95,170,75),Mode["Colors"][3],{14,17,14},3,2,{10,10},145)
	slashy(root.CFrame*cf(0,Mode["flying"],0)*rang(95,170,90),Mode["Colors"][4],{18,25,18,15},4,3,{10,10,10},175)
	swait(Alpha*30)
	-- first projectile burst
	sound(root,"750074147",1.5,1)
	for i = -6,6,4 do
		coroutine.resume(coroutine.create(function()
			PROJECTILE(2,30,root.CFrame,root.CFrame*rang(0,7.5*i,0)*cf(0,0,-10),i*10)
		end))
	end
	swait(Alpha*30)
	-- ~-~ FINAREZ 3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 16/60,'Quad','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=c0l},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.1,0.7,0.5)*ang(math.rad(-10.3),math.rad(-14.8),math.rad(-92.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.3,0.7,-0.2)*ang(math.rad(92.9),math.rad(38.1),math.rad(-13))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.2,0,0.2)*ang(math.rad(0),math.rad(-50),math.rad(10))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	slashy(root.CFrame*cf(0,Mode["flying"],0)*rang(95,10,75),Mode["Colors"][3],{15,18,15},3,2,{10,10},145)
	slashy(root.CFrame*cf(0,Mode["flying"],0)*rang(95,10,90),Mode["Colors"][4],{20,30,20,15},4,3,{10,10,10},175)
	swait(Alpha*30)
	-- second projectile burst
	sound(root,"750074147",1.75,1)
	for i = -8,8,4 do
		coroutine.resume(coroutine.create(function()
			PROJECTILE(1.5,30,root.CFrame,root.CFrame*rang(0,7.5*i,0)*cf(0,0,-10),-i*12.5)
		end))
	end
	swait(Alpha*30)
	swait(15)
	tweenDisable = {}
	attack,idlesoff = false,false
end

function heartlol(moveKey)
	attack,idlesoff = true,true
	-- ~-~ AISURUZ 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,-0.1)*ang(math.rad(-10),math.rad(55),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.8,-0.9)*ang(math.rad(-47.7),math.rad(14.8),math.rad(10.3))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-0.9,0.3)*ang(math.rad(19.4),math.rad(-58.5),math.rad(16.7))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1,-0.1,-0.6)*ang(math.rad(90),math.rad(35),math.rad(90))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,0.4,-0.7)*ang(math.rad(168.2),math.rad(-24.6),math.rad(-32.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0,-0.1)*ang(math.rad(7.7),math.rad(-49.7),math.rad(6.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(10)
	local heart = weirdeff("heart",{
		CFrame = root.CFrame*cf(0,2,0),
		Size = Vector3.new(0,0,0),
		Color = Mode["Colors"][3],
		Material="Neon"
	},{},{
		Size = Vector3.new(1.7,1.4,.2),
		CFrame = root.CFrame*cf(0,1.75,-2)
	},{},10/60,"Back","Out",nil,nil,nil,true,true)
	sound(heart,"5217284309",.8,1.5)
	sound(heart,"1135968468",.8,1.25)
	-- ~-~ AISURUZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 5/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0.2)*ang(math.rad(0),math.rad(55),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.8,-0.9)*ang(math.rad(-47.7),math.rad(14.8),math.rad(10.3))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1,0.3)*ang(math.rad(0.1),math.rad(-59.9),math.rad(0.9))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1,-0.1,-0.6)*ang(math.rad(90),math.rad(35),math.rad(90))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.3,0,-0.3)*ang(math.rad(168.2),math.rad(-24.6),math.rad(52.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*ang(math.rad(0),math.rad(-50),math.rad(0.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	slashy(root.CFrame*cf(0,1.75,-2)*rang(0,90,0)*rang(-70,0,-90),Mode["Colors"][2],{2,4,2},1,2,{10,10},179)
	slashy(root.CFrame*cf(0,1.75,-2)*rang(0,90,0)*rang(70,0,-90),Mode["Colors"][2],{2,4,2},1,2,{10,10},179)
	weirdeff("heart",{
		CFrame = root.CFrame*cf(0,1.75,-2),
		Size = Vector3.new(1.7,1.4,.2),
		Color = Mode["Colors"][2],
		Material="ForceField",
		Transparency=-2
	},{},{
		Size = Vector3.new(1.7,1.4,.2)*2.5,
		Transparency=1
	},{},20/60,"Bounce","Out")
	weirdeff("heart",{
		CFrame = root.CFrame*cf(0,1.75,-2),
		Size = Vector3.new(1.7,1.4,.2),
		Color = Mode["Colors"][2],
		Material="Neon",
		Transparency=.5
	},{},{
		Size = Vector3.new(1.7,1.4,.2)*1.75,
		Transparency=1
	},{},20/60,"Sine","Out")
	swait(10)
	local dir = CFrame.new(heart.CFrame.p,mouse.Hit.p).lookVector
	local hit,pos = rayCast(heart.CFrame.p,dir,1000,workspace.Effects)
	-- ~-~ AISURUZ HEAL3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,0,0)*ang(math.rad(-10),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.8,-0.9)*ang(math.rad(-47.7),math.rad(14.8),math.rad(10.3))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1,0)*ang(math.rad(10),math.rad(-4.9),math.rad(0.9))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-0.8,-0.2,-0.6)*ang(math.rad(90),math.rad(65),math.rad(90))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(0.7,-0.3,-0.6)*ang(math.rad(122.4),math.rad(-61.1),math.rad(-61))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0.3)*ang(math.rad(-10.3),math.rad(-0.1),math.rad(0.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	local marked = nil
	coroutine.resume(coroutine.create(function()
		if keysDown[moveKey] then marked = true end
		weirdeff("woosh",{
			Size=Vector3.new(0,0,20),
			CFrame=CFrame.new(heart.CFrame.p,mouse.Hit.p)*rang(0,180,0),
		},{
			Transparency=-1,
			Texture = "http://www.roblox.com/asset/?id=5149372055",
			Color3 = glowify(Mode["Colors"][1])
		},{
			Size=Vector3.new(25,25,30),
			CFrame=CFrame.new(heart.CFrame.p,mouse.Hit.p)*rang(0,180,0)*cf(0,0,-3.5),
		},{
			Transparency=1
		},20/60,"Sine","Out")
		local eq = RTweenAnimate(heart,{Position=pos},.1 + ((heart.Position-pos).magnitude/480),"Linear","Out")
		local death = makeHitbox({Position=pos,Size=Vector3.new(75,75,75),Shape="Ball"},0,false,false,1000,function(hb,eroot,ehum)
			if marked == true then
				marked = eroot
			else
				ehum.Health = ehum.Health + 100 + 900*(1 - (fakeHealth["Current"]/fakeHealth["Max"]))
			end
		end)
		sound(heart,"5217287574",1.1,.75)
		swait((.1 + ((heart.Position-pos).magnitude/480))*60)
		heart.Position = pos
		for i = 1,6 do
			weirdeff("heart",{
				CFrame = heart.CFrame*randr(),
				Size = Vector3.new(1.7,1.4,.2),
				Color = Mode["Colors"][3],
				Material="Neon",
				Transparency=-2
			},{},{
				Size = Vector3.new(1.7,1.4,.2)*(rand(50,150)/10),
				Transparency=1,
				CFrame = heart.CFrame*randr()*cf(0,0,rand(40,60))*randr(),
			},{},rand(30,60)/60,"Sine","Out")
		end
		weirdeff("InvertedSphere",{
			CFrame = heart.CFrame,
			Size = Vector3.new(5,5,5),
			Color = Mode["Colors"][3],
			Material="ForceField",
			Transparency=1
		},{},{
			Size = Vector3.new(75,75,75),
			Transparency = -5,
		},{},20/60,"Sine","Out",0,true)
		weirdeff("sphere",{
			CFrame = heart.CFrame,
			Size = Vector3.new(15,15,15),
			Color = Mode["Colors"][1],
			Material="Neon",
			Transparency=-1
		},{},{
			Size = Vector3.new(45,45,45),
			Transparency = 1,
		},{},15/60,"Sine","Out")
		for i = 1,5 do
			local cf = heart.CFrame*randr()
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10),
				Color=Mode["Colors"][1],
				Transparency = .05,
				Material="Neon",
				CFrame = cf
			},{ -- mesh start properties
				VertexColor = glowify(Mode["Colors"][3],Vector3.new),
				Scale = Vector3.new(.05,0,.05)
			},{ -- part end properties
				Transparency = 1,
				CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
			},{ -- mesh end properties
				Scale = Vector3.new(.75,0,.75)*(math.random(50,150)/100)
			},rand(60,140)/100,"Quad","Out")
		end
		death.Color = Color3.new(1,0,0)
		sound(heart.CFrame,"4478174740",1.5,1)
		sound(heart.CFrame,"169445602",1.5,1.75)
		sound(heart.CFrame,"605005842",1.2,1.5)
		for i = 1,7 do
			local m = rand(5,25)
			local f = rand(60,140)/100
			slashy(heart.CFrame*rang(rsign()*90,0,0)*randr(40,40,360),Mode["Colors"][rand(1,4)],{0*f,50*f,100*f,150*f},rand(4,16),3,{m/2,m,m*3})
			weirdeff("Explosion2",{
				Material="Neon",
				CFrame=heart.CFrame*randr(),
				Size=Vector3.new(10,0,10),
				Color=Mode["Colors"][rand(1,4)],
				Transparency=.5,
			},{Transparency=1},{
				CFrame=heart.CFrame*randr(),
				Size=Vector3.new(250,0,250),
				Transparency=1
			},{},15/60,"Quad","Out")
			for i = 1,2 do
				weirdeff("heart",{
					CFrame = heart.CFrame*randr(),
					Size = Vector3.new(1.7,1.4,.2)*2,
					Color = Mode["Colors"][3],
					Material="Neon",
					Transparency=-2
				},{},{
					Size = Vector3.new(1.7,1.4,.2)*(rand(50,100)/8),
					Transparency=1,
					CFrame = heart.CFrame*randr()*cf(0,0,rand(80,120))*randr(),
				},{},rand(30,60)/60,"Sine","Out")
			end
			swait(2)
		end
		heart:Destroy()
		death.Color = Color3.new(0,0,1)
		swait(5)
		if marked == true then
			marked = false
		end
	end))
	-- ~-~ AISURUZ HEAL4 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,0,0.5)*ang(math.rad(15),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.2,-0.8,-1)*ang(math.rad(-57.7),math.rad(14.8),math.rad(10.3))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1.1,0)*ang(math.rad(-15),math.rad(-4.9),math.rad(0.9))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.6,0.3,0)*ang(math.rad(90),math.rad(20),math.rad(-70))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.6,0.2,0.3)*ang(math.rad(101.1),math.rad(-18.7),math.rad(74.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0.3)*ang(math.rad(-20.3),math.rad(-0.1),math.rad(0.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	if marked then
		repeat swait() until marked ~= true
		if marked then
			swait(15)
			root.Anchored = true
			marked.Anchored = true
			weirdeff("SpikeBall",{
				CFrame = root.CFrame,
				Size = Vector3.new(50,50,50),
				Color = Mode["Colors"][3],
				Material="Neon",
				Transparency=1
			},{},{
				Size = Vector3.new(15,15,15),
				Transparency = 0,
				Color = Mode["Colors"][1],
			},{},5/60,"Sine","In",nil,true)
			weirdeff("SpikeBall",{
				CFrame = marked.CFrame*cf(0,0,-2),
				Size = Vector3.new(50,50,50),
				Color = Mode["Colors"][3],
				Material="Neon",
				Transparency=1
			},{},{
				Size = Vector3.new(15,15,15),
				Transparency = 0,
				Color = Mode["Colors"][1],
			},{},5/60,"Sine","In",nil,true)
			swait(5)
			-- ~-~ AISURUZ HUG1 ~-~
			RootJoint.C0=cf(0,0,0.1)*ang(math.rad(5),math.rad(0),math.rad(0))*RootCF
			LH.C0=cf(-1.1,-1.1,-0.9)*ang(math.rad(-69.9),math.rad(8.4),math.rad(-7.4))*ang(math.rad(0),math.rad(-90),math.rad(0))
			RH.C0=cf(1,-1.1,-0.1)*ang(math.rad(-10),math.rad(0.4),math.rad(-5))*ang(math.rad(0),math.rad(90),math.rad(0))
			LW.C0=cf(-1.1,0.4,0.9)*ang(math.rad(95),math.rad(0),math.rad(-75))
			RW.C0=cf(0.9,0.3,1)*ang(math.rad(90.1),math.rad(-0.9),math.rad(80))
			Torso.Neck.C0 =cf(0,0,-0.1)*ang(math.rad(5),math.rad(10),math.rad(0))*necko
			root.CFrame = marked.CFrame*cf(0,0,-3.5)*rang(0,180,0)
			slashy(root.CFrame*cf(0,1.75,-2)*rang(0,90,0)*rang(-70,0,-90),Mode["Colors"][1],{2,4,2},1,2,{8,8},90)
			slashy(root.CFrame*cf(0,1.75,-2)*rang(0,90,0)*rang(70,0,-90),Mode["Colors"][1],{2,4,2},1,2,{8,8},90)
			sound(root,"5217285384",1.1,1)
			-- ~-~ AISURUZ HUG2 ~-~
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Sine','In',0,false,0
			local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0)*ang(math.rad(5),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(LH,{C0=cf(-1,-1.1,-0.7)*ang(math.rad(-85),math.rad(10),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(RH,{C0=cf(1,-1.1,0)*ang(math.rad(-25),math.rad(0),math.rad(0))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(LW,{C0=cf(-0.5,-0.2,-1.4)*ang(math.rad(65),math.rad(0),math.rad(80))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(RW,{C0=cf(0.7,0.1,-1.4)*ang(math.rad(82.6),math.rad(-9.7),math.rad(-74.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.2)*ang(math.rad(-16.8),math.rad(44.9),math.rad(-1.5))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(root,{CFrame=marked.CFrame*cf(0,0,-1)*rang(0,180,0)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			swait(Alpha*60)
			sound(root,"5217286484",1.1,1.25)
			weirdeff("heart",{
				CFrame = root.CFrame*cf(0,10,0),
				Size = Vector3.new(1.7,1.4,.2),
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=1
			},{},{
				Size = Vector3.new(1.7,1.4,.2)*2.5,
				Transparency=0,
			},{},120/60,"Quad","Out",nil,true)
			weirdeff("heart",{
				CFrame = root.CFrame*cf(0,10,0),
				Size = Vector3.new(1.7,1.4,.2)*2.5,
				Color = Mode["Colors"][2],
				Material="ForceField",
				Transparency=-2
			},{},{
				Size = Vector3.new(1.7,1.4,.2)*5,
				Transparency=1
			},{},20/60,"Bounce","Out",nil,nil,110/60)
			weirdeff("heart",{
				CFrame = root.CFrame*cf(0,10,0),
				Size = Vector3.new(1.7,1.4,.2)*2.5,
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=.5
			},{},{
				Size = Vector3.new(1.7,1.4,.2)*3.75,
				Transparency=1
			},{},20/60,"Sine","Out",nil,nil,110/60)
			delay(100/60,function()
				slashy(root.CFrame*cf(0,10,0)*rang(0,90,0)*rang(-70,0,-90),Mode["Colors"][2],{5,10,5},1,2,{20,20},179)
				slashy(root.CFrame*cf(0,10,0)*rang(0,90,0)*rang(70,0,-90),Mode["Colors"][2],{5,10,5},1,2,{20,20},179)
			end)
			-- ~-~ AISURUZ HUG3 ~-~
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 60/60,'Sine','InOut',1,true,0
			RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.2)*ang(math.rad(-17.5),math.rad(19.9),math.rad(-1.1))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			for i = 1,60*2*2 do
				if i/3 == math.ceil(i/3) then
					weirdeff("heart",{
						CFrame = root.CFrame*randr(),
						Size = Vector3.new(1.7,1.4,.2),
						Color = Mode["Colors"][3],
						Material="Neon",
						Transparency=0.6
					},{},{
						Size = Vector3.new(1.7,1.4,.2)*(rand(50,100)/50),
						Transparency=1,
						CFrame = root.CFrame*rang(-90,0,0)*randr(90,90,90)*cf(0,0,rand(15,30))*randr(),
					},{},rand(15,45)/60,"Quad","Out",nil,true)
				end
				swait()
			end
			marked.Parent:FindFirstChildOfClass("Humanoid").Health = marked.Parent:FindFirstChildOfClass("Humanoid").Health + 500 + 2500*(1 - (fakeHealth["Current"]/fakeHealth["Max"]))
			root.Anchored = false
			marked.Anchored = false
		end
	end
	swait(30)
	attack,idlesoff = false,false
end

function shinoZ()
	attack,idlesoff = true,true
	--root.Anchored = false
	-- ~-~ SHINZ 1 ~-~
	particle(root,3,{
		LightEmission=1,
		Texture="rbxassetid://1053546634",
		Size=NS({NSK(0,100,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0.6,0)}),
		RotSpeed = NR(360),
		Color = CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
		Lifetime = NR(1)
	})
	local pop = particle(root,0,{
		Texture="rbxassetid://1084997326",
		Size=NS({NSK(0,50,25),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		Rotation = NR(0,360),
		Color = CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Enabled = true,
		Rate = 1000,
		Lifetime = NR(0.2,0.4),
		LightEmission=1,
	})
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 45/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0.2)*ang(math.rad(20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.9,-0.2,-0.8)*ang(math.rad(-29.2),math.rad(11.6),math.rad(-8.2))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-0.8,0)*ang(math.rad(-19.1),math.rad(-10),math.rad(10.1))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-0.7,1.3,-0.9)*ang(math.rad(87.8),math.rad(32.4),math.rad(75.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(0.7,1.3,-0.9)*ang(math.rad(82),math.rad(-33.5),math.rad(-78.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.2)*ang(math.rad(15),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	sound(root,"5132757647",1,1.5)
	swait(35)
	particle(root,2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(20/60),
		Size=NS({NSK(0,75,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	})
	particle(root,50,{
		LightEmission=1,
		Texture="rbxassetid://3028571188",
		Size=NS({NSK(0,100,50),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		Rotation = NR(0,360),
		Color = CS(Mode["Colors"][4]:lerp(Color3.new(1,1,1),.1)),
		Lifetime = NR(20/60)
	})
	sound(root,"5221041045",1,1.5)
	swait(10)
	pop.Enabled = false
	swait(10)
	pop:Destroy()
	-- ~-~ SHINZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Quad','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,-0.2)*ang(math.rad(-20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.9,-0.1,-0.5)*ang(math.rad(-18),math.rad(17.4),math.rad(-19.6))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-0.9,-0.3)*ang(math.rad(-21.1),math.rad(-14.3),math.rad(17.3))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.6,0.2,0.6)*ang(math.rad(120),math.rad(0),math.rad(-110))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.6,0.3,0.4)*ang(math.rad(120),math.rad(0),math.rad(110))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.4,0.6)*ang(math.rad(-45),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(5)
	weirdeff("SpikeBall",{
		CFrame = root.CFrame,
		Size = Vector3.new(0,0,0),
		Color = Mode["Colors"][3],
		Material="Neon",
		Transparency=0
	},{Transparency = 1},{
		Size = Vector3.new(300,300,300),
		Transparency = 1,
	},{},30/60,"Quint","Out")
	for i = 1,10 do
		local cf = root.CFrame*randr(30,360,30)
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Transparency = .05,
			Material="Neon",
			CFrame = cf
		},{ -- mesh start properties
			VertexColor = glowify(Mode["Colors"][rand(2,4)],Vector3.new),
			Scale = Vector3.new(.05,0,.05)
		},{ -- part end properties
			Transparency = 1,
			CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
		},{ -- mesh end properties
			Scale = Vector3.new(3,0,3)*(math.random(25,100)/100)
		},math.random(40,160)/60,"Quint","Out")
	end
	sound(root,"5221040083",1.2,2)
	for i = 1,6 do
		coroutine.resume(coroutine.create(function()
			local cfe = root.CFrame*cf(0,-3,0)*rang(0,60*i,0)*cf(0,0,100)
			local hb = makeHitbox({CFrame = cfe*cf(0,0,100),Size=Vector3.new(200,200,200),Shape="Ball"},850,false,false,4,anchorer)
			weirdeff("sphere",{
				CFrame = cfe*cf(0,0,-100),
				Size = Vector3.new(0,0,0),
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=-2
			},{Transparency = 1},{
				CFrame = cfe,
				Size = Vector3.new(1,1,200),
				Transparency = 1,
			},{},5/60,"Sine","In")
			weirdeff("sphere",{
				CFrame = cfe,
				Size = Vector3.new(0,0,200),
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=1
			},{Transparency = 1},{
				CFrame = cfe*cf(0,0,100),
				Size = Vector3.new(15,15,15),
				Transparency = -2,
			},{},15/60,"Sine","In")
			swait(14)
			weirdeff("InvertedSphere",{
				CFrame = cfe*cf(0,0,100),
				Size = Vector3.new(15,15,15),
				Color = Mode["Colors"][4],
				Material="Neon",
				Transparency=-1
			},{Transparency = 1},{
				Size = Vector3.new(100,100,100),
				Transparency=1
			},{},10/60,"Sine","Out")
			weirdeff("sphere",{
				CFrame = cfe*cf(0,0,100),
				Size = Vector3.new(15,15,15),
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=0
			},{Transparency = 1},{
				Size = Vector3.new(0,0,0),
				Color = Mode["Colors"][3],
			},{},25/60,"Back","In")
			weirdeff("SpikeBall",{
				CFrame = cfe*cf(0,0,100),
				Size = Vector3.new(250,250,250),
				Color = Mode["Colors"][2],
				Material="Neon",
				Transparency=1
			},{Transparency = 1},{
				Size = Vector3.new(0,0,0),
				Transparency = 0,
			},{},25/60,"Sine","In")
			particle(cfe*cf(0,0,100),3,{ZOffset=1,
				Texture="rbxassetid://2109052855",
				LightEmission=1,
				Color=CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(25/60),
				Size=NS({NSK(0,200,0),NSK(1,0,0)}),
				Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
				LockedToPart=true
			})
			sound(root,"5113630277",1.3,1.5)
			swait(24)
			cfe = cfe*cf(0,0,100)
			weirdeff("SpikeBall",{
				CFrame = cfe,
				Size = Vector3.new(250,250,250),
				Color = Color3.new(1,1,1),
				Material="Neon",
				Transparency=-1
			},{Transparency = 1},{
				Size = Vector3.new(500,500,500),
				Transparency = 1,
			},{},5/60,"Elastic","In")
			weirdeff("SpikeBall",{
				CFrame = cfe,
				Size = Vector3.new(250,250,250),
				Color = Color3.new(0,0,0),
				Material="Neon",
				Transparency=-1
			},{Transparency = 1},{
				Size = Vector3.new(500,500,500),
				Transparency = 1,
			},{},5/60,"Elastic","Out")
			particle(cfe,3,{ZOffset=1,
				Texture="rbxassetid://2545920866",
				LightEmission=1,
				Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(2.5),
				Size=NS({NSK(0,150,0),NSK(1,200,0)}),
				Transparency = NS({NSK(0,0.5,0),NSK(1,1,0)})
			},Vector3.new(0,-1,0))
			for i = -1,1,2 do
				weirdeff("Winds",{
					CFrame = cfe,
					Size = Vector3.new(15,150,15),
					Color = Mode["Colors"][3],
					Material="Neon",
					Transparency=-2
				},{Transparency = 1},{
					CFrame = cfe*rang(0,179*i,0),
					Size = Vector3.new(300,150,300),
					Color = Mode["Colors"][1],
					Transparency=1
				},{},60/60,"Sine","Out")
			end
			for i = 1,4 do
				local cf = cfe*randr(40,360,40)
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Transparency = .05,
					Material="Neon",
					CFrame = cf
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][rand(2,4)],Vector3.new),
					Scale = Vector3.new(.05,0,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
				},{ -- mesh end properties
					Scale = Vector3.new(4,0,4)*(math.random(25,100)/100)
				},math.random(40,160)/40,"Quad","Out")
			end
			sound(root,"5221040616",rand(70,130)/100,3)
			hb.Color = Color3.new(1,0,0)
			for i = 1,5 do
				local f = rand(20,40)/10
				slashy(cfe*rang(rsign()*90,0,0)*randr(40,40,360),Mode["Colors"][rand(1,4)],{0*f,50*f,100*f,150*f},rand(10,25),3,{8,16,24})
				weirdeff("Explosion2",{
					Material="Neon",
					CFrame=cfe*randr(),
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][rand(1,4)],
					Transparency=.25,
				},{Transparency=1},{
					CFrame=cfe*randr(),
					Size=Vector3.new(500,0,500),
					Transparency=1
				},{},20/60,"Quint","Out")
				swait(2)
			end
			hb.Color = Color3.new(0,0,1)
		end))
	end
	swait(15)
	attack,idlesoff = false,false
	--root.Anchored = false
end

function sosaku(moveKey)
	attack,idlesoff = true,true
	local charge = 0
	--root.Anchored = false
	-- ~-~ SOSAKUZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Back','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,Mode["Flying"],0.1)*ang(math.rad(19.8),math.rad(-39.3),math.rad(7.6))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-0.7,-1)*ang(math.rad(-44.6),math.rad(5.8),math.rad(-4.1))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1,0)*ang(math.rad(-10.2),math.rad(-12.8),math.rad(6))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0.7,-0.7)*ang(math.rad(163),math.rad(44.8),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0.8,0.1)*ang(math.rad(-21.8),math.rad(-9.8),math.rad(19.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0.2,0.5)*ang(math.rad(-28.6),math.rad(33.6),math.rad(9))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	sound(root,"412756123",1,1)
	if keysDown[moveKey] then
		local sond = sound(root,"4920067277",1,1)
		particle(char["Left Arm"],4,{ZOffset=1,
			Texture="rbxassetid://2109052855",
			LightEmission=1,
			Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(60/60),
			Size=NS({NSK(0,0,0),NSK(.25,10,0),NSK(1,15,0)}),
			Transparency = NS({NSK(0,1,0),NSK(.75,0.5,0),NSK(1,1,0)}),
			LockedToPart=true
		},v3(0,-1,0))
		particle(char["Left Arm"],50,{
			LightEmission=1,
			Texture="rbxassetid://3028571188",
			Size=NS({NSK(0,0,0),NSK(.25,10,5),NSK(1,15,7.5)}),
			Transparency = NS({NSK(0,0,0),NSK(.75,0.5,0),NSK(1,1,0)}),
			Rotation = NR(0,360),
			Color = CS(Mode["Colors"][4]:lerp(Color3.new(1,1,1),.1)),
			Lifetime = NR(40/60,80/60),
			LockedToPart=true
		},v3(0,-1,0))
		local e = particle(char["Left Arm"],5,{
			LightEmission=1,
			Texture="rbxassetid://1053546634",
			Size=NS({NSK(0,0,0),NSK(.05,2.5,0),NSK(1,12.5,0)}),
			Transparency = NS({NSK(0,.5,0),NSK(1,0,0)}),
			RotSpeed = NR(360),
			Color = CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
			Lifetime = NR(20),
			ZOffset = 1,
			LockedToPart=true
		},v3(0,-1,0))
		local pop = particle(char["Left Arm"],0,{
			Texture="rbxassetid://1084997326",
			Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
			Rotation = NR(0,360),
			Color = CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
			Enabled = true,
			Rate = 300,
			Lifetime = NR(0.4,0.8),
			LightEmission=1,
			LockedToPart=true
		})
		repeat
			if charge/30 == math.ceil(charge/30) then
				particle(char["Left Arm"],2,{ZOffset=1,
					Texture="rbxassetid://2109052855",
					LightEmission=1,
					Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
					Lifetime=NR(25/60),
					Size=NS({NSK(0,5*(charge/30),0),NSK(1,0,0)}),
					Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
					LockedToPart=true
				},v3(0,-1,0))
				particle(char["Left Arm"],50,{
					LightEmission=1,
					Texture="rbxassetid://3028571188",
					Size=NS({NSK(0,7.5*(charge/30),3.75*(charge/30)),NSK(1,0,0)}),
					Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
					Rotation = NR(0,360),
					Color = CS(Mode["Colors"][4]:lerp(Color3.new(1,1,1),.1)),
					Lifetime = NR(25/60),
					LockedToPart=true
				},v3(0,-1,0))
				weldedweirdeff("SpikeBall",char["Left Arm"],true,cf(0,-1,0),{
					Size=Vector3.new(7.5*(charge/30),7.5*(charge/30),7.5*(charge/30)),
					Material = "Neon",
					Color = Mode["Colors"][1],
					Transparency=1
				},{Transparency=1},cf(0,-1,0),{
					Size=Vector3.new(0,0,0),
					Transparency=0.75
				},{},15/60,"Sine","In")
			end
			pop.Size=NS({NSK(0,2.5*(charge/30),1.125*(charge/30)),NSK(1,0,0)})
			charge = charge + 1 
			swait()
		until not keysDown[moveKey] or charge >= 550
		e:Destroy()
		sond:Destroy()
		pop:Destroy()
	end
	sound(root,"3029175439",1,2)
	weldedweirdeff("SpikeBall",char["Left Arm"],true,cf(0,-1,0),{
		Size=Vector3.new(0,0,0),
		Material = "Neon",
		Color = Mode["Colors"][1],
		Transparency=-1
	},{Transparency=1},cf(0,-1,0),{
		Size=Vector3.new(10*(charge/30),10*(charge/30),10*(charge/30)),
		Transparency=1
	},{},25/60,"Quad","Out")
	weldedweirdeff("SpikeBall",char["Left Arm"],true,cf(0,-1,0),{
		Size=Vector3.new(0,0,0),
		Material = "Neon",
		Color = Mode["Colors"][3],
		Transparency=0.75
	},{Transparency=1},cf(0,-1,0),{
		Size=Vector3.new(30*(1+(charge/30)),30*(1+(charge/30)),30*(1+(charge/30))),
		Transparency=1
	},{},15/60,"Quad","Out")
	coroutine.resume(coroutine.create(function()
		local BEAM = PART(workspace["Effects"])
		BEAM.CFrame = CFrame.new((root.CFrame*cf(0,0,-3)).p,mouse.Hit.p)
		local dir = CFrame.new(BEAM.CFrame.p,mouse.Hit.p).lookVector
		local hit,pos = rayCast(BEAM.CFrame.p,dir,5000,workspace.Effects)
		weirdeff("woosh",{
			Size=Vector3.new(0,0,20)*(1+(charge/30)),
			CFrame=CFrame.new(BEAM.CFrame.p,mouse.Hit.p)*rang(0,180,0),
		},{
			Transparency=-2,
			Texture = "http://www.roblox.com/asset/?id=5149372055",
			Color3 = glowify(Mode["Colors"][1])
		},{
			Size=Vector3.new(75,75,25)*(1+(charge/30)),
			CFrame=CFrame.new(BEAM.CFrame.p,mouse.Hit.p)*rang(0,180,0)*cf(0,0,-5*(1+(charge/30))),
		},{
			Transparency=1
		},60/60,"Quint","Out")
		weldedweirdeff("woosh",BEAM,true,cf(0,0,75*(1+(charge/30))/4)*rang(0,180,0),{Size=Vector3.new(15,15,75/2)*(1+(charge/30))},{
			Texture = "http://www.roblox.com/asset/?id=5149372490",
			Color3 = glowify(Mode["Colors"][1])
		},cf(0,0,75*(1+(charge/30))/4)*rang(0,180,179),{},{},4/60,"Linear","Out",-1,true)
		weldedweirdeff("woosh",BEAM,true,cf(0,0,75*(1+(charge/30))/4)*rang(0,180,0),{Size=Vector3.new(15,15,75/2)*(1+(charge/30))},{
			Texture = "http://www.roblox.com/asset/?id=5149372490",
			Color3 = glowify(Mode["Colors"][1])
		},cf(0,0,75*(1+(charge/30))/4)*rang(0,180,-179),{},{},4/60,"Linear","Out",-1,true)
		weldedweirdeff("woosh",BEAM,true,cf(0,0,55*(1+(charge/30)))*rang(0,180,90),{Size=Vector3.new(15,15,50)*(1+(charge/30))},{
			Texture = "http://www.roblox.com/asset/?id=5149372055",
			Color3 = glowify(Mode["Colors"][3])
		},cf(0,0,30*(1+(charge/30)))*rang(0,180,90+179),{},{},6/60,"Linear","Out",-1,true)
		weldedweirdeff("woosh",BEAM,true,cf(0,0,55*(1+(charge/30)))*rang(0,180,90),{Size=Vector3.new(15,15,50)*(1+(charge/30))},{
			Texture = "http://www.roblox.com/asset/?id=5149372055",
			Color3 = glowify(Mode["Colors"][3])
		},cf(0,0,30*(1+(charge/30)))*rang(0,180,90-179),{},{},6/60,"Linear","Out",-1,true)
		particle(BEAM,0,{
			Texture="rbxassetid://243728104",
			LightEmission=.9,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.5,1),
			Size=NS({NSK(0,12*(1+(charge/30)),6*(1+(charge/30))),NSK(1,20*(1+(charge/30)),0)}),
			Transparency=NS({NSK(0,0.5,0.25),NSK(0.2,0.75,0.125),NSK(1,1,0)}),
			Speed=NR(50,100*(1+charge/30)),
			Drag=3,
			SpreadAngle=Vector2.new(360,360),
			RotSpeed=NR(-50,50),
			Rotation=NR(1,360),
			EmissionDirection="Front",
			Rate=1000
		})
		if charge < 550 then
			local hb = makeHitbox({Position = pos,Size=Vector3.new(250,250,250)*(1+(charge/120)),Shape="Ball"},250+(charge*1.5),false,false,4,anchorer)
			TweenAnimate(BEAM,{Position=pos},.1 + ((BEAM.Position-pos).magnitude/2048),"Linear","Out")
			swait((.1 + ((BEAM.Position-pos).magnitude/2048))*60)
			BEAM:Destroy()
			for i = 1,6 do
				local cf = hb.CFrame*randr(40,360,40)
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Transparency = .05,
					Material="Neon",
					CFrame = cf
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][3],Vector3.new),
					Scale = Vector3.new(.05,0,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
				},{ -- mesh end properties
					Scale = Vector3.new(4,0,4)*(math.random(25,100)/100)*(1+(charge/120))
				},math.random(40,160)/40,"Quad","Out")
			end
			sound(hb.CFrame,"610327604",1.2,2).EmitterSize = charge/2
			sound(hb.CFrame,"203691378",1.1,4)
			sound(hb.CFrame,"519795084",1.4,4.5)
			hb.Color = Color3.new(1,0,0)
			for i = 1,10 do
				local f = (1+(charge/120))*rand(20,40)/10
				slashy(hb.CFrame*rang(rsign()*90,0,0)*randr(40,40,360),Mode["Colors"][1],{0*f,50*f,100*f,150*f},rand(10,25),3,{12,24,32})
				weirdeff("Explosion2",{
					Material="Neon",
					CFrame=hb.CFrame*randr(),
					Size=Vector3.new(10,0,10)*(1+(charge/30)),
					Color=Mode["Colors"][3],
					Transparency=.25,
				},{Transparency=1},{
					CFrame=hb.CFrame*randr(),
					Size=Vector3.new(500,0,500)*(1+(charge/30)),
					Transparency=1
				},{},30/60,"Quint","Out")
				swait(2)
			end
			hb.Color = Color3.new(0,0,1)
		else
			local hb = makeHitbox({Position = pos,Size=Vector3.new(1500,1500,1500),Shape="Ball"},2500,false,false,4,anchorer)
			TweenAnimate(BEAM,{Position=pos},.1 + ((BEAM.Position-pos).magnitude/2048),"Linear","Out")
			swait((.1 + ((BEAM.Position-pos).magnitude/2048))*60)
			BEAM:Destroy()
			hb.Color = Color3.new(1,0,0)
			weirdeff("sphere",{ -- part start properties
				Size=Vector3.new(2048,2048,2048),
				Color=rgb(255,255,255),
				Transparency = -2,
				Material="Neon",
				CFrame = hb.CFrame
			},{ 
			},{ -- part end properties
				Transparency = 1
			},{
			},.2,"Sine","Out")
			for i = 1,6 do
				local cf = hb.CFrame*randr(60,360,60)
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Transparency = .05,
					Material="Neon",
					CFrame = cf
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][3],Vector3.new),
					Scale = Vector3.new(.05,0,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
				},{ -- mesh end properties
					Scale = Vector3.new(20,0,20)*(math.random(50,100)/100)
				},math.random(40,160)/20,"Quint","Out")
			end
			for i = 1,2 do
				local cfe = hb.CFrame
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Transparency = .05,
					Material="Neon",
					CFrame = cfe*CFrame.new(0,500,0)
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][3],Vector3.new),
					Scale = Vector3.new(.05,50,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cfe*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)*cf(0,100,0)
				},{ -- mesh end properties
					Scale = Vector3.new(30,10,30)*(math.random(50,100)/100)
				},5,"Quint","Out")
			end
			weirdeff("Explosion2",{
				Material="Neon",
				CFrame=hb.CFrame*cf(0,250,0),
				Size=Vector3.new(1750,500,1750),
				Color=Mode["Colors"][3],
				Transparency=-4,
			},{Transparency=1},{
				CFrame=hb.CFrame*cf(0,1000,0),
				Size=Vector3.new(1250,2000,1250),
				Transparency=1,
				Color=Mode["Colors"][2],
			},{},150/60,"Quint","Out")
			weirdeff("Plant",{
				Material="Neon",
				CFrame=hb.CFrame*cf(0,-1000,0)*randr(1,360,1),
				Size=Vector3.new(750,2000,750),
				Color=Mode["Colors"][1],
				Transparency=-4,
			},{Transparency=1},{
				CFrame=hb.CFrame*cf(0,1000,0)*randr(1,360,1),
				Size=Vector3.new(1750,2000,1750),
				Transparency=1,
				Color=Mode["Colors"][3],
			},{},60/60,"Quad","Out")
			weirdeff("Winds",{
				Material="Neon",
				CFrame=hb.CFrame*cf(0,-1000,0)*randr(1,360,1),
				Size=Vector3.new(250,2000,250),
				Color=Mode["Colors"][2],
				Transparency=-4,
			},{Transparency=1},{
				CFrame=hb.CFrame*cf(0,1000,0)*randr(1,360,1),
				Size=Vector3.new(2048,2000,2048),
				Transparency=1,
			},{},240/60,"Quad","Out")
			weirdeff("Swirl",{
				Material="Neon",
				CFrame=hb.CFrame*cf(0,-1000,0)*randr(1,360,1),
				Size=Vector3.new(250,2000,250),
				Color=Mode["Colors"][1],
				Transparency=-4,
			},{Transparency=1},{
				CFrame=hb.CFrame*cf(0,1000,0)*randr(1,360,1),
				Size=Vector3.new(2048,2000,2048),
				Transparency=1,
			},{},240/60,"Quad","Out")
			sound(hb.CFrame,"610327604",.8,9).EmitterSize = 200
			sound(hb.CFrame,"1417056081",.5,7)
			sound(hb.CFrame,"519795084",1,8)
			sound(hb.CFrame,"462692971",.5,6)
			sound(hb.CFrame,"1259054947",.8,7)
			for i = 1,20 do
				local f = rand(20,40)/10
				weirdeff("square",{ -- part start properties
					Size=Vector3.new(30,0,30),
					Color=Mode["Colors"][1],
					Transparency = -2,
					Material="Neon",
					CFrame = hb.CFrame*randr(10,360,10)
				},{ 
				},{ -- part end properties
					Transparency = 1,
					Size=Vector3.new(2048*1.2,0,2048*1.2),
					Color=Mode["Colors"][1],
					CFrame = hb.CFrame*randr(10,360,10)*cf(rand(-1024,1024),0,rand(-1024,1024))
				},{
				},.6,"Quad","Out")
				weirdeff("sphere",{ -- part start properties
					Size=Vector3.new(700,2048,700),
					Color=Mode["Colors"][3],
					Transparency = -1,
					Material="Neon",
					CFrame = hb.CFrame
				},{ 
				},{ -- part end properties
					Transparency = 1,
					Color=Mode["Colors"][2],
					Size=Vector3.new(350,2048,350),
					CFrame = hb.CFrame*cf(rand(-500,500),rand(1500,2500),rand(-500,500))
				},{
				},.4,"Sine","Out")
				weirdeff("Explosion2",{
					Material="Neon",
					CFrame=hb.CFrame*randr(),
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][1],
					Transparency=-3,
				},{Transparency=1},{
					CFrame=hb.CFrame*randr(),
					Size=Vector3.new(2048,0,2048),
					Transparency=1
				},{},45/60,"Quint","Out")
				weirdeff("Ring",{
					Material="Neon",
					CFrame=hb.CFrame*cf(0,i*100,0)*rang(90,0,0),
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][3],
					Transparency=-2,
				},{Transparency=1},{
					Size=Vector3.new(2048,2048,0),
					Transparency=1
				},{},120/60,"Sine","Out")
				slashy(hb.CFrame*cf(0,i*100,0)*rang(rsign()*90,0,0)*randr(1,1,360),Mode["Colors"][1],{1024,1500,2048},rand(10,25),3,{24,48,64})
				swait(3)
			end
			hb.Color = Color3.new(0,0,1)
		end
	end))
	-- ~-~ SOSAKUZ 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Elastic','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,Mode["Flying"],0)*ang(math.rad(9.8),math.rad(-39.3),math.rad(7.6))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-0.6,-0.9)*ang(math.rad(-34.6),math.rad(5),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1,0)*ang(math.rad(10),math.rad(-10),math.rad(10))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0.4,-0.6)*ang(math.rad(162.9),math.rad(44.8),math.rad(-100))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0.8,-0.2)*ang(math.rad(-12.3),math.rad(-6.4),math.rad(35.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0.3)*ang(math.rad(-16.5),math.rad(34.6),math.rad(2.2))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	attack,idlesoff = false,false
	--root.Anchored = false
end

function kyoki()
	attack,idlesoff = true,true
	local charge1 = particle(char["Right Arm"],2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.7),
		Size=NS({NSK(0,100,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.5,1,0),NSK(1,1,0)})
	},Vector3.new(-1,-1,0))
	particle(char["Right Arm"],50,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.4),
		Size=NS({NSK(0,60,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(0.5,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(-1,-1,0))
	local ej = particle(char["Right Arm"],0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(3),
		Size=NS({NSK(0,30,0),NSK(.1,2.5,0),NSK(1,2.5,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.1,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rate=300,
		RotSpeed=NR(720),
		Rate=0
	},Vector3.new(0,-1.15,0))
	ej:Emit(3)
	-- ~-~ KYOKIZ 1 ~-~
	sound(root,"5113647461",1.2,.75)
	sound(root,"5221041045",1.1,1.25)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,0,-0.1)*ang(math.rad(-27.5),math.rad(-51.3),math.rad(-15.6))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.8,-1,0.1)*ang(math.rad(61),math.rad(64.7),math.rad(-43.5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1.3,-0.1,-0.4)*ang(math.rad(-7.9),math.rad(-18.2),math.rad(-2.7))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.6,0.3,-0.1)*ang(math.rad(21.7),math.rad(-27.4),math.rad(-63.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.3,0.6,-0.7)*ang(math.rad(-164.3),math.rad(43.8),math.rad(-110.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.5,0.2,0.2)*ang(math.rad(-39.4),math.rad(49.1),math.rad(40))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(5)
	sound(root,"5172029559",1.2,1)
	-- ~-~ KYOKIZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Quad','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.2,-0.9,-0.1)*ang(math.rad(-54.1),math.rad(56.1),math.rad(24.4))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.9,-1,-0.6)*ang(math.rad(-20.7),math.rad(-54.2),math.rad(-36.3))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,0.4,-0.4)*ang(math.rad(39),math.rad(-14.6),math.rad(26.3))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0.9,0.3)*ang(math.rad(5),math.rad(9.2),math.rad(-81.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(2,0.1,-0.6)*ang(math.rad(10.1),math.rad(33.3),math.rad(69))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.2,0,0.1)*ang(math.rad(-13.7),math.rad(-56.1),math.rad(-10.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	sound(root,"2836444230",.9,.75)
	weirdeff("Explosion2",{
		Material="Neon",
		CFrame=root.CFrame*cf(0,22,-3),
		Size=Vector3.new(5,50,5),
		Color=Mode["Colors"][1],
		Transparency=-2,
	},{Transparency=1},{
		CFrame=root.CFrame*cf(0,-3,3),
		Size=Vector3.new(40,0,40),
		Transparency=1,
		Color=Mode["Colors"][3],
	},{},15/60,"Quad","Out")
	for i = 1,4 do
		slashy(root.CFrame*cf(0,-3,-3)*rang(rsign()*90,0,0)*randr(1,1,360),Mode["Colors"][1],{0,rand(15,30),rand(30,60)},rand(20,50)/10,2,{5,15,45})
	end
	coroutine.resume(coroutine.create(function()
		local e = PART(char)
		e.CFrame = root.CFrame*cf(0,-3,-3)
		local egg1 = particle(e,0,{ZOffset=2,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.1),
			Size=NS({NSK(0,0,0),NSK(1,15,7.5)}),
			Transparency = NS({NSK(0,0,0),NSK(1,0,0)}),
			LockedToPart=true,
			Rate=60
		})
		local egg = particle(e,0,{ZOffset=2,
			Texture="rbxasset://textures/particles/sparkles_main.dds",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.5,2),
			Size=NS({NSK(0,3.5,3),NSK(1,0,0)}),
			Transparency = NS({NSK(0,0,0),NSK(1,0,0)}),
			Speed=NR(20,100),
			SpreadAngle=Vector2.new(70,70),
			Rate=100,
			Drag=5,
			Acceleration=Vector3.new(0,30,0)
		})
		local h,pos = rayCast(mouse.Hit.p + Vector3.new(0,5,0),Vector3.new(0,-1,0),50)
		local anim = RTweenAnimate(e,{Position=pos},.45,"Sine","Out")
		swait(30)
		egg.Enabled = false
		for i = 1,9 do
			sound(e,"4839213917",rand(17,23)/10,1)
			for v = 1,4 do
				local bean = e:Clone()
				sound(bean,"3748209965",1,1.25)
				bean.Position = pos
				bean.Parent = workspace["Effects"]
				local h,pos = rayCast(CFrame.new(pos + Vector3.new(0,250,0))*rang(0,v*90 + (10*i),0)*cf(0,0,60*i).p,Vector3.new(0,-1,0),750)
				local anim = RTweenAnimate(bean,{Position=pos},.5,"Quad","Out")
				local hb = makeHitbox({Position=pos,Size=Vector3.new(120,120,120)*2,Shape="Ball"},450,false,false,0)
				delay(1 - (.1*i),function()
					weldedweirdeff("SpikeBall",bean,false,cf(0,0,0),{
						Size=Vector3.new(120,120,120)*2,
						Material = "Neon",
						Color = Mode["Colors"][1],
						Transparency=1
					},{Transparency=1},cf(0,-1,0),{
						Size=Vector3.new(0,0,0),
						Transparency=0.5
					},{},30/60,"Sine","In")
					swait(30)
					hb.Color = Color3.new(1,0,0)
					sound(cf(pos),"314970761",rand(12,18)/10,3)
					sound(cf(pos),"519795084",1,2.5)
					weirdeff("Explosion2",{
						Transparency = -1,
						Color = Mode["Colors"][1],
						Size = v3(180,0,180)*2,
						Position = pos - v3(0,20,0)*2,
						Material = "Neon"
					},{Transparency=-3,Color3 = glowify(Mode["Colors"][3])},{
						Transparency=1,
						Size = v3(120,180,120)*2,
						Position = pos+v3(0,70,0)*2,
					},{Transparency=1,Color3 = glowify(Mode["Colors"][1])},30/60,"Quad","Out")
					local cfe = cf(pos)*randr(1,360,1)
					weirdeff("Plant",{
						Transparency = -1,
						Color = Mode["Colors"][3],
						Size = v3(25,250,25)*2,
						CFrame = cfe*cf(0,125*2,0)*rang(180,0,0),
						Material = "Neon"
					},{Transparency=-3,Color3 = glowify(Mode["Colors"][1])},{
						Transparency=1,
						Size = v3(180,0,180)*2,
						CFrame=cfe*rang(180,180+rand(-1,1),0),
						Color = Mode["Colors"][1],
					},{Transparency=1,Color3 = glowify(Mode["Colors"][3])},30/60,"Quad","Out")
					weirdeff("SpikeBall",{
						Color = Color3.new(1,1,1),
						Size = v3(250,250,250)*2,
						Position = pos,
						Material = "Neon",
						Transparency=-3
					},{Transparency=1},{
						Transparency=1
					},{},5/60,"Elastic","Out")
					for i = 1,2 do
						local cf = cf(pos)*randr(40,360,40)
						weirdeff("VBLASTMESH2",{ -- part start properties
							Size=Vector3.new(10,0,10),
							Transparency = .05,
							Material="Neon",
							CFrame = cf
						},{ -- mesh start properties
							VertexColor = glowify(Mode["Colors"][rand(1,4)],Vector3.new),
							Scale = Vector3.new(.05,0,.05)
						},{ -- part end properties
							Transparency = 1,
							CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
						},{ -- mesh end properties
							Scale = Vector3.new(8,0,8)*(math.random(50,100)/100)
						},math.random(40,160)/80,"Quad","Out")
					end
					particle(bean,2,{ZOffset=1,
						Texture="rbxassetid://2545920866",
						LightEmission=1,
						Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
						Lifetime=NR(1),
						Size=NS({NSK(0,150,0),NSK(1,200,0)}),
						Transparency = NS({NSK(0,0.5,0),NSK(1,1,0)})
					})
					bean:ClearAllChildren()
					swait(5)
					hb.Color = Color3.new(0,0,1)
					swait(60)
					bean:Destroy()
				end)
			end
			swait(5)
		end
		egg1.Enabled = false
		swait(180)
		e:Destroy()
	end))
	ej:Destroy()
	swait(15)
	attack,idlesoff = false,false
end

function Template()
	attack,idlesoff = true,true
	--root.Anchored = false
	
	attack,idlesoff = false,false
	--root.Anchored = false
end

function getCustomer()
	attack = true
	getMode(Modes["Sozosei"],"Sozosei")
	root.Anchored = true
	local cfe = root.CFrame*CFrame.new(0,1000,0)
	RTweenAnimate(root,{CFrame=cfe},1,"Sine","Out")
	delay(1,function() root.CFrame = cfe end)
	local c = resources.customer:Clone()
	c.cScript.Disabled = false
	c.module.Value = script.stupidAnimThing
	c.datastorestuff.Disabled = false
	c.Parent = Player.PlayerGui
	script.Custom:InvokeClient(plr,c)
	root.Anchored = false
	getMode(Modes["Q"],"Q")
	attack = false
end

function upperFirst(tex)
	return string.upper(string.sub(tex,1,1)) .. string.lower(string.sub(tex,2))
end

