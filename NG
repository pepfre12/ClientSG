bgui.project.Text = "《 NI-CISYNIA // " .. string.upper(char.Name) .. " 》"

function textStuff(name,c1,c2,font,brackets)
	if not brackets then
		modet.Text = "〘" .. name .. "〙"
	else
		modet.Text = name
	end
	modet.TextColor3 = c2
	modet.TextStrokeColor3 = c1
	modet.Font = font
	bgui.project.TextColor3 = c1
	bgui.project.TextStrokeColor3 = c2
	bgui.project.Font = font
	bgui.divider.BackgroundColor3 = c1
	bgui.divider.BorderColor3 = c2
end

--STATS
keysDown = {}

local stats = {}
statStuff = {
	Base={ATK=1,ADMG=0,ASPD=1,DEF=0,DR=0,WS=16,JP=75,JUMPS=2,FLYSPEED=50},
	Add={},
	Multiply={},
	UnleashMultiplier={},
}

local old = hum.Health
local hdebounce = false

function statThing()
	for i,v in pairs(statStuff["Base"]) do
		stats[i] = (v + (statStuff["Add"][i] or 0)) * (statStuff["Multiply"][i] or 1) * (statStuff["UnleashMultiplier"][i] or 1)
	end
end
statThing()

