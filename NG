		Transparency = NS({NSK(0,0,0),NSK(.75,0.5,0),NSK(1,1,0)}),
		LockedToPart=true
	}),2)
	local BOOOOOM = false
	for i,v in pairs({hb1,hb2}) do
		coroutine.resume(coroutine.create(function()
			local e = PART(char)
			e.CFrame = root.CFrame*cf(0,-3,0)
			local egg = marazmify(particle(e,0,{ZOffset=2,
				Texture="rbxassetid://2273224484",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(.1),
				Size=NS({NSK(0,0,0),NSK(1,15,7.5)}),
