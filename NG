--WINGS
handlex = Instance.new("Part")
handlex.Transparency = 1
handlex.Anchored = false
handlex.Size = Vector3.new()
handlex.CanCollide = false
handlex.Massless = true
handlex.Parent = root

handle = handlex:Clone()
CreateWeld(Torso,Torso,handlex,CFrame.new(0,2,2))
handle.Parent = handlex
CreateWeld(handlex,handlex,handle,CFrame.new(0,0,0))

wings = {cores={},lwings={},rwings={},corewelds={},lwingwelds={},rwingwelds={}}

function makeWings(style,numwl,numwr,numc,rcinv)
	wings = {cores={},lwings={},rwings={},corewelds={},lwingwelds={},rwingwelds={}}
	handle:ClearAllChildren()
	for i = 1,numc do
		local w = style.core:Clone()
		table.insert(wings.cores,w)
		local weld = CreateWeld(handle,handle,w.main)
		table.insert(wings.corewelds,weld)
		local weld = resources.qPerfectionWeld:Clone()
		weld.Disabled = false
		weld.Parent = w
		w.Parent = handle
		for i,v in pairs(w:GetChildren()) do
			pcall(function()
				v.Anchored = false
				v.UsePartColor = true
				v.Massless = true
				v.CanCollide = false
				if string.sub(v.Name,1,2) == "cl" and i == 1 and rcinv then
					v.Name = "cl" .. (5-tonumber(string.sub(v.Name,3,3)))
				end
			end)
		end
	end
	for i = 1,numwl do
		local w = style.wing:Clone()
		table.insert(wings.lwings,w)
		local weld = CreateWeld(handle,handle,w.main)
		table.insert(wings.lwingwelds,weld)
		local weld = resources.qPerfectionWeld:Clone()
		weld.Disabled = false
		weld.Parent = w
		w.Parent = handle
		for i,v in pairs(w:GetDescendants()) do
			pcall(function()
				v.Anchored = false
				v.UsePartColor = true
				v.Massless = true
				v.CanCollide = false
			end)
		end
	end
	for i = 1,numwr do
		local w = style.wing:Clone()
		table.insert(wings.rwings,w)
		local weld = CreateWeld(handle,handle,w.main)
		table.insert(wings.rwingwelds,weld)
		local weld = resources.qPerfectionWeld:Clone()
		weld.Disabled = false
		weld.Parent = w
		w.Parent = handle
		for i,v in pairs(w:GetDescendants()) do
			pcall(function()
				v.Anchored = false
				v.UsePartColor = true
				v.Massless = true
				v.CanCollide = false
				if string.sub(v.Name,1,2) == "cl" and rcinv then
					v.Name = "cl" .. (5-tonumber(string.sub(v.Name,3,3)))
				end
			end)
		end
	end
end

function Recolor(colors,transp,mat)
	colors[3],colors[4],transp,mat = colors[3] or colors[1]:lerp(colors[2],.33), colors[4] or colors[2]:lerp(colors[1],.33), transp or {}, mat or {}
	local function recol(v)
		for i,v in pairs(v:GetDescendants()) do
			pcall(function()
				if string.sub(v.Name,1,2) == "cl" then
					if v:IsA("BasePart") then
						v.Color = (colors[tonumber(string.sub(v.Name,3,3))]) or colors[1]
						v.Transparency = (transp[tonumber(string.sub(v.Name,3,3))]) or 0
						v.Material = Enum.Material[(mat[tonumber(string.sub(v.Name,3,3))]) or "Neon"]
					elseif v:IsA("ParticleEmitter") or v:IsA("Beam") or v:IsA("Trail") then
						v.Color = CS((colors[tonumber(string.sub(v.Name,3,3))]) or colors[1])
						v.Enabled = (((transp[tonumber(string.sub(v.Name,3,3))]) or 0) < 1) or v == aura
					elseif v:IsA("PointLight") then
						v.Color = (colors[tonumber(string.sub(v.Name,3,3))]) or colors[1]
					end
				end
			end)
		end
	end
	for i,v in pairs(wings.cores) do recol(v) end
	for i,v in pairs(wings.rwings) do recol(v) end
	for i,v in pairs(wings.lwings) do recol(v) end
	recol(aura)
end

--HITBOXES
debugmode = false

function makeHitbox(hitbProps,dam,multihit,startactive,critchance,hitfunc)
	local exclude = {}
	local e = Instance.new("Part")
	local function check(v)
		for f,h in pairs(exclude) do
			if h == v then
				return
			end
		end
		if v.Name ~= "HumanoidRootPart" and v.Name ~= "Head" then table.insert(exclude,v) return end
		if v.Name == "Effect" or v.Name == "hitboxe" or v.Parent == char or v.Parent == nil then table.insert(exclude,v) return end
		local enem = v.Parent
		if enem:IsA("Model") then
			local ehum = enem:FindFirstChildOfClass("Humanoid")
			local etors = enem:FindFirstChild("Head")
			if ehum and etors then
				for f,h in pairs(exclude) do
					if h == ehum then
						return
					end
				end
				local dam = (dam * stats["ATK"]) + stats["ADMG"]
				if math.random(1,critchance or 10) == 1 then
					--ehum:TakeDamage(dam*3) -- critical hit
					ehum:TakeDamage(dam)
				else
					ehum:TakeDamage(dam)
				end
				if not multihit then
					table.insert(exclude,ehum)
				end
				if hitfunc then
					local eroot = enem:FindFirstChild("HumanoidRootPart")
					hitfunc(e,eroot,ehum,etors)
				end
			end
		end
	end
	e.Anchored = true
	e.Transparency = 1
	if debugmode then e.Transparency = .8 end
	e.CanCollide = false
	e.Material = "Neon"
	e.Massless = true
	e.Name = "hitboxe"
	for i,v in pairs(hitbProps) do
		e[i] = v
	end
	e.Parent = char
	local egg = startactive
	if startactive then
		e.Color = Color3.new(1,0,0)
		pcall(function()
			for i,v in pairs(e:GetTouchingParts()) do
				check(v)
			end
		end)
	else
		e.Color = Color3.new(0,1,0)
	end
	local ev1 = e.Changed:connect(function(prop)
		if prop == "Color" then
			if e.Color == Color3.new(1,0,0) then
				egg = true
				for i,v in pairs(e:GetTouchingParts()) do
					check(v)
				end
			elseif e.Color == Color3.new(0,0,1) then
				if egg then
					for i,v in pairs(e:GetTouchingParts()) do
						check(v)
					end
				end
				e:Destroy()
			end
		end
	end)
	local conect = e.Touched:connect(function(h)
		if e.Color == Color3.new(1,0,0) then
			check(h)
		end
	end)
	coroutine.resume(coroutine.create(function()
		repeat swait() until e.Parent == nil
		ev1:Disconnect()
		conect:Disconnect()
		return
	end))
	return e
end

--ABILITIES
function dash(dir)
	attack,idlesoff = true,true
	local function effect(angle)
		particle(root.CFrame*angles(0,math.rad(angle),0),40*(1+(stats["WS"]/250)),{ZOffset=2,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			EmissionDirection = "Front",
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(20/60,40/60),
			Size=NS({NSK(0,4*((1+(stats["WS"]/25))),0),NSK(0.1,.6*((1+(stats["WS"]/25))),0),NSK(0.2,.4*((1+(stats["WS"]/25))),0),NSK(0.3,.6*((1+(stats["WS"]/25))),0),NSK(0.4,.4*((1+(stats["WS"]/25))),0),NSK(0.5,.6*((1+(stats["WS"]/25))),0),NSK(0.6,.4*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
			Speed = NR(4*((1+(stats["WS"]/25))),80*((1+(stats["WS"]/25)))),
			SpreadAngle=Vector2.new(20,20),
			Acceleration = Vector3.new(0,-5,0),
			Drag = 5})
		particle(root.CFrame*angles(0,math.rad(angle),0),3,{ZOffset=4,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(20/60),
			Size=NS({NSK(0,15*((1+(stats["WS"]/25))),0),NSK(0.1,7.5*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
			RotSpeed=NR(180)})
		weirdeff("VBLASTMESH2",{
			Transparency = 0.05,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,.02,0),
		},{
			Scale = Vector3.new(.01,.01,.01)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(5,5,5)
		},{
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,4.9,0),
		},{
			Scale = Vector3.new(.025,.5,.025)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(1,1,1)
		},30/60,"Quint","Out")
		weirdeff("VBLASTMESH2",{
			Transparency = 0.05,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,5,0),
		},{
			Scale = Vector3.new(.01,.5,.01)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(5,5,5)
		},{
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,.5,0),
		},{
			Scale = Vector3.new(.05,.05,.05)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(1,1,1)
		},45/60,"Quint","Out")
	end
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 3/60,'Linear','Out',0,false,0
	if dir == "w" then
		--GetLocalProperty(hum,"CameraOffset",Vector3.new(0,0,walkspeed*.5))
		effect(0)
		root.CFrame = root.CFrame*CFrame.new(0,0,-walkspeed*.5)
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.1 + (Mode["Flying"] or 0),-0.3)*angles(math.rad(-35),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-1,-1.2,0.1)*angles(math.rad(-34.9),math.rad(4.1),math.rad(-2.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1,-0.7,-1.1)*angles(math.rad(-45),math.rad(-4.9),math.rad(0.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.6,0.6,0.2)*angles(math.rad(-54.6),math.rad(17.9),math.rad(-17.5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.6,0.6,0.3)*angles(math.rad(-50.1),math.rad(-18.2),math.rad(21.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.6)*angles(math.rad(25),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "s" then
		--GetLocalProperty(hum,"CameraOffset",Vector3.new(0,0,-walkspeed*.5))
		effect(180)
		root.CFrame = root.CFrame*CFrame.new(0,0,walkspeed*.5)
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.1 + (Mode["Flying"] or 0),0.3)*angles(math.rad(35),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-0.9,-0.5,-0.1)*angles(math.rad(20.9),math.rad(9.8),math.rad(-5.4))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1,-1.3,0)*angles(math.rad(30.4),math.rad(-6.6),math.rad(2.4))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.5,0.4,-0.2)*angles(math.rad(42.4),math.rad(-14),math.rad(-20.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.6,0.3,-0.2)*angles(math.rad(42.4),math.rad(14),math.rad(20.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.5)*angles(math.rad(-30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "a" then
		--GetLocalProperty(hum,"CameraOffset",Vector3.new(walkspeed*.5,0,0))
		effect(90)
		root.CFrame = root.CFrame*CFrame.new(-walkspeed*.5,0,0)
		local eg = RTweenAnimate(RootJoint,{C0=cf(-0.2,-0.1 + (Mode["Flying"] or 0),-0.1)*angles(math.rad(-33.1),math.rad(21.5),math.rad(32.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-1.2,-0.9,-0.9)*angles(math.rad(-40.6),math.rad(6.8),math.rad(13.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(0.9,-0.8,0)*angles(math.rad(-8),math.rad(-27.7),math.rad(28.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-0.7,0.9,-0.8)*angles(math.rad(125.4),math.rad(-24.6),math.rad(79))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.5,0.3,0.3)*angles(math.rad(4),math.rad(-35.3),math.rad(60.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0.1,-0.5)*angles(math.rad(22.1),math.rad(-20.4),math.rad(1.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "d" then
		--GetLocalProperty(hum,"CameraOffset",Vector3.new(-walkspeed*.5,0,0))
		effect(-90)
		root.CFrame = root.CFrame*CFrame.new(walkspeed*.5,0,0)
		local eg = RTweenAnimate(RootJoint,{C0=cf(.2,-.1 + (Mode["Flying"] or 0),-0.1)*angles(math.rad(-33.1),math.rad(-21.5),math.rad(-32.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-0.9,-0.8,-0.1)*angles(math.rad(-8),math.rad(27.7),math.rad(-28.6))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1.1,-0.9,-0.9)*angles(math.rad(-40.6),math.rad(-6.8),math.rad(-13.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.4,0.3,0.3)*angles(math.rad(4),math.rad(35.3),math.rad(-60.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1,1,-0.7)*angles(math.rad(125.4),math.rad(24.6),math.rad(-79))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,-0.3)*angles(math.rad(22.1),math.rad(20.4),math.rad(-1.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	end
	swait()
	--LocalTweenAnimate(hum,{CameraOffset=Vector3.new(0,0,0)},10/60,"Sine","Out")
	statStuff["Add"]["WS"] = 48
	statStuff["Multiply"]["WS"] = 4
	statStuff["Multiply"]["FLYSPEED"] = 4
	statThing()
	walkspeed = stats["WS"]
	hum.WalkSpeed = stats["WS"]
	swait(10)
	attack,idlesoff = false,false
	statStuff["Add"]["WS"] = 18
	statStuff["Multiply"]["WS"] = 2.5
	statStuff["Multiply"]["FLYSPEED"] = 2.5
	statThing()
	walkspeed = stats["WS"]
	repeat swait() until attack == true or not (keysDown["w"] or keysDown["s"] or keysDown["a"] or keysDown["d"])
	statStuff["Add"]["WS"] = 0
	statStuff["Multiply"]["WS"] = 1
	statStuff["Multiply"]["FLYSPEED"] = 1
	statThing()
	if not attack then
		walkspeed = stats["WS"]
	end
end

function doublejump()
	attack,idlesoff = true,true
	particle(root.CFrame*angles(math.rad(90),0,0),40*(1+(stats["WS"]/250)),{ZOffset=2,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		EmissionDirection = "Front",
		Color=CS(Color3.new(1,1,1)),
		Lifetime=NR(20/60,40/60),
		Size=NS({NSK(0,4*((1+(stats["WS"]/25))),0),NSK(0.1,.6*((1+(stats["WS"]/25))),0),NSK(0.2,.4*((1+(stats["WS"]/25))),0),NSK(0.3,.6*((1+(stats["WS"]/25))),0),NSK(0.4,.4*((1+(stats["WS"]/25))),0),NSK(0.5,.6*((1+(stats["WS"]/25))),0),NSK(0.6,.4*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
		Speed = NR(4*((1+(stats["WS"]/25))),80*((1+(stats["WS"]/25)))),
		SpreadAngle=Vector2.new(20,20),
		Acceleration = Vector3.new(0,-5,0),
		Drag = 5})
	particle(root.CFrame*angles(math.rad(90),0,0),3,{ZOffset=4,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		Color=CS(Color3.new(1,1,1)),
		Lifetime=NR(20/60),
		Size=NS({NSK(0,15*((1+(stats["WS"]/25))),0),NSK(0.1,7.5*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
		RotSpeed=NR(180)})
	weirdeff("VBLASTMESH2",{
		Transparency = 0.05,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,.02,0),
	},{
		Scale = Vector3.new(.01,.01,.01)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(5,5,5)
	},{
		Transparency = 1,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,5,0),
	},{
		Scale = Vector3.new(.025,.5,.025)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(1,1,1)
	},30/60,"Quad","Out")
	weirdeff("VBLASTMESH2",{
		Transparency = 0.05,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,5,0),
	},{
		Scale = Vector3.new(.01,.5,.01)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(5,5,5)
	},{
		Transparency = 1,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,.5,0),
	},{
		Scale = Vector3.new(.1,.05,.1)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(1,1,1)
	},45/60,"Quad","Out")
	Mode["jumped"] = (tonumber(Mode["jumped"]) or 0) + 1
	local bv = Instance.new("BodyVelocity")
	bv.Velocity = Vector3.new(root.Velocity.X*1.25,stats["JP"],root.Velocity.Z*1.25)
	bv.MaxForce = Vector3.new(1e9,1e10,1e9)
	bv.Parent = root
	for i = 1,5 do
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 2/60,'Linear','Out',0,false,0
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0)*angles(math.rad(-step*20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-0.9,-0.7,-1)*angles(math.rad(-55.4),math.rad(-5),math.rad(-5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(0.9,-0.8,-1)*angles(math.rad(-55.4),math.rad(5),math.rad(5))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.3,0,-0.5)*angles(math.rad(106.8),math.rad(16.3),math.rad(53.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.2,0.1,-0.6)*angles(math.rad(110),math.rad(-8.6),math.rad(-59.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.4)*angles(math.rad(-30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(2)
	end
	bv:Destroy()
	attack,idlesoff = false,false
end

function hardlanding(p)
	attack,idlesoff = true,true
	root.Anchored = true
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 5/60,'Sine','In',0,false,0
	RTweenAnimate(RootJoint,{C0=cf(0,-1.1,-0.2)*rang(-50,0,0)*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.7,-0.7)*rang(-19.6,3,-6.4)*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,0.2,-0.3)*rang(44.1,9.5,5.9)*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.4,0.3,-1)*rang(58,-17.4,-4.6)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,0.3,-0.9)*rang(55.1,12.2,7.1)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(char.Torso.Neck,{C0 =cf(0,0.3,0.7)*rang(-45,0,0)*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(root,{Position=p + Vector3.new(0,3,0)},5/60,"Sine","In").Completed:wait()
	sound(root,"5132759818",.7,2)
	sound(root,"341301373",2,2.5)
	root.Velocity = Vector3.new(0,0,0)
	weirdeff("Winds",{
		Material="ForceField",
		Transparency=-15,
		Color=Color3.new(1,1,1),
		CFrame=root.CFrame*CFrame.new(0,25,0),
		Size=Vector3.new(0,50,0)
	},{Transparency = 1},{
		Size=Vector3.new(100,50,100),
		Transparency=1,
		CFrame=root.CFrame*CFrame.new(0,25,0)*rang(0,179,0)
	},{},90/60,"Quint","Out")
	weirdeff("Swirl",{
		Material="ForceField",
		Transparency=-15,
		Color=Color3.new(1,1,1),
		CFrame=root.CFrame*CFrame.new(0,-25,0),
		Size=Vector3.new(80,50,80)
	},{Transparency = 1},{
		Size=Vector3.new(120,50,120),
		Transparency=1,
		CFrame=root.CFrame*CFrame.new(0,40,0)*rang(0,-179,0)
	},{},75/60,"Quint","Out")
	weirdeff("Blast",{
		Material="Neon",
		Transparency=-2,
		Color=Color3.new(1,1,1),
		CFrame=root.CFrame*CFrame.new(0,12,0),
		Size=Vector3.new(10,30,10)
	},{Transparency = 1},{
		Size=Vector3.new(50,100,50),
		Transparency=1,
		CFrame=root.CFrame*CFrame.new(0,47,0)
	},{},20/60,"Sine","Out")
	weirdeff("Explosion2",{
		Material="Neon",
		Transparency=.5,
		Color=Color3.new(1,1,1),
		CFrame=root.CFrame*CFrame.new(0,12,0),
		Size=Vector3.new(10,30,10)
	},{Transparency = 1},{
		Size=Vector3.new(50,10,50),
		Transparency=1,
		CFrame=root.CFrame*CFrame.new(0,2,0)
	},{},90/60,"Quad","Out")
	weirdeff("SpikeBall",{
		Material="Neon",
		Transparency=-2,
		Color=Color3.new(1,1,1),
		CFrame=root.CFrame,
		Size=Vector3.new(5,5,5)
	},{Transparency = 1},{
		Size=Vector3.new(100,100,100),
		Transparency=1
	},{},20/60,"Quint","Out")
	for i = 1,5 do
		local egg = root.CFrame*randr()
		weirdeff("VBLASTMESH2",{
			Color = Color3.new(1,1,1),
			Transparency=0.05,
			CFrame=egg
		},{
			VertexColor=glowify(Color3.new(1,1,1),Vector3.new),
			Scale=Vector3.new(0.05,0,0.05)
		},{
			Transparency=1,
			CFrame=egg*randr(10,360,10)
		},{
			Scale=Vector3.new(1.25,0,1.25)*(rand(50,100)/100)
		},20/60,"Sine","Out")
	end
	for i = 1,2 do
		weirdeff("VBLASTMESH2",{
			Color = Color3.new(1,1,1),
			Transparency=0.05,
			CFrame=root.CFrame*cf(0,27,0)
		},{
			VertexColor=glowify(Color3.new(1,1,1),Vector3.new),
			Scale=Vector3.new(0.1,3,0.1)
		},{
			CFrame=root.CFrame*cf(0,7,0),
			Transparency=1
		},{
			Scale=Vector3.new(1.5,1,1.5)
		},30/60,"Sine","Out")
	end
	swait(45)
	root.Velocity = Vector3.new(0,0,0)
	attack,idlesoff = false,false
	root.Anchored = false
end

anchorer = function(hb,eroot,ehum)
	if ehum.Health > 0 then
		eroot.Anchored = true
		swait(60)
		if attack == false then
			eroot.Anchored = false
		else
			repeat swait() until attack == false
			eroot.Anchored = false
		end
	end
end

function click1()
	attack = true
	tweenDisable = {Torso.Neck,RW,RootJoint}
	sound(root,"4701290769",1.25,.75)
	sound(root,"4701312550",1.25,.75)
	local ler1 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(5),math.rad(25),math.rad(0))*RootCF,.5)
	local ler2 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(-5),math.rad(25),math.rad(0))*RootCF,.5)
	particle(char["Right Arm"],2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(0.2),
		Size=NS({NSK(0,0,0),NSK(1,7.5,0)}),
		Transparency = NS({NSK(0,0,0),NSK(.75,0.5,0),NSK(1,1,0)}),
		LockedToPart=true
	},Vector3.new(0,-1,0))
	particle(char["Right Arm"],3,{ZOffset=1,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(0.4),
