effectstuff = resources["effectstuff"]

function weirdeff(name,startprop,startprop1,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb,server,marazm)
	if not server then
		Whatever:FireAllClients("weirdeff",name,startprop,startprop1,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb,Mode["Name"] == "MARAZM" and marazm)
	else
		local e = effectstuff[name]:Clone()
		for i,v in pairs(e:GetChildren()) do
			--if v:IsA("Decal") then
				for d,g in pairs(startprop1) do
					v[d] = g
				end
			--end
		end
		for d,g in pairs(startprop) do
			e[d] = g
		end
		e.Anchored = true
		e.CanCollide = false
		e.Massless = true
		e.Name = "Effect"
		e.Parent = workspace:FindFirstChild("Effects") or workspace
		local er = RTweenAnimate(e,endprop,timer,easing,easing2,repet,reverse,del)
		for i,v in pairs(e:GetChildren()) do
			RTweenAnimate(v,endprop1,timer,easing,easing2,repet,reverse,del)
		end
		if not deb then
			delay((timer*((reverse and 2) or 1)*((repet+1)) or 1)+del,function()
				e:Destroy()
			end)
		end
		return e
	end
end

function weldedweirdeff(name,part,parenttopart,wstartc0,startprop,startprop1,wendc0,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb,server,marazm)
	if not server then
		Whatever:FireAllClients("weldedweirdeff",name,part,parenttopart,wstartc0,startprop,startprop1,wendc0,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb,Mode["Name"] == "MARAZM" and marazm)
	else
		-- im doing this when i actually need it
	end
end

function particle(pos,num,props,attPos)
	local part = pos
	if typeof(pos) == typeof(CFrame.new()) then
		part = Instance.new("Part")
		part.Anchored = true
		part.CFrame = pos
		part.Transparency = 1
		part.CanCollide = false
		part.Parent = workspace:FindFirstChild("Effects") or workspace
	end
	local at = Instance.new("Attachment",part)
	at.Position = attPos or Vector3.new(0,0,0)
	local eff = Instance.new("ParticleEmitter",at)
	eff.Enabled = false
	eff.Speed = NumberRange.new(0)
	for i,v in pairs(props) do
		eff[i] = v
	end
	if num ~= 0 then
		coroutine.resume(coroutine.create(function()
			eff:Emit(num)
			wait(10)
			eff:Destroy()
		end))
	else
		eff.Enabled = true
	end
	return eff
end

function slashy(cfr,color,sizex,sizey,spins,spinspeeds,wahoo,move)
	Whatever:FireAllClients("slashy",cfr,color,sizex,sizey,spins,spinspeeds,wahoo,move)
end

function sound(p,id,pitch,vol,loop)
	local s = Instance.new("Sound")
	s.SoundId = "rbxassetid://" .. id
	s.Volume = (vol or 1)
	s.EmitterSize = 20*(vol or 1)
	s.Looped = loop or false
	s.PlaybackSpeed = pitch or 1
	local kill = false
	if typeof(p) == typeof(CFrame.new()) then
		kill = true
		local pe = Instance.new("Part")
		pe.CFrame = p
		pe.Anchored = true
		pe.CanCollide = false
		pe.Size = Vector3.new()
		pe.Transparency = 1
		pe.Parent = char
		p = pe
	end
	s.Parent = p
	game:GetService("Debris"):AddItem(s,10)
	s:Play()
	if not loop then
		s.Ended:connect(function()
			s:Destroy()
			if kill then
				p:Destroy()
			end
		end)
	end
	return s
end
 
local function getTextObject(message, fromPlayerId)
	local textObject
	local success, errorMessage = pcall(function()
		textObject = game:GetService("TextService"):FilterStringAsync(message, fromPlayerId, 2)
	end)
	if success then
		return textObject
	end
	return false
end
 
local function getFilteredMessage(textObject,id)
	local filteredMessage
	local success, errorMessage = pcall(function()
		filteredMessage = textObject:GetNonChatStringForUserAsync(id)
	end)
	if success then
		return filteredMessage
	end
	return false
end

function dialog(name,texts,image,times,sound,imagestuff,noup,filter)
	if filter then
		for i,v in pairs(game.Players:GetChildren()) do
			local filteredTexts = {}
			for x,c in pairs(texts) do
				local ge = getTextObject(c,plr.UserId)
				if ge then
					local eg = getFilteredMessage(ge,v.UserId)
					if eg then
						table.insert(filteredTexts,eg)
					else
						filteredTexts = {"filtering failed, blame roblox"}
						times = {.5}
						break
					end
				else
					filteredTexts = {"filtering failed, blame roblox"}
					times = {.5}
					break
				end
			end
			Whatever:FireClient(v,"dialog",Mode,char,name,filteredTexts,image,times,sound,imagestuff,noup)
		end
	else
		Whatever:FireAllClients("dialog",Mode,char,name,texts,image,times,sound,imagestuff,noup)
	end
end

function SKILLCUTIN(cl1,cl2,tx1,tx2,sound)
	Whatever:FireAllClients("SKILLCUTIN",cl1,cl2,tx1,tx2,sound,char)
end

-- end effect functions, begin everything else

function CreateWeld(parent,part0,part1,C0,C1)
	local weld = Instance.new("Motor6D")
	weld.Part0 = part0
	weld.Part1 = part1
	weld.C1 = C1 or CFrame.new()
	weld.C0 = C0 or CFrame.new()
	weld.Parent = parent
	return weld
end

function rayCast(Pos, Dir, Max, Ignore)  -- Origin Position , Direction, MaxDistance , IgnoreDescendants
	local ign = {Ignore}
	for i,v in pairs(players:GetPlayers()) do
		table.insert(ign,v.Character)
	end
	return workspace:FindPartOnRayWithIgnoreList(Ray.new(Pos, Dir.unit * (Max or 999.999)), ign) 
end 

function rsign()
	return rand(1,2)*2 - 3
end

function rang(x,y,z)
	return ang(rad(x),rad(y),rad(z))
end

function randr(maxX,maxY,maxZ)
	return CFrame.Angles(rad(rand(-(maxX or 360),(maxX or 360))),rad(rand(-(maxY or 360),(maxY or 360))),rad(rand(-(maxZ or 360),(maxZ or 360))))
end

function glowify(c,ret)
	local e = rgb((c.R*65)^2,(c.G*65)^2,(c.B*65)^2)
	return (ret or Color3.new)(e.R,e.G,e.B)
end

function PART(p)
	local e = Instance.new("Part")
	e.Anchored = true
	e.CanCollide = false
	e.Transparency = 1
	e.Size = Vector3.new()
	e.Material = "Neon"
	e.Color = Mode["Colors"][1]
	e.Massless = true
	e.Parent = p
	return e
end

function TRAIL(p,s)
	local a1 = Instance.new("Attachment",p)
	local a2 = Instance.new("Attachment",p)
	a1.Position = Vector3.new(0,(s or 1)/2,0)
	a2.Position = Vector3.new(0,-(s or 1)/2,0)
	local e = Instance.new("Trail")
	e.WidthScale = NS({NSK(0,1,0),NSK(1,0,0)})
	e.LightEmission = 1
	e.Attachment0 = a1
	e.Attachment1 = a2
	e.FaceCamera = true
	e.Transparency = NS({NSK(0,0,0),NSK(1,1,0)})
	e.Lifetime = 0.3
	e.Parent = p
	return e,a1,a2
end

