			sound(e,"3748209965",.9,5)
			local anim = RTweenAnimate(e,{Position=v.Position},1,"Sine","InOut")
			delay(1,function()
				delay(1,function() e:Destroy() end)
				sound(e.CFrame,"3744388331",.9,5)
				sound(e.CFrame,"459523787",.9,3)
				for i = 1,4 do
					slashy(v.CFrame*rang(90,0,rand(1,360)),Mode["Name"] ~= "MARAZM" and Mode["Colors"][1] or Color3.new(math.random(),math.random(),math.random()),{0,512,1024},25,2,{10*(rand(5,15)/10),20*(rand(5,15)/10)})
				end
				weirdeff("sphere",{
					Size=Vector3.new(50,50,50),
					CFrame=v.CFrame,
					Material = "Neon",
					Transparency = 0,
					Color = Mode["Colors"][1]
				},{
				},{
					Size=Vector3.new(1024,1024,1024),
					Transparency = 1,
				},{
				},15/60,"Quad","Out",nil,nil,nil,nil,nil,true)
				weirdeff("SpikeBall",{
					Size=Vector3.new(100,100,100),
					CFrame=v.CFrame,
					Material = "Neon",
					Transparency = 0.5,
					Color = Mode["Colors"][2]
				},{Transparency = 1,
				},{
