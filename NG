customModes = {}

function getCustoms()
	customModes = {}
	datastore = {}
	local success,fail = pcall(function()
		local bingbong = dataStores:GetDataStore("ncCustom",plr.UserId)
		local kees = string.split("Q E R T Y U F G H J K L"," ")
		local types = string.split(",Major,Sub 1,Sub 2,Ultimate",",")
		for f,h in pairs(types) do
			for x,c in pairs(kees) do
				local datastore = {}
				pcall(function()
					if h ~= "" then
						if not customModes[c] then
							return
						end
					end
					local keys = bingbong:GetAsync(c..h.."keys")
					if not keys then return end
					keys = game.HttpService:JSONDecode(keys)
					local values = game.HttpService:JSONDecode(bingbong:GetAsync(c..h.."values"))
					for i,v in pairs(keys) do
						if v == "CAnim" then
							v = "CoreAnim"
						elseif v == "WAnim" then
							v = "WingAnim"
						elseif v == "IAnim" then
							v = "Idle"
						end
						datastore[v] = values[i]
					end
					for i,v in pairs(datastore) do
						if typeof(v) == typeof({"bruh"}) or typeof(v) == typeof({bruh=true}) then
							for x,c in pairs(v) do
								if typeof(c) == typeof({}) and i == "Colors" then 
									v[x] = Color3.new(c[1],c[2],c[3])
								end
							end
						end
					end
					if h == "Major" and customModes[c] then
						customModes[c.."Major"] = datastore
						customModes[c]["M"] = c.."Major"
					elseif h == "Sub 1" and customModes[c] then
						customModes[c.."Sub1"] = datastore
						customModes[c]["N"] = c.."Sub1"
					elseif h == "Sub 2" and customModes[c] then
						customModes[c.."Sub2"] = datastore
						customModes[c]["B"] = c.."Sub2"
					elseif h == "Ultimate" and customModes[c] and customModes[customModes[c]["M"]] then
						customModes[c.."Ult"] = datastore
						customModes[customModes[c]["M"]]["M"] = c.."Ult"
					elseif h == "" then
						customModes[c] = datastore
					end
				end)
			end
		end
		return datastore
	end)
	Modes = customModes
	getMode(Modes["Q"])
end

Sides = {one=ModesP1,two=getCustoms}

Modes = Sides["one"]

cantdie = false

hum.HealthChanged:connect(function(new)
	if new ~= 0 then
		hum.Health = hum.MaxHealth/2
		local change = new - old
		if change < 0 then
			local regen = (math.abs(change*(stats["DR"])) + stats["DEF"])
			if regen < math.abs(change) then
				fakeHealth['Current'] = fakeHealth['Current'] - (math.abs(change)-regen)
			end
		else
			fakeHealth['Current'] = fakeHealth['Current'] + math.abs(change)
		end
		if fakeHealth['Current'] < 0 and not cantdie then
			repeat swait() until not attack
			RESURRECTION()
		end
		old = hum.Health
	end
end)

Anim = "Idle"

function getMode(list,id)
	if list["Name"] == "what" and plr.Name ~= "doz_koob" and plr.Name ~= "divineMoai" then return end
	makeWings(resources.WingModels[wingstyle], list["Wings"]["Left"],list["Wings"]["Right"], list["Wings"]["Cores"],list["Wings"]["Invert"])
	Recolor(list["Colors"],list["Transparency"],list["Materials"])
	sv.Value = list["Theme"]["Name"]
	textStuff(list["Name"],list["Colors"][1],list["Colors"][3] or list["Colors"][2],list["Font"]) -- star in gaelician
	statStuff["Base"] = list["Stats"]
	statThing()
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	local glide = Mode["gliding"]
	Mode["Moves"] = list["Moves"]
	Mode = {ID=id,Name=list["Name"] == "what" and "MARAZM" or list["Name"],IAnim=list["Idle"],WAnim=list["WingAnim"],CAnim=list["CoreAnim"],Flying=list["Flying"],Moves=list["Moves"],jumped=0,gliding=nil,
		Colors=list["Colors"],Transparency=list["Transparency"],Materials=list["Materials"],M=list["M"],N=list["N"],B=list["B"],P=list["P"],CS=list["CS"],Taunt=list["Taunt"]}
	if Mode["Flying"] > 0 and glide then
		Mode["gliding"] = glide
		Mode["jumped"] = 9999
	elseif glide then
		glide:Destroy()
	end
	Whatever:FireAllClients("wings",Mode,step,wings,aura,music,char,root)
	newTheme(list["Theme"]["ID"],list["Theme"]["Pitch"],list["Theme"]["Volume"])
end

local clicky = 1

function b1d()
	if attack == false then
		if Anim == "Jump/Fall" then
			if (Mode["jumped"] - 1) < (stats["JUMPS"] - 1) then
				doublejump()
			end
			airclick1(true)
		elseif Mode["gliding"] then
			if clicky == 1 or clicky == 2 or clicky == 3 then
				airclick1()
			elseif clicky == 4 then
				airclick2()
				clicky = 0
			end
			clicky = clicky + 1
		else
			if clicky == 1 then
				click1()
			elseif clicky == 2 then
				click2()
			elseif clicky == 3 then
				click3()
			elseif clicky == 4 then
				click4()
				clicky = 0
			end
			clicky = clicky + 1
		end
	end
end

taunting = false

function keydown(k)
	pcall(function() keysDown[k] = true end)
	if not attack then
		if Sides[k] then
			if typeof(Sides[k]) == typeof(function() end) then
				Sides[k]()
			else
				Modes = Sides[k]
			end
			getMode(Modes["Q"],"Q")
		elseif Modes[string.upper(k)] then getMode(Modes[string.upper(k)],string.upper(k))
		elseif Modes[Mode[string.upper(k)]] then getMode(Modes[Mode[string.upper(k)]],Mode[string.upper(k)])
		elseif k == "p" then
			textStuff("???",Color3.new(1,1,1),Color3.new(0,0,0),Enum.Font.Code)
			makeWings(resources.WingModels[wingstyle], rand(0,5),rand(0,5), rand(0,3))
			Mode["Idle"] = math.random(1,7)
			Mode["CAnim"] = math.random(1,3)
			Mode["WAnim"] = math.random(1,5)
			newTheme("3112443138")
			Recolor({Color3.new(rand(0,255)/255,rand(0,255)/255,rand(0,255)/255),Color3.new(rand(0,255)/255,rand(0,255)/255,rand(0,255)/255),Color3.new(rand(0,255)/255,rand(0,255)/255,rand(0,255)/255),Color3.new(rand(0,255)/255,rand(0,255)/255,rand(0,255)/255)})
			Whatever:FireAllClients("wings",Mode,step,wings,aura,music,char,root)
		elseif k == "rightcontrol" then
			if keysDown["w"] then dash("w")
			elseif keysDown["s"] then dash("s")
			elseif keysDown["a"] then dash("a")
			elseif keysDown["d"] then dash("d")
			elseif stats["FLYSPEED"] > 25 then
				if not Mode["gliding"] then
					local bv = Instance.new("BodyVelocity")
					bv.Velocity = Vector3.new(root.Velocity.X,math.abs(root.Velocity.Y + 100),root.Velocity.Z)*2
					bv.MaxForce = Vector3.new(1e10,1e10,1e10)
					bv.Parent = root
					Mode["gliding"] = bv
				else
					Mode["gliding"]:Destroy()
					Mode["gliding"] = nil
				end
			end
		elseif k == "space" and ((tonumber(Mode["jumped"]) or 0) < ((stats["JUMPS"] - 1) or 0)) and (Anim == "Jump/Fall") then doublejump()
		elseif k == "space" and (Anim == "Jump/Fall" or Mode["gliding"]) and stats["FLYSPEED"] > 25 then
			if not Mode["gliding"] then
				local bv = Instance.new("BodyVelocity")
				bv.Velocity = Vector3.new(root.Velocity.X,math.abs(root.Velocity.Y),root.Velocity.Z)*2
				bv.MaxForce = Vector3.new(1e10,1e10,1e10)
				bv.Parent = root
				Mode["gliding"] = bv
			else
				Mode["gliding"]:Destroy()
				Mode["gliding"] = nil
			end
		elseif k == "semicolon" then if music.IsPlaying then music:Pause() else music:Resume() end
		elseif k == "quote" then if bgui.Enabled then bgui.Enabled = false else bgui.Enabled = true end
		elseif k == "rightbracket" then if aura.Parent == char then aura.Parent = game.Lighting else aura.Parent = char end
		elseif k == "leftbracket" then if debugmode then debugmode = false else debugmode = true end
		elseif k == "zero" then 
			if Mode["Taunt"] and not taunting then
				taunting = true
				dialog(upperFirst(Mode["Name"]) .. " " .. plr.Name, Mode["Taunt"][1],"player",Mode["Taunt"][2],nil,nil,nil,Modes~=ModesP1)
				for i,v in pairs(Mode["Taunt"][2]) do
					swait(v*75)
				end
				swait(75)
				taunting = false
			end
		elseif Mode["Moves"] and Mode["Moves"][k] then 
			if not attack then
				if Mode["GenSelected"] then Mode["GenSelected"] = "AAIJNDGJKA" end
				Mode["Moves"][k](k)
				if Mode["GenSelected"] == "AAIJNDGJKA" then
					sound(handle,"3226828768",1.1)
					textStuff("GENSONO",rgb(180, 180, 180),rgb(230, 160, 160),"SciFi")
					Recolor({rgb(180, 180, 180),rgb(160,230,160),rgb(230, 160, 160),rgb(160,160,230)})
					Mode["Colors"] = {rgb(180, 180, 180),rgb(160,230,160),rgb(230, 160, 160),rgb(160,160,230)}
					Mode["Taunt"] = {{"I'm a little more.. capable than the others.","Showing you how is going to be fun."},{1.25,1.5}}
					Mode["GenSelected"] = false
					if Mode["GenElement"] == "Inferno" then
						Mode["GenElement"] = "Deluge"
					elseif Mode["GenElement"] == "Gale" then
						Mode["GenElement"] = "Inferno"
					else
						Mode["GenElement"] = "Gale"
					end
					virtuemoment()
				end
			end
		elseif k == "eight" then
			local heg = table.find(wingstyles,wingstyle) + 1
			if heg > #wingstyles then heg = 1 end
			wingstyle = wingstyles[heg]
			bgui.project.Text = "《 NI-" .. wingstyle .. " // " .. string.upper(char.Name) .. " 》"
			getMode(Modes[Mode["ID"]],Mode["ID"])
		end
	end
end

mouse.Button1Down:connect(b1d)

mouse.KeyDown:connect(keydown)

mouse.KeyUp:connect(function(k) pcall(function() keysDown[k] = false end) end)

keydown("q") keysDown["q"] = false

local idles = animThing.idles

ArtificialHB.Event:connect(function()
	local torvel=(root.Velocity*Vector3.new(1,0,1)).magnitude
	local velderp=root.Velocity.y
	local hitfloor,posfloor=rayCast(root.Position,(CFrame.new(root.Position,root.Position - Vector3.new(0,1,0))).lookVector,(Mode["longfall"] and 50) or 4.5,char)
	
	-- footplanting code
	local Walking = hum.MoveDirection.magnitude>0
	local FwdDir = (Walking and hum.MoveDirection*root.CFrame.lookVector or Vector3.new())
	local RigDir = (Walking and hum.MoveDirection*root.CFrame.rightVector or Vector3.new())
	local Vec = {
		X=RigDir.X+RigDir.Z,
		Z=FwdDir.X+FwdDir.Z
	};
	local Divide = 1
	if(Vec.Z<0)then
		Divide=math.clamp(-(1.25*Vec.Z),1,2)
	end
	Vec.Z = Vec.Z/Divide
	Vec.X = Vec.X/Divide
	
	if not attack then
		hum.WalkSpeed = walkspeed
		hum.JumpPower = jumppower
	end
	if Mode["gliding"] then
		local bruh = Mode["gliding"]
		local eg,_ = pcall(function()
			local ccf = GetLocalProperty("camera","CFrame")
			if keysDown["w"] then bruh.Velocity = bruh.Velocity:lerp(ccf.lookVector*stats["FLYSPEED"]*2,.05) end
			if keysDown["s"] then bruh.Velocity = bruh.Velocity:lerp(-ccf.lookVector*stats["FLYSPEED"]*2,.05) end
			if keysDown["a"] then bruh.Velocity = bruh.Velocity:lerp(-ccf.rightVector*stats["FLYSPEED"]*2,.05) end
			if keysDown["d"] then bruh.Velocity = bruh.Velocity:lerp(ccf.rightVector*stats["FLYSPEED"]*2,.05) end
			if not keysDown["w"] and not keysDown["s"] and not keysDown["a"] and not keysDown["d"] then
				bruh.Velocity = bruh.Velocity:lerp(Vector3.new(0,0,0),.05)
			end
		end)
		if not eg then bruh:Destroy() Mode["gliding"] = nil end
	end
	if hitfloor and not attack then 
		if Mode["longfall"] then
			hardlanding(posfloor)
			Mode["longfall"] = false
		end
		Mode["jumped"] = 0 
	end
	if not idlesoff then
		local Mode = Mode
		local flying = Mode["Flying"]
		if Mode["gliding"] then
			flying = 0
		end
		if (torvel < 2 and (hitfloor or Mode["gliding"])) or (not hitfloor and math.abs(Torso.Velocity.magnitude) < 1) then
			Anim = "Idle"
			idles(Mode,step,RootJoint,LH,RH,LW,RW,Torso,flying,necko,RootCF,bgui,tweenDisable,true,Whatever)
		elseif torvel >= 32 and (hitfloor or Mode["gliding"]) then
			Anim = "Run"
			local Sine,Alpha,Easing,Direction,Repeat,Reverse,Delay = math.sin(step/12),7/60,'Sine','InOut',0,false,0
			if flying > 0 or Mode["gliding"] then
				RTweenAnimate(RootJoint,{C0=RootCF*cf(0 - 0.15 * math.cos(step / 47),0,flying+0.5*sin(step/32))*angles(math.rad(55*Vec.Z),math.rad(-16*Vec.X - root.RotVelocity.Y),math.rad(2*Vec.X - root.RotVelocity.Y *4.5 + 3 * math.cos(step / 47)))},Alpha,'Linear','Out',0,false,0)
				RTweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(-20 - 8 * math.sin(step/37)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0 - 4 * math.cos(step/46)*Vec.Z),math.rad(-90),math.rad(6*Vec.X))},Alpha,'Linear','Out',0,false,0)
				RTweenAnimate(RH,{C0=cf(1,-0.3,-0.7)*angles(math.rad(-20 - 7 * math.cos(step/46)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(5*Vec.X))},Alpha,'Linear','Out',0,false,0)
				RTweenAnimate(LW,{C0=cf(-1.5,0.7,0.1)*angles(math.rad(-16.1*Vec.Z),math.rad(8.5),math.rad(-23.5- 5 * math.cos(step/42)))},Alpha,'Linear','Out',0,false,0)
				RTweenAnimate(RW,{C0=cf(1.5,0.7,0)*angles(math.rad(-16.1*Vec.Z),math.rad(-11.1),math.rad(17.2- 6 * math.cos(step/45)))},Alpha,'Linear','Out',0,false,0)
				RTweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(-55*Vec.Z),math.rad(5*Vec.X),math.rad(2*Vec.X))},Alpha,'Linear','Out',0,false,0)
			else
				RTweenAnimate(RootJoint,{C0=cf(0,.11-.08*math.cos(step/3),0)*angles(math.rad(-25*Vec.Z),math.rad(-10*math.sin(step/6)),math.rad(-13*Vec.X))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LH,{C0=cf(-1,-1-.37*math.sin(step/6),-.07+.23*math.sin(step/6)*Vec.Z)*angles(math.rad(70*math.cos(step/6)*Vec.Z+10*math.sin(step/6)*Vec.X),math.rad(-2*math.cos(step/6)),math.rad(30*math.cos(step/6)*Vec.X))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RH,{C0=cf(1,-1+.37*math.sin(step/6),-.07-.23*math.sin(step/6)*Vec.Z)*angles(math.rad(-70*math.cos(step/6)*Vec.Z-10*math.sin(step/6)*Vec.X),math.rad(2*math.cos(step/6)),math.rad(-30*math.cos(step/6)*Vec.X))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LW,{C0=cf(-1.5,0.5,0)*angles(math.rad(-80*math.cos(step/6)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RW,{C0=cf(1.5,0.5,0)*angles(math.rad(80*math.cos(step/6)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(-8),math.rad(0),math.rad(10*math.sin(step/6)-40*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			end
		elseif torvel >= 2 and (hitfloor or Mode["gliding"]) then
			Anim = "Walk"
			local Sine,Alpha,Easing,Direction,Repeat,Reverse,Delay = math.sin(step/12),7/60,'Sine','InOut',0,false,0
			if flying > 0 or Mode["gliding"] then
				local eg = RTweenAnimate(RootJoint,{C0=(cf(0+0*-Vec.Z,flying+0.5*sin(step/32),0+0.2*-Vec.Z)*ang(math.rad(0+25*-Vec.Z+-4.5*Vec.X),math.rad(0+0*-Vec.Z+-2.9*Vec.X),math.rad(0+0*-Vec.Z+-25.3*Vec.X)))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LH,{C0=cf(-1.1+0*-Vec.Z,-0.2+0.1*-Vec.Z,-0.5+0.4*-Vec.Z)*ang(math.rad(-2.4+27.5*-Vec.Z+-15*Vec.X),math.rad(7.5+0.8*-Vec.Z),math.rad(2.4+0.2*-Vec.Z+-20*Vec.X))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RH,{C0=cf(1+-0.1*-Vec.Z,-0.8+0.4*-Vec.Z,0.1+0*-Vec.Z)*ang(math.rad(0+15*-Vec.Z+-5*Vec.X),math.rad(-6.5+-0.4*-Vec.Z),math.rad(-3.7+-0.2*-Vec.Z+-15*Vec.X))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LW,{C0=cf(-1.5+0.1*-Vec.Z,0.6+0.2*-Vec.Z,-0.1+0*-Vec.Z)*ang(math.rad(-3.8+37.4*-Vec.Z+28.2*Vec.X),math.rad(-1.4+-11.9*-Vec.Z+-21.6*Vec.X),math.rad(-23.8+-2.7*-Vec.Z+-34.5*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RW,{C0=cf(1.5+0*-Vec.Z,0.6+0.3*-Vec.Z,-0.2+0*-Vec.Z)*ang(math.rad(-4.8+36.7*-Vec.Z+14.1*Vec.X),math.rad(3.7+10.4*-Vec.Z+5.1*Vec.X),math.rad(22.4+4.4*-Vec.Z+19.4*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(Torso.Neck,{C0 =cf(0+0*-Vec.Z,-0.1+0*-Vec.Z,0.1+0.4*-Vec.Z)*ang(math.rad(0+-20*-Vec.Z+-1.3*Vec.X),math.rad(0+0*-Vec.Z+4.8*Vec.X),math.rad(0+0*-Vec.Z+15.1*Vec.X))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			else
				RTweenAnimate(RootJoint,{C0=cf(0,.07-.05*math.cos(step/7),0)*angles(math.rad(-15*Vec.Z),math.rad(-7*math.sin(step/14)),math.rad(-13*Vec.X))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LH,{C0=cf(-1,-1-.13*math.sin(step/14),-.05+.15*math.sin(step/14)*Vec.Z)*angles(math.rad(30*math.cos(step/14)*Vec.Z),math.rad(-2*math.cos(step/14)),math.rad(20*math.cos(step/14)*Vec.X))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RH,{C0=cf(1,-1+.13*math.sin(step/14),-.05-.15*math.sin(step/14)*Vec.Z)*angles(math.rad(-30*math.cos(step/14)*Vec.Z),math.rad(2*math.cos(step/14)),math.rad(-20*math.cos(step/14)*Vec.X))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LW,{C0=cf(-1.5,0.5,0)*angles(math.rad(-30*math.cos(step/14)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RW,{C0=cf(1.5,0.5,0)*angles(math.rad(30*math.cos(step/14)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(2),math.rad(0),math.rad(7*math.sin(step/14)-40*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			end
		elseif not hitfloor then
			Anim = "Jump/Fall"
			if root.Velocity.Y <= -500 then
				Mode["longfall"] = true
			end
			if Mode["longfall"] == true then
				-- ~-~ IDLES FALL ~-~
				local Sine,Alpha,Easing,Direction,Repeat,Reverse,Delay = -math.clamp((root.Velocity.Y + 250)/(hum.JumpPower*5),-1,1),10/60,'Sine','InOut',0,false,0
				local eg = RTweenAnimate(RootJoint,{C0=cf(0+0*Sine,-0.5+-0.3*Sine,-0.2+-0.1*Sine)*ang(rad(-125+-45*Sine),rad(0+0*Sine),rad(0+0*Sine))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LH,{C0=cf(-0.9+-0.1*Sine,-0.9+0.1*Sine,-0.1+0.1*Sine)*ang(rad(-15.1+-2.4*Sine),rad(14.6+8*Sine),rad(-5.7+8.9*Sine))*ang(rad(0),rad(-90),rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RH,{C0=cf(1+0.1*Sine,-0.9+0.1*Sine,0+0.1*Sine)*ang(rad(-13.3+-3*Sine),rad(-19.6+-8.2*Sine),rad(5.7+-8.6*Sine))*ang(rad(0),rad(90),rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LW,{C0=cf(-1.4+0.2*Sine,0.3+0*Sine,0.4+0.1*Sine)*ang(rad(9.8+-36.2*Sine),rad(45.9+-34.3*Sine),rad(-22.4+27.5*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RW,{C0=cf(1.4+-0.2*Sine,0.3+0*Sine,0.4+0*Sine)*ang(rad(10.1+-34.3*Sine),rad(-44.5+31.7*Sine),rad(21.6+-26.9*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(char.Torso.Neck,{C0=cf(0+0*Sine,0.1+-0.2*Sine,-0.8+-0.4*Sine)*ang(rad(49.5+27.5*Sine),rad(0+0*Sine),rad(0+0*Sine))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			else
				local Sine,Alpha,Easing,Direction,Repeat,Reverse,Delay = -math.clamp(root.Velocity.Y/(hum.JumpPower/3),-1,1),3/60,'Sine','InOut',0,false,0
				local eg = RTweenAnimate(RootJoint,{C0=cf(0+0*Sine,flying+0.5*sin(step/32),-0.1+-0.1*Sine)*ang(math.rad(-7.5+-12.5*Sine),math.rad(0+0*Sine),math.rad(0+0*Sine))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LH,{C0=cf(-1+0*Sine,-0.3+0*Sine,-0.3+0.1*Sine)*ang(math.rad(-2.9+12.7*Sine),math.rad(7.9+-2.9*Sine),math.rad(0.5+-2.2*Sine))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RH,{C0=cf(1+0*Sine,-1+0.1*Sine,-0.1+-0.2*Sine)*ang(math.rad(-10.2+-9.5*Sine),math.rad(-7.4+1.6*Sine),math.rad(-1.4+5.3*Sine))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(LW,{C0=cf(-1.3+0.1*Sine,0.7+0.3*Sine,0.3+0*Sine)*ang(math.rad(9.8+35.4*Sine),math.rad(27.1+-3.4*Sine),math.rad(-35.1+-27.1*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(RW,{C0=cf(1.3+0*Sine,0.7+0.3*Sine,0.3+0.1*Sine)*ang(math.rad(8.5+35.2*Sine),math.rad(-24.7+1.7*Sine),math.rad(34.2+29.3*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				RTweenAnimate(Torso.Neck,{C0 =cf(0+0*Sine,-0.1+0*Sine,-0.1+0.3*Sine)*ang(math.rad(7.5+-17.5*Sine),math.rad(0+0*Sine),math.rad(0+0*Sine))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			end
		end
	end
	-- end character anims, begin auras
	
	-- end auras, begin misc. stuff
	aura.Position = posfloor - Vector3.new(0,0.2,0)
	if Mode["CS"] == 3 then
		Mode["Colors"] = {Color3.fromHSV(rand(1,360)/360,1,math.ceil(rand(0,10)/10)),Color3.fromHSV(rand(1,360)/360,math.ceil(rand(0,10)/10),1),Color3.fromHSV(rand(1,360)/360,rand(0,10)/10,rand(0,10)/10),Color3.fromHSV(rand(1,360)/360,rand(0,10)/10,rand(0,10)/10)}
	end
	if fakeHealth["Current"] > fakeHealth["Max"] then fakeHealth["Current"] = fakeHealth["Max"] end
	local hp = math.clamp(fakeHealth["Current"]/fakeHealth["Max"],.01,.99)
	bgui.divider.TextLabel.Text = math.floor(fakeHealth["Current"]) .. " / " .. fakeHealth["Max"]
	bgui.divider.UIGradient.Color = CS({CSK(0,Color3.new(1,1,1)),CSK(hp-.001,Color3.new(1,1,1)),CSK(hp+.001,Color3.new(0,0,0)),CSK(1,Color3.new(0,0,0)),})
	bgui.divider.TextLabel.UIGradient.Color = CS({CSK(0,Color3.new(0,0,0)),CSK(hp-.001,Color3.new(0,0,0)),CSK(hp+.001,Color3.new(1,1,1)),CSK(1,Color3.new(1,1,1)),})
	if Mode["Name"] ~= "FINARE" and bgui:FindFirstChild("UNLEASHED") then
		bgui:FindFirstChild("UNLEASHED").Visible = true
	elseif bgui:FindFirstChild("UNLEASHED") then
		bgui:FindFirstChild("UNLEASHED").Visible = false
	end
	step = step + 1
end)
