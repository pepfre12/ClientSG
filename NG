		Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
		LockedToPart=true,
		RotSpeed=NR(720)
	},Vector3.new(0,-1,0))
	particle(char["Right Arm"],8,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.3),
		Size=NS({NSK(0,0,0),NSK(1,7.5,5)}),
		Transparency = NS({NSK(0,0.25,0),NSK(1,1,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,-1,0))
	particle(root.CFrame*CFrame.new(0,0,-20),2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(0.3),
		Size=NS({NSK(0,30,0),NSK(1,15,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.75,0.25,0),NSK(1,1,0)})
	},Vector3.new(0,-1,0))
	particle(root.CFrame*CFrame.new(0,0,-20),100,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.4),
		Size=NS({NSK(0,100,40),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,-1,0))
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*10/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=ler1},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.4,1.1,0.1)*ang(math.rad(174),math.rad(25),math.rad(4.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.2,0,-0.2)*ang(math.rad(11),math.rad(-24.2),math.rad(9.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	swait(stats["ASPD"]*Alpha*60)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Elastic','Out',0,false,0
	RTweenAnimate(RootJoint,{C0=ler2},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.6,0.8,-0.3)*ang(math.rad(87.6),math.rad(15.5),math.rad(28.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0,0.2)*ang(math.rad(-5.5),math.rad(-24.5),math.rad(2.7))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	local death = makeHitbox({CFrame = root.CFrame*CFrame.new(0,0,-27.5),Size=Vector3.new(50,50,50),Shape="Ball"},75,false,false,8,anchorer)
	sound(root,"2319521125",.65,.5)
	swait(stats["ASPD"]*5)
	camShake(1,3)
	death.Color = Color3.new(1,0,0)
	sound(root,"1467330245")
	sound(root,"231977665")
	sound(root,"2689242256",1.05,1.25)
	particle(death.CFrame,2,{ZOffset=1,
		Texture="rbxassetid://2545920866",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,1.5),
		Size=NS({NSK(0,65,0),NSK(1,125,0)}),
		Transparency = NS({NSK(0,0.5,0),NSK(1,1,0)})
	},Vector3.new(0,-1,0))
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(35,35,35),
		Color=Mode["Colors"][1],
		Transparency = 1,
		Material="ForceField",
		CFrame = death.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Transparency = -2
	},{ -- decal end properties
	},10/60,"Sine","Out",0,true,0)
	local e2 = weirdeff("SpikeBall",{ -- part start properties
		Size=Vector3.new(40,40,40),
		Color=Mode["Colors"][2],
		Transparency = -2,
		Material="Neon",
		CFrame = death.CFrame*randr()
	},{ -- decal start properties
		Color3=glowify(Mode["Colors"][1]),
		Transparency = -1,
	},{ -- part end properties
		Size=Vector3.new(75,75,75),
		Transparency = 1
	},{ -- decal end properties
		Transparency = 1
	},10/60,"Sine","Out",0,false,0)
	local e2 = weirdeff("sphere",{ -- part start properties
		Size=Vector3.new(35,35,35),
		Color=Color3.new(1,1,1),
		Transparency = -1,
		Material="Neon",
		CFrame = death.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Transparency = 1,
		Color=Color3.new(-1,-1,-1),
		Size=Vector3.new(75,75,75),
	},{ -- decal end properties
	},3/60,"Linear","Out",1,false,0)
	for i = 1,6 do
		local cf = death.CFrame*randr()
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=Mode["Colors"][1],
			Transparency = .05,
			Material="Neon",
			CFrame = cf
		},{ -- mesh start properties
			VertexColor = glowify(Mode["Colors"][math.random(1,2)],Vector3.new),
			Scale = Vector3.new(.05,0,.05)
		},{ -- part end properties
			Transparency = 1,
			CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
		},{ -- mesh end properties
			Scale = Vector3.new(.65,0,.65)*(math.random(25,100)/100)
		},math.random(40,160)/60,"Sine","Out")
	end
	for i = 1,5 do
		local m = rand(5,10)
		slashy(death.CFrame*rang(rsign()*90,0,0)*randr(20,20,360),Mode["Colors"][math.random(1,2)],{30,35,40,100},4,3,{m,m*1.5,m*3})
	end
	death.Color = Color3.new(0,0,1)
	tweenDisable = {}
	attack = false
end

function click2()
	attack = true
	tweenDisable = {Torso.Neck,LW,RootJoint}
	particle(char["Left Arm"],2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(0.2),
		Size=NS({NSK(0,0,0),NSK(1,7.5,0)}),
		Transparency = NS({NSK(0,0,0),NSK(.75,0.5,0),NSK(1,1,0)}),
		LockedToPart=true
	},Vector3.new(0,-1,0))
	particle(char["Left Arm"],3,{ZOffset=1,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(0.4),
		Size=NS({NSK(0,0,0),NSK(1,5,0)}),
		Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
		LockedToPart=true,
		RotSpeed=NR(720)
	},Vector3.new(0,-1,0))
	particle(char["Left Arm"],8,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.3),
		Size=NS({NSK(0,0,0),NSK(1,7.5,5)}),
		Transparency = NS({NSK(0,0.25,0),NSK(1,1,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,-1,0))
	sound(root,"5113230537",1.5)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(-0.1,Mode["Flying"],0)*ang(math.rad(-6.6),math.rad(29.4),math.rad(7.3))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.5,0.4,0.5)*ang(math.rad(62.4),math.rad(67.4),math.rad(-25.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.3,0.1,0.1)*ang(math.rad(-16.2),math.rad(-37.2),math.rad(-16.1))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	swait(stats["ASPD"]*Alpha*60)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Elastic','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(-0.1,Mode["Flying"],-0.2)*ang(math.rad(-12.9),math.rad(-58.8),math.rad(-16.7))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.6,0.9,-0.5)*ang(math.rad(-172.1),math.rad(29.9),math.rad(-16.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,-0.3)*ang(math.rad(17.2),math.rad(59),math.rad(-0.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	sound(root.CFrame,"233091161")
	local bugs = root.CFrame
	delay(10/60,function()
		camShake(1.5,10)
	end)
	for i = 1,10 do
		local death = makeHitbox({CFrame = bugs*CFrame.new(0,-3,-2.5-7.5*i),Size=Vector3.new(22.5,22.5,22.5),Shape="Ball"},125,false,false,8,anchorer)
		local s = sound(death.CFrame,"4836146175",1.5,.75)
		particle(death.CFrame,2,{ZOffset=1,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(0.4),
			Size=NS({NSK(0,5,0),NSK(1,25,0)}),
			Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
			LockedToPart=true,
			RotSpeed=NR(-720)
		},Vector3.new(0,-1,0))
		particle(death.CFrame,30,{ZOffset=1,
			Texture="rbxassetid://3028571188",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.6),
			Size=NS({NSK(0,25,0),NSK(1,0,0)}),
			Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
			LockedToPart=true,
			Rotation=NR(1,360)
		},Vector3.new(0,-1,0))
		delay(10/60,function()
			death.Color = Color3.new(1,0,0)
			s:Stop()
			sound(death.CFrame,"199145534")
			sound(death.CFrame,"192410077")
			local e2 = weirdeff("sphere",{ -- part start properties
				Size=Vector3.new(15,10,15),
				Color=Mode["Colors"][1],
				Transparency = -1,
				Material="Neon",
				CFrame = death.CFrame
			},{ -- decal start properties
			},{ -- part end properties
				Transparency = 1,
				Size=Vector3.new(12.5,35,12.5),
			},{ -- decal end properties
			},30/60,"Sine","Out",0,false,0)
			local e2 = weirdeff("sphere",{ -- part start properties
				Size=Vector3.new(12.5,10,12.5),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = death.CFrame
			},{ -- decal start properties
			},{ -- part end properties
				Transparency = 1,
				Size=Vector3.new(15,70,15),
			},{ -- decal end properties
			},30/60,"Sine","Out",0,false,0)
			for pbl = 400,800,400 do
				weirdeff("VBLASTMESH2",{
					CFrame = death.CFrame*randr(1,360,1),
					Transparency=.5
				},{
					Scale = Vector3.new(.05,.05,.05),
					Offset = Vector3.new(0,0.7,0),
					VertexColor = glowify(Mode["Colors"][math.floor(pbl/400)],Vector3.new)
				},{
					CFrame = death.CFrame*randr(1,360,1),
					Transparency = 1
				},{
					Offset = Vector3.new(0,1.2 + pbl/40,0),
					Scale=Vector3.new(.15 + pbl/800,.1 + pbl/500,.15 + pbl/800)
				},math.random(40,80)/60,"Quad","In")
			end
			for i = 1,3 do
				local cf = death.CFrame*randr()
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][1],
					Transparency = .05,
					Material="Neon",
					CFrame = cf
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][math.random(1,2)],Vector3.new),
					Scale = Vector3.new(.05,0,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
				},{ -- mesh end properties
					Scale = Vector3.new(.5,0,.5)*(math.random(25,100)/100)
				},math.random(40,160)/60,"Sine","Out")
			end
			for i = 1,3 do
				local m = rand(15,30)
				slashy(death.CFrame*rang(rsign()*90,0,0)*randr(10,10,360),Mode["Colors"][math.random(1,2)],{10,20,50},4,2,{m,m*3})
			end
			particle(death.CFrame,2,{ZOffset=1,
				Texture="rbxassetid://2545920866",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(.2,1.5),
				Size=NS({NSK(0,50,0),NSK(1,100,0)}),
				Transparency = NS({NSK(0,0.5,0),NSK(1,1,0)})
			},Vector3.new(0,-1,0))
			swait(10)
			death.Color = Color3.new(0,0,1)
		end)
		swait()
	end
	swait(stats["ASPD"]*20)
	tweenDisable = {}
	attack = false
end

function click3()
	attack,idlesoff = true,true
	root.Anchored = true
	sound(root,"169445602")
	particle(root,2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.5),
		Size=NS({NSK(0,50,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	},Vector3.new(0,-1,0))
	particle(root,60,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.5),
		Size=NS({NSK(0,75,40),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,-1,0))
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Sine','InOut',0,false,0
	RTweenAnimate(RootJoint,{C0=cf(0,0,-0.1)*ang(math.rad(-15),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.6,-0.9)*ang(math.rad(-44.9),math.rad(4.3),math.rad(-2.5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1.1,0)*ang(math.rad(-34.9),math.rad(-4.3),math.rad(2.5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.6,0.4,0.3)*ang(math.rad(48),math.rad(7),math.rad(-47.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.6,0.4,0.3)*ang(math.rad(48),math.rad(-7),math.rad(47.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,-0.5)*ang(math.rad(35),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(stats["ASPD"]*30)
	sound(root,"5113657874",1.25,.75)
	sound(root,"192410077")
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*10/60,'Elastic','Out',0,false,0
	local twweeween = RTweenAnimate(root,{CFrame=root.CFrame*CFrame.new(0,0,-150)},stats["ASPD"]*30/60,"Linear","Out")
	RTweenAnimate(RootJoint,{C0=cf(0,-0.2,-0.4)*ang(math.rad(-55),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.2,-0.8)*ang(math.rad(-25),math.rad(5),math.rad(0))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1.2,0.1)*ang(math.rad(-25),math.rad(-5),math.rad(0))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0.3,0.6)*ang(math.rad(-40),math.rad(25),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0.3,0.6)*ang(math.rad(-40),math.rad(-20),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,-0.7)*ang(math.rad(40),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local hb1 = makeHitbox({Size=Vector3.new(16,16,24),Anchored=false},75,false,true,8)
	local welb = CreateWeld(root,root,hb1,CFrame.new(0,0,0))
	camShake(.5,15)
	for i = 1,stats["ASPD"]*30 do
		local cff = root.CFrame:lerp(root.CFrame*CFrame.new(0,0,-150),i/(stats["ASPD"]*30))
		weirdeff("Blast",{ -- part start properties
			Size=Vector3.new(5,30,5),
			Color=Mode["Colors"][1],
			Transparency = 0,
			Material="Neon",
			CFrame = cff*rang(90,0,0)*randr(1,360,1)
		},{ -- decal start properties
			Transparency = 1
		},{ -- part end properties
			Transparency = 1,
			Size=Vector3.new(30,30,30),
			CFrame = cff*rang(90,0,0)*randr(1,360,1)
		},{ -- decal end properties
		},45/60,"Sine","Out",0,false,0)
		for pbl = 25,50,25 do
			weirdeff("VBLASTMESH2",{
				CFrame = cff*rang(90,0,0)*randr(1,360,1),
				Transparency=.75
			},{
				Scale = Vector3.new(.05,.05,.05),
				Offset = Vector3.new(0,0.7,0),
				VertexColor = glowify(Mode["Colors"][math.floor(pbl/25)],Vector3.new)
			},{
				CFrame = cff*rang(90,0,0)*randr(1,360,1),
				Transparency = 1
			},{
				Offset = Vector3.new(0,21.2 + pbl/40,0),
				Scale=Vector3.new(.15 + pbl/800,2 + pbl/500,.15 + pbl/800)
			},math.random(40,80)/60,"Quad","In")
		end
		swait()
	end
	hb1.Color = Color3.new(0,0,1)
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	root.Anchored = false
	attack,idlesoff = false,false
end

function click4()
	attack,idlesoff = true,true
	walkspeed = 4
	jumppower = 0
	tweenDisable = {RH,LH}
	local charger = particle(root,0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2),
		Size=NS({NSK(0,0,0),NSK(1,5,2.5)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rate=300
	},Vector3.new(0,17.5,0))
	particle(root,120,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.8),
		Size=NS({NSK(0,125,50),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,17.5,0))
	particle(root,2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(1),
		Size=NS({NSK(0,150,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	},Vector3.new(0,17.5,0))
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,Mode["Flying"],0)*ang(math.rad(5),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-1,0)*ang(math.rad(0),math.rad(0),math.rad(0))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1,0)*ang(math.rad(0),math.rad(0),math.rad(0))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.4,0.7,0.4)*ang(math.rad(-151.3),math.rad(-26.9),math.rad(28.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,0.7,0.5)*ang(math.rad(-153.9),math.rad(22.5),math.rad(-27.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.5)*ang(math.rad(30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	sound(root,"5113647461",.95,2)
	sound(root,"203691282",1,.75)
	for i = 1,stats["ASPD"]*20 do
		charger.Size = NS({NSK(0,0,0),NSK(1,5 + i*1.5,2.5 + i*.75)})
		swait()
	end
	local hb1 = makeHitbox({CFrame=root.CFrame*cf(0,10,0)*rang(0,0,90),Size=Vector3.new(20,175,175),Shape="Cylinder"},250,false,false,8)
	tweenDisable = {}
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Quad','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-1.4,-0.2)*ang(math.rad(-55),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.4,-1.1)*ang(math.rad(-35),math.rad(0),math.rad(0))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1.1,0)*ang(math.rad(-15),math.rad(0),math.rad(0))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.4,0.6,-0.8)*ang(math.rad(98.7),math.rad(-26.9),math.rad(28.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.4,0.6,-0.8)*ang(math.rad(96.1),math.rad(22.5),math.rad(-27.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.4)*ang(math.rad(-20),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(stats["ASPD"]*10)
	charger:Destroy()
	weirdeff("sphere",{ -- part start properties
		Size=Vector3.new(10,10,10),
		Color=Mode["Colors"][2],
		Transparency = 0,
		Material="ForceField",
		CFrame = root.CFrame*cf(0,-3,0)
	},{ -- decal start properties
	},{ -- part end properties
		Transparency = 1,
		Size=Vector3.new(300,300,300),
		CFrame = root.CFrame*cf(0,9.5,0)*rang(0,179.9,0)
	},{ -- decal end properties
	},45/60,"Quad","Out",0,false,0)
	weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(10,10,10),
		Color=Mode["Colors"][1],
		Transparency = 0.5,
		Material="Neon",
		CFrame = root.CFrame*cf(0,-3,0)
	},{ -- decal start properties
	},{ -- part end properties
		Transparency = 1,
		Size=Vector3.new(300,300,300),
		CFrame = root.CFrame*cf(0,9.5,0)*rang(0,179.9,0)
	},{ -- decal end properties
	},15/60,"Quad","Out",0,false,0)
	weirdeff("Blast",{ -- part start properties
		Size=Vector3.new(50,50,50),
		Color=Mode["Colors"][1],
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame*cf(0,22,0)
	},{ -- decal start properties
		Transparency = 1
	},{ -- part end properties
		Transparency = 1,
		Size=Vector3.new(250,25,250),
		CFrame = root.CFrame*cf(0,9.5,0)*rang(0,179.9,0)
	},{ -- decal end properties
	},60/60,"Sine","Out",0,false,0)
	weirdeff("Plant",{ -- part start properties
		Size=Vector3.new(130,0,130),
		Color=Mode["Colors"][1],
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame*cf(0,-2.97,0)
	},{ -- decal start properties
		Transparency = 0,
		Color3=glowify(Mode["Colors"][2]),
	},{ -- part end properties
		Transparency = 1,
		Size=Vector3.new(250,0,250),
		CFrame = root.CFrame*cf(0,-2.97,0)*rang(0,179.9,0),
		Color=Mode["Colors"][2],
	},{ -- decal end properties
		Transparency = 1,
		Color3=glowify(Mode["Colors"][1]),
	},30/60,"Sine","In",0,false,0)
	for i = 1,10 do
		local m = rand(10,30)
		slashy(root.CFrame*cf(0,-3,0)*rang(rsign()*90,0,0)*randr(10,10,360),Mode["Colors"][math.random(1,2)],{10,50,90 + m,120 + m*4},4,3,{m,m*2,m*3})
	end
	for pbl = 1200,2400,200 do
		weirdeff("VBLASTMESH2",{
			CFrame = root.CFrame*cf(0,-3,0)*randr(1,360,1),
			Transparency=.5
		},{
			Scale = Vector3.new(.05,.05,.05),
			Offset = Vector3.new(0,0.7,0),
			VertexColor = glowify(Mode["Colors"][1],Vector3.new)
		},{
			CFrame = root.CFrame*cf(0,-3,0)*randr(1,360,1),
			Transparency = 1
		},{
			Offset = Vector3.new(0,10.2 + pbl/40,0),
			Scale=Vector3.new(.15 + pbl/800,1 + pbl/500,.15 + pbl/800)
		},math.random(40,80)/60,"Quad","In")
	end
	for i = 1,10 do
		local cf = root.CFrame*randr()
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=Mode["Colors"][1],
			Transparency = .05,
			Material="Neon",
			CFrame = cf
		},{ -- mesh start properties
			VertexColor = glowify(Mode["Colors"][1],Vector3.new),
			Scale = Vector3.new(.25,0,.25)
		},{ -- part end properties
			Transparency = 1,
			CFrame = cf*randr(1,360,1)
		},{ -- mesh end properties
			Scale = Vector3.new(.75,0,.75)*(math.random(75,125)/100)
		},math.random(40,160)/60,"Sine","Out")
	end
	particle(root.CFrame*cf(0,-3,0),5,{ZOffset=1,
		Texture="rbxassetid://2545920866",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(1.5,2.5),
		Size=NS({NSK(0,100,0),NSK(1,200,0)}),
		Transparency = NS({NSK(0,0.25,0),NSK(1,1,0)})
	})
	particle(root,5,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.5),
		Size=NS({NSK(0,0,0),NSK(1,200,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.75,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	})
	particle(root,5,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.35),
		Size=NS({NSK(0,0,0),NSK(1,200,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.75,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	})
	camShake(10,15)
	sound(root,"192410070",1.2,2)
	sound(root,"192410077",.6,2.25)
	sound(root,"241816029",.9,1.75)
	hb1.Color = Color3.new(1,0,0)
	swait(stats["ASPD"]*20)
	hb1.Color = Color3.new(0,0,1)
	swait(stats["ASPD"]*40)
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	attack,idlesoff = false,false
end

function airclick1(jumpy)
	attack = true
	walkspeed = 0
	jumppower = 0
	local charger = particle(char["Right Arm"],0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2),
		Size=NS({NSK(0,0,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rate=300
	},Vector3.new(0,-1,0))
	particle(char["Right Arm"],2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.5),
		Size=NS({NSK(0,25,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	},Vector3.new(0,-1,0))
	sound(root,"5113630277",1.1,2)
	tweenDisable = {Torso.Neck,RW,RootJoint}
	local ler1 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(5),math.rad(25),math.rad(0))*RootCF,.5)
	local ler2 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(-5),math.rad(25),math.rad(0))*RootCF,.5)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=ler1},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.4,1.1,0.1)*ang(math.rad(174),math.rad(25),math.rad(4.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.2,0,-0.2)*ang(math.rad(11),math.rad(-24.2),math.rad(9.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	for i = 1,stats["ASPD"]*Alpha*60 do
		charger.Size = NS({NSK(0,0,0),NSK(1,1 + i/10,0)})
		swait()
	end
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Elastic','Out',0,false,0
	RTweenAnimate(RootJoint,{C0=ler2},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.6,0.8,-0.3)*ang(math.rad(87.6),math.rad(15.5),math.rad(28.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0,0.2)*ang(math.rad(-5.5),math.rad(-24.5),math.rad(2.7))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	charger.Size = NS({NSK(0,0,0),NSK(1,10,5)})
	swait(2)
	charger.Size = NS({NSK(0,0,0),NSK(1,7.5,2.5)})
	local limit = 20
	if jumpy then limit = 8 sound(root,"2943064526",1.25) else sound(root,"5113620110",1.25,2.5) end
	for i = 1,limit do
		local del = (2*i)/60
		if jumpy then del = 0 end
		delay(del,function()
			local e = PART()
			e.CFrame = root.CFrame*CFrame.new(-1.5,1,-2)
			local dir = (CFrame.new(e.CFrame.p,mouse.Hit.p)*rang(rand(-50,50)/20,rand(-50,50)/20,rand(-50,50)/20)).lookVector
			local hit,pos = rayCast(e.CFrame.p,dir,5000,char)
			local tr = TRAIL(e,3)
			tr.Color = CS(Mode["Colors"][1])
			tr.LightEmission = 0.5
			local tr2 = TRAIL(e,2)
			tr.Color = CS(Mode["Colors"][2])
			particle(e,0,{ZOffset=2,
				Texture="rbxassetid://2273224484",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(1),
				Size=NS({NSK(0,0,0),NSK(.25,2,0),NSK(1,3,1)}),
				Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
				LockedToPart=true,
				Rate=30
			})
			e.Parent = char
			local tw = RTweenAnimate(e,{Position=pos},stats["ASPD"]*(.1 + (e.Position-pos).magnitude/750),"Quad","In")
			local death = makeHitbox({Position = pos,Size=Vector3.new(17.5,17.5,17.5),Shape="Ball"},25,false,false,6)
			swait((stats["ASPD"]*(.1 + (e.Position-pos).magnitude/750))*60)
			e.Position = pos
			death.Color = Color3.new(1,0,0)
			particle(death.CFrame,3,{ZOffset=1,
				Texture="rbxassetid://2545920866",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(.2,1.25),
				Size=NS({NSK(0,50,0),NSK(1,75,0)}),
				Transparency = NS({NSK(0,0.5,0),NSK(1,1,0)})
			},Vector3.new(0,-1,0))
			local e2 = weirdeff("InvertedSphere",{ -- part start properties
				Size=Vector3.new(15,15,15),
				Color=Mode["Colors"][1],
				Transparency = 1,
				Material="ForceField",
				CFrame = death.CFrame
			},{ -- decal start properties
			},{ -- part end properties
				Size=Vector3.new(20,20,20),
				Transparency = -2
			},{ -- decal end properties
			},20/60,"Sine","Out",0,true,0)
			local e2 = weirdeff("SpikeBall",{ -- part start properties
				Size=Vector3.new(20,20,20),
				Color=Mode["Colors"][2],
				Transparency = -2,
				Material="Neon",
				CFrame = death.CFrame*randr()
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(40,40,40),
				Transparency = 1
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out",0,false,0)
			sound(e,"199145534",1.3)
			sound(e,"4836457184",.9,.75)
			swait(10)
			death.Color = Color3.new(0,0,1)
			swait(60)
			e:Destroy()
		end)
	end
	swait(20)
	charger.Enabled = false
	swait(Alpha*60 - 5)
	charger:Destroy()
	tweenDisable = {}
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	attack = false
end

function airclick2()
	attack = true
	walkspeed = 0
	jumppower = 0
	local BALLOFDEATH = PART(char)
	BALLOFDEATH.Shape = "Ball"
	BALLOFDEATH.Transparency = 0
	BALLOFDEATH.Anchored = false
	local wel = CreateWeld(Torso,Torso,BALLOFDEATH,CFrame.new(0,6,0))
	local charger = particle(BALLOFDEATH,0,{ZOffset=2,
		Texture="rbxassetid://2545920866",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.5),
		Size=NS({NSK(0,0,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360),
		Rate=30
	})
	particle(BALLOFDEATH,120,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.8),
		Size=NS({NSK(0,150,50),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	})
	particle(BALLOFDEATH,0,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.4,1.2),
		Size=NS({NSK(0,0,0),NSK(1,175,0)}),
		Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
		LockedToPart=true,
		Rotation=NR(1,360),
		Rate=120
	})
	particle(BALLOFDEATH,0,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.8),
		Size=NS({NSK(0,0,0),NSK(1,200,0)}),
		Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
		LockedToPart=true,
		Rotation=NR(1,360),
		Rate=120
	})
	particle(BALLOFDEATH,2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(1.25),
		Size=NS({NSK(0,200,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	})
	sound(BALLOFDEATH,"206083364",1,1.25)
	sound(BALLOFDEATH,"4836146175",1.4,2.5)
	local weee = sound(BALLOFDEATH,"4836146175",0.9,2.5)
	tweenDisable = {Torso.Neck,RW,LW,RootJoint}
	local ler1 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(5),math.rad(25),math.rad(0))*RootCF,.5)
	local ler2 = RootJoint.C0:lerp(cf(0,0,0.1)*ang(math.rad(-5),math.rad(25),math.rad(0))*RootCF,.5)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0.1)*ang(math.rad(10),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.3,1.1,0.3)*ang(math.rad(-179.1),math.rad(-23.4),math.rad(-21.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.3,1.1,0.3)*ang(math.rad(-179.1),math.rad(23.4),math.rad(21.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.3)*ang(math.rad(-20),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	local tweeeeen = RTweenAnimate(BALLOFDEATH,{Size=Vector3.new(100,100,100)},stats["ASPD"]*45/60,"Quad","Out")
	RTweenAnimate(BALLOFDEATH,{Color=Mode["Colors"][2]},15/60,"Sine","InOut",-1,true)
	RTweenAnimate(wel,{C0=CFrame.new(0,53,0)},stats["ASPD"]*45/60,"Quad","Out")
	swait(stats["ASPD"]*45)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Elastic','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.1,0.1)*ang(math.rad(-25),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(LW,{C0=cf(-1.2,0.8,-0.9)*ang(math.rad(85.9),math.rad(-23.4),math.rad(18.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(RW,{C0=cf(1.2,0.8,-0.9)*ang(math.rad(85.9),math.rad(23.4),math.rad(-18.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*ang(math.rad(5),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay,true)
	sound(BALLOFDEATH,"4575206546",0.5,5)
	swait(stats["ASPD"]*5)
	weee:Destroy()
	wel:Destroy()
	BALLOFDEATH.Anchored = true
	local dir = (CFrame.new(BALLOFDEATH.CFrame.p,mouse.Hit.p)*rang(rand(-50,50)/20,rand(-50,50)/20,rand(-50,50)/20)).lookVector
	local hit,pos = rayCast(BALLOFDEATH.CFrame.p,dir,5000,char)
	local tw = RTweenAnimate(BALLOFDEATH,{Position=pos},stats["ASPD"]*(.3 + (BALLOFDEATH.Position-pos).magnitude/250),"Sine","In")
	local death = makeHitbox({Position = pos,Size=Vector3.new(250,250,250),Shape="Ball"},500,false,false,16)
	coroutine.resume(coroutine.create(function()
		swait((stats["ASPD"]*(.3 + (BALLOFDEATH.Position-pos).magnitude/250))*60)
		BALLOFDEATH.CFrame = death.CFrame
		death.Color = Color3.new(1,0,0)
		sound(BALLOFDEATH,"1354014962",0.8,4.75)
		sound(BALLOFDEATH,"241816029",1.2,5)
		sound(BALLOFDEATH,"4701310931",0.8,5.25)
		RTweenAnimate(BALLOFDEATH,{Size=Vector3.new(500,500,500),Transparency=1},1,"Quad","Out")
		for i,v in pairs(BALLOFDEATH:GetDescendants()) do
			if v.ClassName == "ParticleEmitter" then
				v.Enabled = false
			end
		end
		local e2 = weirdeff("SpikeBall",{ -- part start properties
			Size=Vector3.new(200,200,200),
			Color=Mode["Colors"][2],
			Transparency = -2,
			Material="Neon",
			CFrame = BALLOFDEATH.CFrame*randr()
		},{ -- decal start properties
			Color3=glowify(Mode["Colors"][1]),
			Transparency = -1,
		},{ -- part end properties
			Size=Vector3.new(1000,1000,1000),
			Transparency = 1,
			CFrame = BALLOFDEATH.CFrame*randr()
		},{ -- decal end properties
			Transparency = 1
		},60/60,"Sine","In",0,false,0)
		local charger = particle(BALLOFDEATH,5,{ZOffset=2,
			Texture="rbxassetid://2545920866",
			LightEmission=1,
			Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(3),
			Size=NS({NSK(0,100,0),NSK(1,200,0)}),
			Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
			LockedToPart=true
		})
		local e2 = weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(300,300,300),
			Color=Mode["Colors"][1],
			Transparency = 0,
			Material="ForceField",
			CFrame = BALLOFDEATH.CFrame
		},{ -- decal start properties
		},{ -- part end properties
			Size=Vector3.new(2048,2048,2048),
			Transparency = 1
		},{ -- decal end properties
		},90/60,"Sine","Out")
		local e2 = weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(300,300,300),
			Color=Mode["Colors"][2],
			Transparency = 0,
			Material="ForceField",
			CFrame = BALLOFDEATH.CFrame
		},{ -- decal start properties
		},{ -- part end properties
			Size=Vector3.new(2048,2048,2048),
			Transparency = 1
		},{ -- decal end properties
		},60/60,"Sine","Out")
		particle(BALLOFDEATH,200,{
			Texture="rbxassetid://243728104",
			LightEmission=.9,
			Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(2,4),
			Size=NS({NSK(0,200,50),NSK(1,150,0)}),
			Transparency=NS({NSK(0,0,0),NSK(0.2,0.5,0.2),NSK(1,1,0)}),
			Speed=NR(50,1500),
			Drag=3,
			SpreadAngle=Vector2.new(360,360),
			RotSpeed=NR(-100,100),
			Rotation=NR(1,360),
			EmissionDirection="Front"
		})
		for i = 1,8 do
			local cf = BALLOFDEATH.CFrame*randr()
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10),
				Color=Mode["Colors"][1],
				Transparency = .05,
				Material="Neon",
				CFrame = cf
			},{ -- mesh start properties
				VertexColor = glowify(Mode["Colors"][2],Vector3.new),
				Scale = Vector3.new(.3,0,.3)
			},{ -- part end properties
				Transparency = 1,
				CFrame = cf*randr(1,360,1)
			},{ -- mesh end properties
				Scale = Vector3.new(15,0,15)*(math.random(25,125)/100)
			},math.random(40,160)/20,"Quint","Out")
		end
		swait(30)
		death.Color = Color3.new(0,0,1)
		swait(180)
		BALLOFDEATH:Destroy()
	end))
	swait(stats["ASPD"]*Alpha*60)
	tweenDisable = {}
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	attack = false
end

function RESURRECTION()
	attack,idlesoff = true,true
	local bgi = bgui.Enabled
	local musc = music.IsPlaying
	bgui.Enabled = false
	music:Stop()
	cantdie = true
	handle:ClearAllChildren() -- kill wings
	for i,v in pairs(char:GetDescendants()) do
		if v:IsA("BasePart") then
			local h = Instance.new("NumberValue",v)
			h.Value = v.Transparency
			h.Name = "restransp"
			v.Transparency = 1
		end
	end
	sound(root,"2613355772",1.1,.5).EmitterSize = 200
	for i = 1,8 do
		local cf = root.CFrame*randr()
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=Mode["Colors"][1],
			Transparency = .05,
			Material="Neon",
			CFrame = cf
		},{ -- mesh start properties
			VertexColor = glowify(Mode["Colors"][2],Vector3.new),
			Scale = Vector3.new(.3,0,.3)
		},{ -- part end properties
			Transparency = 1,
			CFrame = cf*randr(1,360,1)
		},{ -- mesh end properties
			Scale = Vector3.new(3,0,3)*(math.random(50,125)/100)
		},math.random(40,160)/10,"Quint","Out")
	end
	local e2 = weirdeff("SpikeBall",{ -- part start properties
		Size=Vector3.new(10,10,10),
		Color=Mode["Colors"][2],
		Transparency = -2,
		Material="Neon",
		CFrame = root.CFrame*randr()
	},{ -- decal start properties
		Transparency = 1
	},{ -- part end properties
		Size=Vector3.new(200,200,200),
		Transparency = 1,
		CFrame = root.CFrame*randr()
	},{ -- decal end properties
	},30/60,"Quad","Out",0,false,0)
	particle(root,5,{ZOffset=2,
		Texture="rbxassetid://2545920866",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1),rgb(255, 130, 143)),
		Lifetime=NR(1.5),
		Size=NS({NSK(0,100,0),NSK(1,5,0)}),
		Transparency = NS({NSK(0,0,0),NSK(1,0,0)}),
		LockedToPart=true
	})
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=Mode["Colors"][1],
		Transparency = 0,
		Material="ForceField",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(500,500,500),
		Transparency = 1
	},{ -- decal end properties
	},15/60,"Sine","Out",3)
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=Mode["Colors"][2],
		Transparency = 0,
		Material="ForceField",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(500,500,500),
		Transparency = 1
	},{ -- decal end properties
	},20/60,"Sine","Out",3)
	local STILLALIVE = particle(root,0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(rgb(223, 57, 80):lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2),
		Size=NS({NSK(0,0,0),NSK(1,5,1.5)}),
		Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
		LockedToPart=true,
		Rate=30
	})
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 2/60,'Linear','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,1.9,-0.4)*ang(math.rad(-45),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.9,-1,-0.4)*ang(math.rad(20.4),math.rad(5.4),math.rad(-4.6))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-1,-0.4)*ang(math.rad(20.4),math.rad(-5.4),math.rad(4.6))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0.3,-0.2)*ang(math.rad(50),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0.3,-0.2)*ang(math.rad(50),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.3,0.7)*ang(math.rad(-50),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	if bgui:FindFirstChild("UNLEASHED") then
		local cc = Instance.new("ColorCorrectionEffect",game.Lighting)
		cc.Brightness = .25
		cc.Saturation = -5
		game:GetService("Debris"):AddItem(cc,5)
		RTweenAnimate(cc,{Brightness=0,Saturation=-.5},1,"Sine","Out")
		swait(90)
		cc.Brightness = .5
		RTweenAnimate(cc,{Saturation=0},1,"Elastic")
		RTweenAnimate(cc,{Brightness=0},3,"Elastic","Out")
		sound(root.CFrame,"2613355772",.5,1.5).EmitterSize = 200
		sound(root.CFrame,"3404036141",.8,1.25).EmitterSize = 200
		sound(root.CFrame,"1354014962",1,1.25).EmitterSize = 200
		weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=Mode["Colors"][1],
			Transparency = 0.5,
			Material="Neon",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(1500,1500,1500),
			Transparency = 1
		},{ -- decal end properties
		},6,"Sine","Out")
		weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=Mode["Colors"][3],
			Transparency = 0.5,
			Material="Neon",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(2000,2000,2000),
			Transparency = 1
		},{ -- decal end properties
		},9,"Sine","Out")
		for i = 1,6 do
			local cf = root.CFrame*randr()
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10),
				Color=Mode["Colors"][1],
				Transparency = .05,
				Material="Neon",
				CFrame = cf
			},{ -- mesh start properties
				VertexColor = glowify(Mode["Colors"][2],Vector3.new),
				Scale = Vector3.new(.3,0,.3)
			},{ -- part end properties
				Transparency = 1,
				CFrame = cf*randr(1,360,1)
			},{ -- mesh end properties
				Scale = Vector3.new(5,0,5)*(math.random(50,125)/100)
			},math.random(40,160)/20,"Sine","Out")
		end
		for i = 1,8 do
			local m = rand(20,40)
			slashy(root.CFrame*rang(rsign()*90,0,0)*randr(40,40,360),Mode["Colors"][3],{400,600,1000,2048},100,3,{5,m*1.5,m*3})
		end
		particle(root,5,{ZOffset=2,
			Texture="rbxassetid://2545920866",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(2),
			Size=NS({NSK(0,200,0),NSK(1,200,0)}),
			Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
			LockedToPart=true
		})
		particle(root,300,{
			Texture="rbxassetid://243728104",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]),
			Lifetime=NR(.5,2),
			Size=NS({NSK(0,150,50),NSK(0.3,175,25),NSK(1,200,0)}),
			Transparency=NS({NSK(0,0,0),NSK(0.2,0.7,0.2),NSK(1,1,0)}),
			Speed=NR(100,2000),
			Drag=5,
			SpreadAngle=Vector2.new(360,360),
			RotSpeed=NR(-200,200),
			Rotation=NR(1,360)
		})
		char:BreakJoints()
	else
		swait(450)
		statStuff['Base']["WS"] = 0
		statStuff['Base']["JP"] = 0
		statThing()
		local glow = Instance.new("PointLight",root)
		glow.Color = rgb(255, 130, 143)
		glow.Range = 0
		glow.Brightness = 0
		RTweenAnimate(glow,{Range=60,Brightness=10},2,"Sine","Out")
		STILLALIVE.Size = NS({NSK(0,0,0),NSK(1,10,5)})
		for i,v in pairs(char:GetDescendants()) do
			if v:FindFirstChild("restransp") then
				pcall(function()
					RTweenAnimate(v,{Transparency=v:FindFirstChild("restransp").Value},1,"Quad","Out")
					coroutine.resume(coroutine.create(function()
						swait(60)
						v.Transparency = v:FindFirstChild("restransp").Value
					end))
					v:FindFirstChild("restransp"):Destroy()
				end)
			end
		end
		sound(root,"4701312550",0.8,2)
		sound(root,"4701310085",0.5,3)
		for i = 0,1,.025 do
			STILLALIVE.Transparency = NS({NSK(0,i,0),NSK(1,1,0)})
			swait()
		end
		local charging = true
		local charge = particle(root,0,{ZOffset=1,
			Texture="rbxassetid://3028571188",
			LightEmission=1,
			Color=CS(rgb(223, 57, 80)),
			Lifetime=NR(.2,.8),
			Size=NS({NSK(0,200,0),NSK(1,0,0)}),
			Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
			LockedToPart=true,
			Rotation=NR(1,360),
			Rate = 10
		})
		sound(root,"5112980602",1.5,5)
		sound(root,"233091161",1.2,4.5)
		sound(root,"3028505295",.4,6.5)
		sound(root,"1368583274",.3,5)
		sound(root,"289315275",.5,5.5)
		sound(root,"1368605755",.3,6)
		for i = 1,480 do
			local size = rand(75,225)
			if i/20 == math.ceil(i/20) and i < 360 then
				coroutine.resume(coroutine.create(function()
					local e = PART(char)
					local length = rand(50,250)
					local e2 = particle(e,0,{ZOffset=5,
						Texture="rbxassetid://2545920866",
						LightEmission=1,
						Color=CS(({rgb(255, 130, 143),rgb(223, 57, 80),rgb(255, 52, 25)})[rand(1,3)]),
						Lifetime=NR(length/50),
						Size=NS({NSK(0,10,0),NSK(1,5*(i/60),0)}),
						Transparency = NS({NSK(0,1,0),NSK(1,0.9,0)}),
						Speed=NR(50),
						Rate=0
					})
					local cfe = randr()
					while charging do
						cfe = cfe * rang(sin(step/520),cos(step/520),sin((120+step)/520))
						e.CFrame = root.CFrame * cfe * CFrame.new(0,-length,0)
						e2:Emit(2)
						swait()
					end
					swait(120)
					e:Destroy()
				end))
				charge.Rate = charge.Rate + 10
			end
			if i/30 == math.ceil(i/30) and i >= 360 then
				sound(root,"3028505295",1.25 + (math.ceil((i-360)/80))/5,4)
				local e2 = weirdeff("InvertedSphere",{ -- part start properties
					Size=Vector3.new(1000,1000,1000),
					Color=rgb(223, 57, 80),
					Transparency = 1,
					Material="Neon",
					CFrame = root.CFrame
					},{ -- decal start properties
					},{ -- part end properties
					Size=Vector3.new(0,0,0),
					Transparency = 0.75
					},{ -- decal end properties
				},60/60,"Sine","In")
				local e2 = weirdeff("SpikeBall",{ -- part start properties
					Size=Vector3.new(1500,1500,1500),
					Color=rgb(255, 80, 120),
					Transparency = 1,
					Material="Neon",
					CFrame = root.CFrame*randr()
				},{ -- decal start properties
					Transparency = 1
				},{ -- part end properties
					Size=Vector3.new(0,0,0),
					Transparency = .5,
					CFrame = root.CFrame*randr()
				},{ -- decal end properties
				},30/60,"Sine","In",0,false,0.5)
			end
			if i/20 == math.ceil(i/20) then
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][1],
					Transparency = 1,
					Material="Neon",
					CFrame = root.CFrame*randr()
				},{ -- mesh start properties
					VertexColor = glowify(rgb(255, 80, 120),Vector3.new),
					Scale = Vector3.new(3,0,3)*(math.random(50,125)/100)
				},{ -- part end properties
					Transparency = .05
				},{ -- mesh end properties
					Scale = Vector3.new(0,0,0)
				},math.random(40,160)/30,"Quint","Out")
			end
			swait()
		end
		local charger = particle(root,0,{ZOffset=2,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.15),
			Size=NS({NSK(0,0,0),NSK(1,5,2.5)}),
			Transparency = NS({NSK(0,0,0),NSK(.25,0.75,0),NSK(1,1,0)}),
			LockedToPart=true,
			Rate=120
		},Vector3.new(0,0,0))
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 45/60,'Sine','InOut',0,false,0
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,-0.1)*ang(math.rad(-10),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-0.9,0,-0.6)*ang(math.rad(-19.6),math.rad(5),math.rad(-8.7))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(0.9,-1,0.1)*ang(math.rad(-18.7),math.rad(-13.6),math.rad(3.8))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-0.8,0.6,-0.6)*ang(math.rad(168.8),math.rad(-27.2),math.rad(72.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(0.6,0.5,-0.5)*ang(math.rad(169.3),math.rad(6.3),math.rad(-65.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.6,0.9)*ang(math.rad(-60),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		for i = 1,15 do
			charger.Size = NS({NSK(0,0,0),NSK(1,i*6,i*3)})
			swait()
		end
		charger.Enabled = false
		swait(5)
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 45/60,'Elastic','Out',0,false,0
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.2,0.4)*ang(math.rad(50),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-1,-0.9,-1.2)*ang(math.rad(-64),math.rad(7.3),math.rad(-8.4))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1,-1.3,-0.1)*ang(math.rad(-39),math.rad(-8.9),math.rad(6.7))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1,0.2,0.8)*ang(math.rad(111.7),math.rad(4.7),math.rad(-110.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.1,0.2,0.7)*ang(math.rad(112.1),math.rad(-4.5),math.rad(115.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.3,-0.6)*ang(math.rad(40),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		sound(root,"3748208751",1.5,8)
		sound(root,"2319521125",.6,7.5)
		sound(root,"2836444230",.6,8.5)
		ModesP1["Q"]["M"] = "Finare"
		Modes = ModesP1
		bgui.namer.Position = UDim2.new(0,0,-.1,0)
		if bgui:FindFirstChild("notunleashed") then
			bgui.notunleashed.Name = "UNLEASHED"
		end
		local oldaura = aura
		aura = resources.UNLEASHED:Clone()
		aura.Parent = oldaura.Parent
		oldaura:Destroy()
		weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=Mode["Colors"][1],
			Transparency = 0.5,
			Material="Neon",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(1500,1500,1500),
			Transparency = 1
		},{ -- decal end properties
		},3,"Sine","Out")
		weirdeff("InvertedSphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=Mode["Colors"][3],
			Transparency = 0.5,
			Material="Neon",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(2000,2000,2000),
			Transparency = 1
		},{ -- decal end properties
		},4,"Sine","Out")
		for i = 1,6 do
			local cf = root.CFrame*randr()
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10),
				Color=Mode["Colors"][1],
				Transparency = .05,
				Material="Neon",
				CFrame = cf
			},{ -- mesh start properties
				VertexColor = glowify(Mode["Colors"][2],Vector3.new),
				Scale = Vector3.new(.3,0,.3)
			},{ -- part end properties
				Transparency = 1,
				CFrame = cf*randr(1,360,1)
			},{ -- mesh end properties
				Scale = Vector3.new(5,0,5)*(math.random(50,125)/100)
			},math.random(40,160)/20,"Sine","Out")
		end
		for i = 1,8 do
			local m = rand(20,40)
			slashy(root.CFrame*rang(rsign()*90,0,0)*randr(40,40,360),Mode["Colors"][3],{400,600,1000,2048},100,3,{5,m*1.5,m*3})
		end
		particle(root,5,{ZOffset=2,
			Texture="rbxassetid://2545920866",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(2),
			Size=NS({NSK(0,200,0),NSK(1,200,0)}),
			Transparency = NS({NSK(0,0,0),NSK(1,1,0)}),
			LockedToPart=true
		})
		particle(root,300,{
			Texture="rbxassetid://243728104",
			LightEmission=1,
			Color=CS(Mode["Colors"][1]),
			Lifetime=NR(.5,2),
			Size=NS({NSK(0,150,50),NSK(0.3,175,25),NSK(1,200,0)}),
			Transparency=NS({NSK(0,0,0),NSK(0.2,0.7,0.2),NSK(1,1,0)}),
			Speed=NR(100,2000),
			Drag=5,
			SpreadAngle=Vector2.new(360,360),
			RotSpeed=NR(-200,200),
			Rotation=NR(1,360)
		})
		charging = false
		glow:Destroy()
		charge:Destroy()
		charger:Destroy()
		STILLALIVE:Destroy()
		statStuff["UnleashMultiplier"] = {ATK=1.25,ADMG=1.25,ASPD=.75,DEF=1.5,DR=1,WS=1.5,JP=1.25,JUMPS=1,FLYSPEED=1.5}
		statThing()
		getMode(Finare,"Q")
		local hp = fakeHealth["Max"]*2.5
		fakeHealth = {Current=hp,Max=hp}
		if musc then music:Play() end
		if bgi then bgui.Enabled = true end
		swait(60)
		cantdie = false
		attack,idlesoff = false,false
	end
end

function virtuemoment()
	if not Mode["GenSelected"] then
		if not Mode["GenElement"] then
			Mode["GenElement"] = "Deluge"
		end
		local h = particle(handle,0,{ZOffset=2,
			Texture="rbxassetid://2273224484",
			LightEmission=1,
			Color=CS(rgb(223, 57, 80):lerp(Color3.new(1,1,1),.1)),
			Lifetime=NR(.8),
			Size=NS({NSK(0,15,0),NSK(0.25,5,0),NSK(1,0,0)}),
			Transparency = NS({NSK(0,0,0),NSK(1,0,0)}),
			LockedToPart=true,
			Rate=0,
			RotSpeed=NR(360/.35)
		})
		if Mode["GenElement"] == "Deluge" then
			sound(handle,"4839213917",1.3,1)
			Mode["GenElement"] = "Inferno"
			h.Color = CS(rgb(230,70,70))
			Recolor({rgb(180, 180, 180),rgb(230,0,0),rgb(230, 70, 70),rgb(160,0,0)})
			h:Emit(2)
		elseif Mode["GenElement"] == "Inferno" then
			sound(handle,"4839213917",1.7,1)
			Mode["GenElement"] = "Gale"
			h.Color = CS(rgb(70,230,70))
			Recolor({rgb(180, 180, 180),rgb(0, 230, 0),rgb(70, 230, 70),rgb(0,160,0)})
			h:Emit(2)
		elseif Mode["GenElement"] == "Gale" then
			sound(handle,"4839213917",.9,1)
			Mode["GenElement"] = "Deluge"
			h.Color = CS(rgb(70,70,230))
			Recolor({rgb(180, 180, 180),rgb(0,0,230),rgb(70, 70, 230),rgb(0,0,160)})
			h:Emit(2)
		end
	else
		sound(handle,"3226828768",1.1)
		textStuff("GENSONO",rgb(180, 180, 180),rgb(230, 160, 160),"SciFi")
		Recolor({rgb(180, 180, 180),rgb(160,230,160),rgb(230, 160, 160),rgb(160,160,230)})
		Mode["Colors"] = {rgb(180, 180, 180),rgb(160,230,160),rgb(230, 160, 160),rgb(160,160,230)}
		Mode["Taunt"] = {{"I'm a little more.. capable than the others.","Showing you how is going to be fun."},{1.25,1.5}}
		Mode["GenSelected"] = false
		Mode["GenElement"] = nil
	end
end

