function zoomer()
	attack,idlesoff = true,true
	local charge1 = particle(char["Right Arm"],2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.7),
		Size=NS({NSK(0,100,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.5,1,0),NSK(1,1,0)})
	},Vector3.new(-1,-1,0))
	particle(char["Right Arm"],50,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][3]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2,.4),
		Size=NS({NSK(0,60,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(0.5,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(-1,-1,0))
	local ej = particle(char["Right Arm"],0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(3),
		Size=NS({NSK(0,30,0),NSK(.1,5,0),NSK(1,5,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.1,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rate=300,
		RotSpeed=NR(720),
		Rate=0
	},Vector3.new(-1,-1,0))
	ej:Emit(3)
	-- ~-~ GENZ 1 ~-~
	sound(root,"5112980602")
	sound(root,"4648407351")
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 12/60,'Sine','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,-0.6,-0.1)*ang(math.rad(-37),math.rad(-60.2),math.rad(-19.1))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.2,-0.2,0.3)*ang(math.rad(25.9),math.rad(34.2),math.rad(-25.5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-1,-0.6)*ang(math.rad(-20.6),math.rad(19.7),math.rad(35.3))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-0.1,0.7,-0.9)*ang(math.rad(163.8),math.rad(-26.1),math.rad(75.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0,0.2)*ang(math.rad(97.9),math.rad(-56.8),math.rad(42.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0.2,0,0.1)*ang(math.rad(-17.7),math.rad(51.4),math.rad(14.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	sound(root,"4648407351",2,.75)
	local pewpew = sound(root,"4836146175",2,.75)
	swait(8)
	-- ~-~ GENZ 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 12/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,0,0)*ang(math.rad(-19.1),math.rad(-6.8),math.rad(1.3))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-1,-0.3)*ang(math.rad(1.5),math.rad(-15.8),math.rad(-8.7))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,0.1,-0.5)*ang(math.rad(25),math.rad(-7.1),math.rad(7.6))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.1,1.3,0.3)*ang(math.rad(-157.2),math.rad(-10.4),math.rad(28.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.2,1.2,0.3)*ang(math.rad(-160.1),math.rad(4.1),math.rad(-17.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,-0.3)*ang(math.rad(18.4),math.rad(-0.5),math.rad(-6.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(3)
	-- ~-~ GENZ 3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 25/60,'Elastic','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(-0.3,-0.9,-0.2)*ang(math.rad(-36.1),math.rad(21.8),math.rad(14))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.9,-0.3)*ang(math.rad(-24),math.rad(-0.1),math.rad(-27.8))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1.2,0,-0.2)*ang(math.rad(33.7),math.rad(-21),math.rad(9.4))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(0.4,0.4,-1.2)*ang(math.rad(5.9),math.rad(38),math.rad(94.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.1,1.2,-0.3)*ang(math.rad(-16.3),math.rad(78.4),math.rad(144.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.2)*ang(math.rad(11.1),math.rad(-31),math.rad(-3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(5)
	if Mode["GenSelected"] then
		for i = 1,2 do
			local e = weirdeff("Slash1",{ -- part start properties
				Size=Vector3.new(10,2,5),
				Color=Mode["Colors"][2],
				Transparency = 0,
				Material="Neon",
				CFrame = root.CFrame*CFrame.new(0,-2,-5)
			},{ -- decal start properties
				Transparency = 1
			},{ -- part end properties
				Size=Vector3.new(100*i,6,50*i),
				CFrame = root.CFrame*rang(0,10,0)*CFrame.new(0,0,-45*i)*rang(0,30,0),
				Transparency = 1
			},{ -- mesh end properties
			},15/60,"Quad","Out")
			local e = weirdeff("Slash2",{ -- part start properties
				Size=Vector3.new(10,2,5),
				Color=Mode["Colors"][2],
				Transparency = 0,
				Material="Neon",
				CFrame = root.CFrame*CFrame.new(0,-2,-5)*rang(0,180,0)
			},{ -- decal start properties
				Transparency = 1
			},{ -- part end properties
				Size=Vector3.new(100*i,6,50*i),
				CFrame = root.CFrame*rang(0,170,0)*CFrame.new(0,0,45*i)*rang(0,-30,0),
				Transparency = 1
			},{ -- mesh end properties
			},15/60,"Quad","Out")
		end
		for i = -1,1,2 do
			local e = weirdeff("Blast",{ -- part start properties
				Size=Vector3.new(0,35,50),
				Color=Mode["Colors"][2],
				Transparency = -2,
				Material="ForceField",
				CFrame = root.CFrame*rang(0,30*i,20*i)*CFrame.new(-5*i,(35/2)-3,-25)
			},{ -- decal start properties
				Transparency = 1
			},{ -- part end properties
				Size=Vector3.new(0,50,140),
				CFrame = root.CFrame*rang(0,30*i,10*i)*CFrame.new(-5*i,22,-50),
				Transparency=1
			},{ -- mesh end properties
			},35/60,"Quint","Out")
		end 
		if Mode["GenElement"] == "Inferno" then
			local initDamge = makeHitbox({CFrame = root.CFrame*CFrame.new(0,0,-50),Size=Vector3.new(60,30,100)},450,false,true,5,anchorer)
			coroutine.resume(coroutine.create(function()
				local keptcolor = Mode["Colors"][1]
				swait(10)
				initDamge.Color = Color3.new(0,0,1)
				local e = PART(char)
				e.Size = Vector3.new(60,0,100)
				e.CFrame = initDamge.CFrame
				local par = particle(e,300,{
					Texture="rbxassetid://296874871",
					LightEmission=1,
					Color=CS(keptcolor),
					Lifetime=NR(1.2,2.4),
					Size=NS({NSK(0,15,7.5),NSK(.3,5,2.5),NSK(1,2.5,1.25)}),
					Transparency=NS({NSK(0,1,0),NSK(0.25,0.5,0.2),NSK(1,1,0)}),
					Speed=NR(25,70),
					Drag=2,
					SpreadAngle=Vector2.new(80,80),
					RotSpeed=NR(-70,70),
					Rotation=NR(1,360),
					Acceleration=Vector3.new(0,30,0),
					Rate=1000,
					Enabled=true
				})
				par.Parent = e
				swait(10)
				for i = 1,10 do
					local burn = makeHitbox({CFrame = initDamge.CFrame,Size=Vector3.new(60,5,100)},100,false,true,20000)
					swait(10)
					burn.Color = Color3.new(0,0,1)
					swait(10)
				end
				par.Enabled = false
				swait(60)
				e:Destroy()
			end))
			local e = weirdeff("Explosion2",{ -- part start properties
				Size=Vector3.new(75,15,75),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*rang(-90,0,0)*cf(0,15,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(25,150,25),
				CFrame = root.CFrame*rang(-90,0,0)*cf(0,75,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out")
			local e = weirdeff("Explosion",{ -- part start properties
				Size=Vector3.new(50,100,50),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*rang(-90,0,0)*cf(0,50,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(25,250,25),
				CFrame = root.CFrame*rang(-90,0,0)*cf(0,125,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},15/60,"Quad","Out")
			particle(root,300,{
				Texture="rbxassetid://296874871",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]),
				Lifetime=NR(.3,.6),
				Size=NS({NSK(0,8,2),NSK(1,12,4)}),
				Transparency=NS({NSK(0,0.7,0.2),NSK(0.5,0.8,0.2),NSK(1,1,0)}),
				Speed=NR(50,500),
				Drag=4,
				SpreadAngle=Vector2.new(15,25),
				RotSpeed=NR(-50,50),
				Rotation=NR(1,360),
				Acceleration=Vector3.new(0,20,0),
				EmissionDirection="Front",
			})
			sound(root,"241816029",1.5,2)
		elseif Mode["GenElement"] == "Gale" then
			local blowy = makeHitbox({CFrame = root.CFrame*CFrame.new(0,0,-100),Size=Vector3.new(50,50,200)},150,false,true,5,function(hb,eroot,ehum)
				if ehum.Health > 0 then
					eroot.Anchored = true
					local twn = TweenAnimate(eroot,{CFrame=root.CFrame*cf(0,0,-12)},15/60,"Quint","Out")
					twn.Completed:connect(function()
						eroot.CFrame=root.CFrame*cf(0,0,-12)
						if attack == false then
							eroot.Anchored = false
						else
							repeat swait() until attack == false
							eroot.Anchored = false
						end
					end)
				end
			end)
			delay(5/60,function() blowy.Color = Color3.new(0,0,1) end)
			local e = weirdeff("Winds",{ -- part start properties
				Size=Vector3.new(75,150,75),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*rang(-90,0,0)*cf(0,75,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(50,15,50),
				CFrame = root.CFrame*rang(-90,179,0)*cf(0,7.5,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},30/60,"Sine","Out")
			local e = weirdeff("Swirl",{ -- part start properties
				Size=Vector3.new(25,250,25),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*rang(-90,180,0)*cf(0,125,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(75,50,75),
				CFrame = root.CFrame*rang(-90,1,0)*cf(0,25,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},25/60,"Quad","Out")
			particle(root.CFrame*rang(-90,0,0)*CFrame.new(0,100,0)*rang(180,0,0),300,{
				Texture="rbxassetid://243728104",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]),
				Lifetime=NR(.3,.6),
				Size=NS({NSK(0,14,2),NSK(1,18,4)}),
				Transparency=NS({NSK(0,0.85,0.1),NSK(0.5,0.9,0.05),NSK(1,1,0)}),
				Speed=NR(50,500),
				Drag=4,
				SpreadAngle=Vector2.new(25,35),
				RotSpeed=NR(-50,50),
				Rotation=NR(1,360),
				Acceleration=Vector3.new(0,20,0),
			})
			sound(root,"192410077",1.3,2)
		elseif Mode["GenElement"] == "Deluge" then
			local blowy = makeHitbox({CFrame = root.CFrame*CFrame.new(0,0,-30),Size=Vector3.new(80,80,80)},850,false,true,5,function(hb,eroot,ehum)
				if ehum.Health > 0 then
					local bv = Instance.new("BodyVelocity")
					bv.Velocity = (hb.CFrame*rang(-60,0,0)).upVector*150
					bv.MaxForce = Vector3.new(1e9,1e9,1e9)
					bv.Parent = eroot
					swait(15)
					bv:Destroy()
				end
			end)
			delay(5/60,function() blowy.Color = Color3.new(0,0,1) end)
			local e = weirdeff("Plant",{ -- part start properties
				Size=Vector3.new(25,0,25),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*cf(0,0,-30)*rang(-60,0,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(115,150,115),
				CFrame = root.CFrame*cf(0,0,-30)*rang(-60,179,0)*cf(0,75,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},30/60,"Linear","Out")
			local e = weirdeff("Swirl",{ -- part start properties
				Size=Vector3.new(25,25,25),
				Color=Mode["Colors"][2],
				Transparency = -1,
				Material="Neon",
				CFrame = root.CFrame*cf(0,0,-30)*rang(-60,0,0)
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = 0
			},{ -- part end properties
				Size=Vector3.new(100,100,100),
				CFrame = root.CFrame*cf(0,0,-30)*rang(-60,-179,0)*cf(0,50,0),
				Transparency=1
			},{ -- decal end properties
				Transparency = 1
			},35/60,"Linear","Out")
			particle(root.CFrame*CFrame.new(0,-10,-50)*rang(-60,0,0),300,{
				Texture="rbxassetid://243728104",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]),
				Lifetime=NR(.5,1),
				Size=NS({NSK(0,30,10),NSK(1,40,12)}),
				Transparency=NS({NSK(0,0.85,0.1),NSK(0.5,0.9,0.05),NSK(1,1,0)}),
				Speed=NR(50,750),
				Drag=4,
				SpreadAngle=Vector2.new(60,60),
				RotSpeed=NR(-50,50),
				Rotation=NR(1,360),
				Acceleration=Vector3.new(0,20,0),
			})
			sound(root,"5113230537",1.5,1.25)
			sound(root,"262562442",1.8,2)
		end
	else -- if you don't have an element
		particle(root.CFrame*rang(-90,0,0)*cf(0,2,0),20,{ZOffset=2,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(20/60,40/60),
			Size=NS({NSK(0,10,0),NSK(0.25,2.5,0),NSK(1,0,0)}),
			Speed = NR(20,50),
			SpreadAngle=Vector2.new(20,20),
			Acceleration = Vector3.new(0,-15,0),
			Drag = 2})
		sound(root,"4524476032",1.5,.6)
	end
	ej:Destroy()
	pewpew:Destroy()
	swait((Alpha*60) - 5)
	attack,idlesoff = false,false
end

function PurifyingScatter() -- imagine equinox orbs + heavenly disks
	attack,idlesoff = true,true
	root.Anchored = true
	local function DISK(power,timed,speed,from,to,anglol)
		coroutine.resume(coroutine.create(function()
			local e = PART()
			e.Size = Vector3.new(0,.25,0)
			e.Color = Mode["Colors"][1]
			e.Material = "Neon"
			e.CFrame = CFrame.new(from.p,to.p)
			local m = Instance.new("SpecialMesh",e)
			m.MeshType = "Sphere"
			local tr = TRAIL(e)
			tr.FaceCamera = false
			tr.Color = CS(Mode["Colors"][3])
			tr.Attachment0.Position = Vector3.new(2*power,0,0)
			tr.Attachment1.Position = Vector3.new(2*power,0,0)
			e.Parent = char
			local g = RTweenAnimate(e,{Transparency=0,Color=Mode["Colors"][3],Size=Vector3.new(4*power,.25,4*power)},timed/60,"Quad","Out")
			particle(e,0,{ZOffset=2,
				Texture="rbxassetid://2273224484",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(.5),
				Size=NS({NSK(0,0,0),NSK(.5,2,0),NSK(1,3,1)}),
				Transparency = NS({NSK(0,0,0),NSK(.25,0.5,0),NSK(1,1,0)}),
				LockedToPart=true,
				Rate=30
			})
			particle(e,2,{ZOffset=1,
				Texture="rbxassetid://2109052855",
				LightEmission=1,
				Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
				Lifetime=NR(timed/60),
				Size=NS({NSK(0,30*power,0),NSK(1,0,0)}),
				Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
				LockedToPart=true
			},Vector3.new(0,0,0))
			local dir = (e.CFrame*rang(0,anglol or 0,0)).lookVector
			local hit,pos = rayCast(e.CFrame.p,dir,5000,workspace.Effects)
			local eq = RTweenAnimate(e,{Position=pos},(timed/60) + (.2 + (e.Position-pos).magnitude/speed),"Sine","In")
			swait(((timed/60) + (.2 + (e.Position-pos).magnitude/speed))*60)
			e.Position = pos
			local death = makeHitbox({CFrame = e.CFrame,Size=Vector3.new(40,40,40)*power,Shape="Ball"},225*power,false,true,12,anchorer)
			delay(5/60,function() death.Color = Color3.new(0,0,1) end)
			local e2 = weirdeff("SpikeBall",{ -- part start properties
				Size=Vector3.new(30*power,30*power,30*power),
				Color=Mode["Colors"][3],
				Transparency = -4,
				Material="Neon",
				CFrame = e.CFrame
			},{ -- decal start properties
				Color3=glowify(Mode["Colors"][1]),
				Transparency = -5,
			},{ -- part end properties
				Size=Vector3.new(120*power,120*power,120*power),
				Transparency = 1
			},{ -- decal end properties
				Transparency = 1
			},15/60,"Elastic","Out",0,false,0)
			local e2 = weirdeff("InvertedSphere",{ -- part start properties
				Size=Vector3.new(10*power,10*power,10*power),
				Color=Mode["Colors"][3],
				Transparency = -4,
				Material="Neon",
				CFrame = e.CFrame
			},{ -- decal start properties
			},{ -- part end properties
				Size=Vector3.new(40*power,40*power,40*power),
				Transparency = 1
			},{ -- decal end properties
			},45/60,"Sine","Out",0,false,0)
			local e2 = weirdeff("sphere",{ -- part start properties
				Size=Vector3.new(10*power,10*power,10*power),
				Color=Mode["Colors"][1],
				Transparency = -1,
				Material="Neon",
				CFrame = e.CFrame
			},{ -- decal start properties
			},{ -- part end properties
				Size=Vector3.new(30*power,30*power,30*power),
				Transparency = 1
			},{ -- decal end properties
			},45/60,"Sine","Out",0,false,0)
			--[[for i = 1,5 do
				local cfe = e.CFrame*rang(90,0,0)*randr(30,30,360)
				local randre = rand(6,14)/10
				local e2 = weirdeff("sphere",{ -- part start properties
					Size=Vector3.new(5*randre*power,20*randre*power,5*randre*power),
					Color=Mode["Colors"][1],
					Transparency = -1,
					Material="Neon",
					CFrame = cfe
				},{ -- decal start properties
				},{ -- part end properties
					Size=Vector3.new(0,50*randre*power,0),
					Transparency = 1,
					CFrame = cfe*cf(0,randre*power*rand(30,70),0)
				},{ -- decal end properties
				},(20 + (10*randre))/60,"Quad","Out",0,false,0)
			end]]
			for i = 1,3 do
				local cf = e.CFrame*randr()
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][1],
					Transparency = .05,
					Material="Neon",
					CFrame = cf
				},{ -- mesh start properties
					VertexColor = glowify(Mode["Colors"][3],Vector3.new),
					Scale = Vector3.new(.05,0,.05)
				},{ -- part end properties
					Transparency = 1,
					CFrame = cf*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
				},{ -- mesh end properties
					Scale = Vector3.new(.75,0,.75)*(math.random(30,100)/100)
				},math.random(40,160)/60,"Sine","Out")
			end
			for i = 1,2 do
				local m = rand(10,20)
				slashy(e.CFrame*rang(rsign()*90,0,0)*randr(30,30,360),Mode["Colors"][1],{20*power,40*power,90*power},4,2,{m,m*3})
			end
			e.Transparency = 1
			e:ClearAllChildren()
			sound(e,"1666361078",1.2 - power/20,1 + power/10)
			sound(e,"782200047",1.2 - power/5,1.5 + power/2.5)
			swait(60)
			e:Destroy()
		end))
	end
	if (Mode["jumped"] - 1) < (stats["JUMPS"] - 1) then
		Mode["jumped"] = Mode["jumped"] + 1
		if Anim ~= "Jump/Fall" then
			sound(root,"1316464399",1,2)
			sound(root,"588738198",1.2,1)
			-- ~-~ PUREZ 1 ~-~
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Sine','Out',0,false,0
			local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.8,-0.2)*ang(math.rad(-22.1),math.rad(5.7),math.rad(1.9))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(LH,{C0=cf(-0.9,-1.1,-0.6)*ang(math.rad(-45.7),math.rad(3.1),math.rad(-3.5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(RH,{C0=cf(1.1,-0.2,-0.8)*ang(math.rad(-4.9),math.rad(-12.1),math.rad(-2.1))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(LW,{C0=cf(-1.4,0.7,0.4)*ang(math.rad(-16),math.rad(41.9),math.rad(-21.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(RW,{C0=cf(1.5,0.2,-0.1)*ang(math.rad(161),math.rad(49.8),math.rad(-72.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			RTweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.3)*ang(math.rad(9.9),math.rad(-8.1),math.rad(-0.7))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			swait(Alpha*60)
			for i = 1,5 do
				weirdeff("VBLASTMESH2",{ -- part start properties
					Size=Vector3.new(10,0,10),
					Color=Mode["Colors"][1],
					Transparency = .05,
					Material="Neon",
					CFrame = root.CFrame*rang(-6*i,0,0)*CFrame.new(0,50,0)
				},{ -- mesh start properties
					VertexColor = Vector3.new(Mode["Colors"][1].R*5,Mode["Colors"][1].G*5,Mode["Colors"][1].B*5),
					Scale = Vector3.new(0,5,0)
				},{ -- part end properties
					Transparency = 1,
					CFrame = root.CFrame*rang(-6*i,0,0)*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)*CFrame.new(0,50,0)
				},{ -- mesh end properties
					Scale = Vector3.new(1,0,1)*(math.random(25,100)/100)
				},math.random(20,80)/80,"Quad","Out")
			end
			weirdeff("Explosion2",{ -- part start properties
				Size=Vector3.new(50,250,50),
				Color=Mode["Colors"][1],
				Transparency = -2,
				Material="Neon",
				CFrame = root.CFrame*rang(-30,0,0)*CFrame.new(0,125,0)
			},{ -- decal start properties
				Color3 = Color3.new(Mode["Colors"][2].R*10,Mode["Colors"][2].G*10,Mode["Colors"][2].B*10),
				Transparency = -1
			},{ -- part end properties
				Size=Vector3.new(150,50,150),
				Transparency = 1,
				CFrame = root.CFrame*rang(-30,0,0)*CFrame.new(0,25,0)
			},{ -- decal end properties
				Transparency = 1
			},.25,"Quad","Out")
			sound(root,"1295446488",1.3,2)
		end
		sound(root,"763716870",.8,2.5)
		sound(root,"782225570",1,2)
		sound(root,"763718160",.8,2)
		-- ~-~ PUREZ 2 ~-~
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Sine','In',0,false,0
		local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,-0.3)*ang(math.rad(-31.2),math.rad(7.1),math.rad(0.1))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-1.1,-1.1,0)*ang(math.rad(-25),math.rad(0),math.rad(5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1.1,-0.4,-0.9)*ang(math.rad(-30.7),math.rad(-7.5),math.rad(-4.4))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.6,0.7,0.2)*ang(math.rad(-24),math.rad(21.3),math.rad(-19.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(0.4,0.8,-1)*ang(math.rad(-175.3),math.rad(-21.7),math.rad(-114))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,-0.4)*ang(math.rad(23.1),math.rad(-10),math.rad(0.9))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		sound(root,"763717569")
		sound(root,"231917934",1.3,.75)
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 45/60,'Quint','Out',0,false,0
		for i = 1,5 do
			DISK(1+(i/10),15,750,root.CFrame*cf(0,(100/5)*i,0),root.CFrame*rang(-30 + 2.5*i,0,0)*CFrame.new(0,(75/5)*i,-100))
		end
		root.CFrame = root.CFrame*cf(0,75,-100)
		local sse = RTweenAnimate(root,{CFrame=root.CFrame*rang(-30,0,0)*CFrame.new(0,50,0)*rang(30,0,0)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		delay(Alpha,function()
			for i = -2,2,1 do
				DISK(3,15,1000,root.CFrame*CFrame.new(0,0,-5),mouse.Hit,12.5*i)
			end
		end)
		swait(25)
		-- ~-~ PUREZ 4 ~-~
		slashy(root.CFrame*rang(-120,0,0),Mode["Colors"][1],{6,8,10},2,2,{10,20},90)
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 30/60,'Elastic','Out',0,false,0
		local eg = RTweenAnimate(RootJoint,{C0=cf(0.1,1.1,-0.2)*ang(math.rad(-44),math.rad(-32.6),math.rad(-13.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LH,{C0=cf(-1.5,-0.4,-0.7)*ang(math.rad(-19.6),math.rad(22.3),math.rad(11.2))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RH,{C0=cf(1.1,-1.1,-0.3)*ang(math.rad(-44.5),math.rad(-0.6),math.rad(11.2))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(LW,{C0=cf(-1.3,0.4,0.4)*ang(math.rad(-6.9),math.rad(34.4),math.rad(-77.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(RW,{C0=cf(1.4,0.9,0.7)*ang(math.rad(0.9),math.rad(-10),math.rad(95.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		RTweenAnimate(Torso.Neck,{C0 =cf(0.1,0,-0.1)*ang(math.rad(1.6),math.rad(30.3),math.rad(2.7))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
	end
	attack,idlesoff = false,false
	root.Anchored = false
end

function ImpossibleOverdrive()
	attack,idlesoff = true,true
	local function marazmify(part,index)
		if Mode["Name"] ~= "MARAZM" then return part end
		coroutine.resume(coroutine.create(function()
			if part:IsA("BasePart") then
				while part.Parent ~= nil do
					part.Color = Mode["Colors"][index]:lerp(Color3.new(1,1,1),.1)
					swait()
				end
			elseif part:IsA("ParticleEmitter") then
				while part.Parent ~= nil do
					part.Color = CS(Mode["Colors"][index]:lerp(Color3.new(1,1,1),.1))
					swait()
				end
			end
		end))
		return part
	end
	--[[SKILLCUTIN(Mode["Colors"][1],Mode["Colors"][2],"// OMEN OF CATACLYSM  //","IMPOSSIBLE//OVERDRIVE","1837829863")
	-- ~-~ IMPOSSIBLEOVERDRIVE 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Back','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.4,0.2)*ang(math.rad(-30),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1,-0.6,-0.1)*ang(math.rad(5),math.rad(10),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-0.4,-0.7)*ang(math.rad(0),math.rad(-10),math.rad(5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-0.9,0.5,-0.5)*ang(math.rad(79.5),math.rad(40.1),math.rad(64.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(0.9,0.6,-0.9)*ang(math.rad(139.1),math.rad(-44.5),math.rad(-33.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.6)*ang(math.rad(-35),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(5)
	-- ~-~ IMPOSSIBLEOVERDRIVE 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Quad','Out',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,0,0.1)*ang(math.rad(10),math.rad(-0.9),math.rad(-4.9))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-0.9,-0.9,-0.2)*ang(math.rad(-19.1),math.rad(10),math.rad(-5.1))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-0.9,-0.2)*ang(math.rad(-16),math.rad(-14.5),math.rad(15.5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.5,0,-0.2)*ang(math.rad(154.5),math.rad(53.5),math.rad(-18.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.5,0,0)*ang(math.rad(150),math.rad(-55),math.rad(20))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(-0.3,0.1,0.2)*ang(math.rad(-4.9),math.rad(0.9),math.rad(-20))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)]]
	local charger = marazmify(particle(root,0,{ZOffset=2,
		Texture="rbxassetid://2273224484",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.2),
		Size=NS({NSK(0,0,0),NSK(1,10,5)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rate=300
	},Vector3.new(0,17.5/2,0)),3)
	marazmify(particle(root,120,{ZOffset=1,
		Texture="rbxassetid://3028571188",
		LightEmission=1,
		Color=CS(Mode["Colors"][2]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.3,.6),
		Size=NS({NSK(0,125/2,50/2),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(1,0,0)}),
		LockedToPart=true,
		Rotation=NR(1,360)
	},Vector3.new(0,17.5/2,0)),1)
	marazmify(particle(root,2,{ZOffset=1,
		Texture="rbxassetid://2109052855",
		LightEmission=1,
		Color=CS(Mode["Colors"][1]:lerp(Color3.new(1,1,1),.1)),
		Lifetime=NR(.5),
		Size=NS({NSK(0,150/2,0),NSK(1,0,0)}),
		Transparency = NS({NSK(0,1,0),NSK(.25,0.5,0),NSK(1,0,0)}),
		LockedToPart=true
	},Vector3.new(0,17.5/2,0)),2)
	sound(root,"5113647461",1.5)
	-- ~-~ IMPOSSIBLEOVERDRIVE 3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Sine','InOut',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-0.2,0.1)*ang(math.rad(15),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-0.9,-0.3)*ang(math.rad(-24.1),math.rad(10),math.rad(-5.1))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(1,-0.9,-0.2)*ang(math.rad(-25),math.rad(-25),math.rad(10))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.3,1.3,-0.2)*ang(math.rad(-7.1),math.rad(-67.9),math.rad(156.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.3,1.2,-0.2)*ang(math.rad(1.9),math.rad(69.6),math.rad(-169.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*ang(math.rad(0),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	sound(root,"5132757647",1.3,2)
	swait(stats["ASPD"]*15)
	root.Anchored = true
	local hb1 = makeHitbox({CFrame = root.CFrame*CFrame.new(1054/1.75,-3,0),Size=Vector3.new(1024,1024,1024),Shape="Ball"},4500,false,false,20,function(hb,ehum,eroot,etors)
		if ehum.Health > 0 then
			local bv = Instance.new("BodyVelocity")
			bv.Velocity = Vector3.new(0,1000,0)
			bv.MaxForce = Vector3.new(1e9,1e10,1e9)
			bv.Parent = eroot
			swait(15)
			bv:Destroy()
		end
	end)
	local hb2 = makeHitbox({CFrame = root.CFrame*CFrame.new(-1054/1.75,-3,0),Size=Vector3.new(1024,1024,1024),Shape="Ball"},4500,false,false,20,function(hb,ehum,eroot,etors)
		if ehum.Health > 0 then
			local bv = Instance.new("BodyVelocity")
			bv.Velocity = Vector3.new(0,1000,0)
			bv.MaxForce = Vector3.new(1e9,1e10,1e9)
			bv.Parent = eroot
			swait(15)
			bv:Destroy()
		end
	end)
	local hb3 = makeHitbox({CFrame = root.CFrame*CFrame.new(0,-3,0),Size=Vector3.new(2048,2048,2048),Shape="Ball"},500,false,false,20)
	-- ~-~ IMPOSSIBLEOVERDRIVE 4 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Sine','In',0,false,0
	local eg = RTweenAnimate(RootJoint,{C0=cf(0,-1.2,-0.4)*ang(math.rad(-55),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LH,{C0=cf(-1.1,-0.3,-1.3)*ang(math.rad(-35),math.rad(5),math.rad(-5))*ang(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RH,{C0=cf(0.9,-0.3,-1.3)*ang(math.rad(-35),math.rad(-5),math.rad(5))*ang(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(LW,{C0=cf(-1.2,-0.1,-1.1)*ang(math.rad(44.3),math.rad(-10.5),math.rad(-46))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(RW,{C0=cf(1.3,0,-1)*ang(math.rad(47.2),math.rad(13.7),math.rad(41.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	RTweenAnimate(Torso.Neck,{C0 =cf(0,0.4,0.7)*ang(math.rad(-40),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	sound(root,"1467330245",1.3,2)
	sound(root,"5132753657",.8,1.5)
	charger:Destroy()
	marazmify(particle(root,180,{ZOffset=1,
		Texture="rbxassetid://3028571188",
