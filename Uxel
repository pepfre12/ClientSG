function unanchor()
	if UNANCHOR == true then
		g = CharacterA:GetChildren()
		for i = 1, #g do
			if g[i].ClassName == "Part" then
				g[i].Anchored = false
			end
		end
	end
end
local perfix = "!"
local c1 = perfix.."play "
local c2 = perfix.."pitch "
local c3 = perfix.."default"
local c4 = perfix.."vol "
Player.Chatted:Connect(function(m)
	if string.sub(string.lower(m),1,string.len(c1)) == string.lower(c1) then
		VIS = m:sub(6)
		chatfunc("New sound has been set.",Color3.fromRGB(82, 124, 174))
	  end
	if string.sub(string.lower(m),1,string.len(c2)) == string.lower(c2) then
		PITCH = m:sub(7)
		chatfunc("Pitch was set to "..m:sub(7)..".",Color3.fromRGB(82, 124, 174))
	  end
	if string.sub(string.lower(m),1,string.len(c4)) == string.lower(c4) then
		VOLUME = m:sub(5)
	  end
	if string.sub(string.lower(m),1,string.len(c3)) == string.lower(c3) then
		PITCH = 1
		VIS = 635908787
		VOLUME = 1
		chatfunc("Set to default.",Color3.fromRGB(82, 124, 174))
	  end
end)
