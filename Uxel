		local pos = LeftLeg.CFrame*CFrame.new(0,0,0)*CFrame.Angles(0,0,0)
			JJEffects("Block",{
	Time = mt,
	parent = LeftLeg,
	Anchored=true,
	material = "Neon",
	color = LeftLeg.Color,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1, 2, 1),
				size2 = Vector3.new(1, 2, 1),
	cframe = pos*CF(0,0,0)*angles(RAD(0),RAD(0),RAD(0))
})
	local pos = RightLeg.CFrame*CFrame.new(0,0,0)*CFrame.Angles(0,0,0)
			JJEffects("Block",{
	Time = mt,
	parent = RightLeg,
	Anchored=true,
	material = "Neon",
	color = RightLeg.Color,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1, 2, 1),
				size2 = Vector3.new(1, 2, 1),
	cframe = pos*CF(0,0,0)*angles(RAD(0),RAD(0),RAD(0))
	})
	local pos = Torso.CFrame*CFrame.new(0,0,0)*CFrame.Angles(0,0,0)
			JJEffects("Block",{
	Time = mt,
	parent = Torso,
	Anchored=true,
	material = "Neon",
	color = Torso.Color,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(2, 2, 1),
				size2 = Vector3.new(2, 2, 1),
	cframe = pos*CF(0,0,0)*angles(RAD(0),RAD(0),RAD(0))
	})
	local pos = Head.CFrame*CFrame.new(0,0,0)*CFrame.Angles(0,0,0)
			JJEffects("Block",{
	Time = mt,
	parent = Head,
	Anchored=true,
	material = "Neon",
	color = Head.Color,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1, 1, 1),
				size2 = Vector3.new(1, 1, 1),
	cframe = pos*CF(0,0,0)*angles(RAD(0),RAD(0),RAD(0))
	})
]]
		end
		end
	unanchor()
	Humanoid.MaxHealth = "inf"
	Humanoid.Health = "inf"
	if Rooted == false then
		Disable_Jump = false
		Humanoid.WalkSpeed = Speed
	elseif Rooted == true then
		Disable_Jump = true
		Humanoid.WalkSpeed = 0
	end

	if INSTANT == true then
		if TRANSFORMED == false then
			sick.TimePosition = 119
			sick:Play()
		end
		TRANSFORMED = true
	end
	if sick.Parent ~= Torso then
	sick = IT("Sound", Torso)
	end
	sick.Parent = Torso
	sick.Playing = true
    sick.Looped = true
	sick.Volume = VOLUME
	sick.Pitch = PITCH
	sick.SoundId = "rbxassetid://"..VIS --871066478
	sick.Name = "le muzik"
end
