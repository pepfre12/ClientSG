local Keys
function KeyDown(Key)
	KEYHOLD = true
	if Key == "1" and ATTACK == false then
		VIS = 3584953190
	end
	if Key == "q" and ATTACK == false then
		CFuncs.Sound.Create("rbxassetid://149162327", Torso, 5.25, 1)
	end
	if Key == "t" and ATTACK == false then
		local texts = {
[1]="give me ur rares1!",
[2]="bro,,,",
[3]="Get shutup"
}
local mth = texts[math.random(1,3)]
		chatfunc(mth,Color3.fromRGB(82, 124, 174))
	end
	if Key == "z" and ATTACK == false then
		Boop()
	end
	if Key == "f" and ATTACK == false then
		Taunt2()
	end
	if Key == "x" and ATTACK == false then
		AOETime(RootPart.Position,1e9)
	end
		
end
chatfunc("Made by PROISDEAD12",Color3.fromRGB(82, 124, 174))
function KeyUp(Key)
	KEYHOLD = false
end

	Mouse.Button1Down:connect(function(NEWKEY)
		MouseDown(NEWKEY)
	end)
	Mouse.Button1Up:connect(function(NEWKEY)
		MouseUp(NEWKEY)
	end)
	Mouse.KeyDown:connect(function(NEWKEY)
		KeyDown(NEWKEY)
	end)
	Mouse.KeyUp:connect(function(NEWKEY)
		KeyUp(NEWKEY)
	end)

