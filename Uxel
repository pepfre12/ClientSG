						AOETime(POS,70)
						break          
					else
						BULLET.CFrame = Clerp(BULLET.CFrame,CF(BULLET.Position,Mouse.Hit.p),0.001) * CF(0,0,-5)
						WACKYEFFECT({
							Time = 12,
							EffectType = "Round Slash",
