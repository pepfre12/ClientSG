	end	
	WEL.CFrame  = Torso.CFrame*CFrame.new(0+ 1 * COS(sine / 48),4 + 1 * COS(sine / 28),3 + 1 * COS(sine / 34))
    MainW.C1=clerp(MainW.C1,cf(0,0,0)*angles(RAD(0),RAD(0-sine),RAD(0)),.3)
	CoreR1W.C1=clerp(CoreR1W.C1,cf(0,0,0)*angles(RAD(sine-sine-sine),RAD(sine-sine-sine),RAD(sine-sine-sine)),.3)
	CoreR2W.C1=clerp(CoreR2W.C1,cf(0,0,0)*angles(RAD(sine+sine+sine),RAD(sine+sine+sine),RAD(sine+sine+sine)),.3)
	HaloW.C1=clerp(HaloW.C1,cf(1.2,0,0)*angles(RAD(0+ 1 * COS(sine / 32)),RAD(3 + 2 * COS(sine / 32)),RAD(90)),.3)
	col4 = Color3.new(math.min(0),math.min(GetClientProperty(sick,'PlaybackLoudness')/350),math.min(GetClientProperty(sick,'PlaybackLoudness')/350))
--[[local pos = RightArm.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(0,0,0)
JJEffects("RoundBlock",{
	Time = t1,
	parent = RightArm,
	Anchored=true,
	material = "Neon",
	color = col4,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1.1,1.1,1.1),
		size2 = Vector3.new(1.1,1.1,1.1),
	cframe = pos*CF(0,0,0)*angles(RAD(0+ 360 * COS(sine / 13)),RAD(0+ 360 * COS(sine / 13)),RAD(0+ 360 * COS(sine / 13)))
})
	local pos = LeftArm.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(0,0,0)
	JJEffects("RoundBlock",{
	Time = t1,
	parent = RightArm,
	Anchored=true,
	material = "Neon",
	color = col4,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1.1,1.1,1.1),
		size2 = Vector3.new(1.1,1.1,1.1),
	cframe = pos*CF(0,0,0)*angles(RAD(0- 360 * COS(sine / 12)),RAD(0- 360 * COS(sine / 12)),RAD(0- 360 * COS(sine / 12)))
})
		local pos = LeftLeg.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(0,0,0)
	JJEffects("RoundBlock",{
	Time = t2,
	parent = LeftLeg,
	Anchored=true,
	material = "Neon",
	color = col4,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1.1,1.1,1.1),
		size2 = Vector3.new(1.1,1.1,1.1),
	cframe = pos*CF(0,0,0)*angles(RAD(0+ 360 * COS(sine / 13)),RAD(0+ 360 * COS(sine / 13)),RAD(0+ 360 * COS(sine / 13)))
})
	local pos = RightLeg.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(0,0,0)
	JJEffects("RoundBlock",{
	Time = t2,
	parent = RightLeg,
	Anchored=true,
	material = "Neon",
	color = col4,
	transparency = 0.7,
	transparency2 = 0.7,
	size = Vector3.new(1.1,1.1,1.1),
		size2 = Vector3.new(1.1,1.1,1.1),
	cframe = pos*CF(0,0,0)*angles(RAD(0- 360 * COS(sine / 12)),RAD(0- 360 * COS(sine / 12)),RAD(0- 360 * COS(sine / 12)))
	})
	
	
	----------------------------
	

	
	]]
	
	
	
	local sidevec = CLAMP((RootPart.Velocity*RootPart.CFrame.RightVector).X+(RootPart.Velocity*RootPart.CFrame.RightVector).Z,-Humanoid.WalkSpeed,Humanoid.WalkSpeed)
	local forwardvec =  CLAMP((RootPart.Velocity*RootPart.CFrame.LookVector).X+(RootPart.Velocity*RootPart.CFrame.LookVector).Z,-Humanoid.WalkSpeed,Humanoid.WalkSpeed)
	local sidevelocity = sidevec/Humanoid.WalkSpeed
	local forwardvelocity = forwardvec/Humanoid.WalkSpeed
	if TORSOVERTICALVELOCITY > 1 and HITFLOOR == nil then
			ANIM = "Jump"
			if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			--Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-40), RAD(0), RAD(20)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-40), RAD(0), RAD(-20)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.3) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(-5), RAD(0), RAD(-20)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.3) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-5), RAD(0), RAD(20)), 1 / Animation_Speed)
            end
		elseif TORSOVERTICALVELOCITY < -1 and HITFLOOR == nil then
			ANIM = "Fall"
			if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			--Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(60)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-60)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(20)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(10)), 1 / Animation_Speed)
            end
		elseif TORSOVELOCITY < 1 and HITFLOOR ~= nil then
			ANIM = "Idle"
		if ATTACK == false then
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(10), RAD(5 * SIN(SINE / 40)), RAD(5 * SIN(SINE / 35))), 0.5 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0.5+ 0.5 * COS(sine / 33), 0 + 0.5 * COS(sine / 25), 3 + 0.5 * COS(sine / 28)) * ANGLES(RAD(0+ 8 * COS(sine / 28)), RAD(0), RAD(0)), 0.5 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(0 + 10 * COS(sine / 28))) * RIGHTSHOULDERC0, 0.3 / Animation_Speed)
		    LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-0 - 10 * COS(sine / 28))) * LEFTSHOULDERC0, 0.3 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 + 0.05 * SIN(SINE / 35), -0.01) * ANGLES(RAD(0), RAD(90), RAD(-20+ 8 * COS(sine / 28))), 0.5 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 + 0.05 * SIN(SINE / 35), -0.01) * ANGLES(RAD(0), RAD(-90), RAD(25+ 8 * COS(sine / 28))), 0.5 / Animation_Speed)
			
			
		end
	elseif TORSOVELOCITY > 1 and HITFLOOR ~= nil then
		ANIM = "Walk"
		if ATTACK == false then
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(-forwardvelocity*25), RAD(5 * SIN(SINE / 40)), RAD(-sidevelocity*45)), 0.5 / Animation_Speed)
			--RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0.5+ 0.5 * COS(sine / 33), 0 + 0.5 * COS(sine / 25), 3 + 0.5 * COS(sine / 28)) * ANGLES(RAD(35+ 8 * COS(sine / 28)), RAD(0 + RootPart.RotVelocity.Y*1.5),RAD(0 - RootPart.RotVelocity.Y - 1 * COS(sine / 6))), 0.5 / Animation_Speed)
			RootJoint.C0 = clerp(RootJoint.C0, ROOTC0 * CF(-0.5+ 0.5 * COS(sine / 33), 0 + 0.5 * COS(sine / 25), 3 + 0.5 * COS(sine / 28)) * ANGLES(RAD(forwardvelocity*35 + 2.5), RAD(-sidevelocity*10) - RootPart.RotVelocity.Y / 75, RAD(0)), 0.5 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-forwardvelocity*35), RAD(-sidevelocity*10) + RootPart.RotVelocity.Y / 75, RAD(0)) * RIGHTSHOULDERC0, 0.3 / Animation_Speed)
		    LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-forwardvelocity*35), RAD(-sidevelocity*10) + RootPart.RotVelocity.Y / 75, RAD(-0 - 10 * COS(sine / 28))) * LEFTSHOULDERC0, 0.3 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 + 0.05 * SIN(SINE / 35), -0.01) *ANGLES(RAD(-forwardvelocity*35), RAD(0), RAD(-sidevelocity*10) + RootPart.RotVelocity.Y / 75)*ANGLES(RAD(0), RAD(90), RAD(0)), 0.5 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 + 0.05 * SIN(SINE / 35), -0.01) *ANGLES(RAD(-forwardvelocity*35), RAD(0), RAD(-sidevelocity*10) + RootPart.RotVelocity.Y / 75) *ANGLES(RAD(0), RAD(-90), RAD(0)), 0.5 / Animation_Speed)	
--[[
local pos = RightArm.CFrame*CFrame.new(0,0,0)*CFrame.Angles(0,0,0)
			JJEffects("Block",{
	Time = mt,
	parent = RightArm,
	Anchored=true,
	material = "Neon",
	color = RightArm.Color,
