CreateSound(1413550336, Torso, 3, 1, false)
CameraShake(HITPOS, 15, 2, 180)
WACKYEFFECT({Time = 85, EffectType = "Sphere", Size = VT(15,15,15), Size2 = VT(17,17,17), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0), SoundID = 0, SoundPitch = 0, SoundVolume = 0, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 35})
WACKYEFFECT({Time = 85, EffectType = "Sphere", Size = VT(12,12,12), Size2 = VT(15,15,15), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1), SoundID = 0, SoundPitch = 0, SoundVolume = 0, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 35})
