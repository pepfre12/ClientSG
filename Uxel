function Joy()
	ATTACK = true
	Rooted = true
	local LOOP = true
	KEY = Mouse.KeyDown:connect(function(NEWKEY)
		if NEWKEY == "c" then
			KEY:Disconnect()
			LOOP = false
		end
	end)
	repeat
		for i = 0, 0.5, 0.1 / Animation_Speed do
		Swait()
		if LOOP == false then
		break
		end
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(0.9, 0.5, -0.5) * ANGLES(RAD(110), RAD(0), RAD(-80)) * RIGHTSHOULDERC0, 0.3 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-0.9, 0.5, -0.5) * ANGLES(RAD(90), RAD(0), RAD(80)) * LEFTSHOULDERC0, 0.3 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.6, -0.3) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		for i = 0, 0.5, 0.1 / Animation_Speed do
		Swait()
		if LOOP == false then
		break
		end
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, -0.2) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(0.9, 0.5, -0.5) * ANGLES(RAD(110), RAD(0), RAD(-80)) * RIGHTSHOULDERC0, 0.3 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-0.9, 0.5, -0.5) * ANGLES(RAD(90), RAD(0), RAD(80)) * LEFTSHOULDERC0, 0.3 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.8, -0.3) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.8, -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
	until LOOP == false
	ATTACK = false
	Rooted = false
end

function chatfunc(text, color)
	local chat = coroutine.wrap(function()
		if Character:FindFirstChild("TalkingBillBoard") ~= nil then
			Character:FindFirstChild("TalkingBillBoard"):destroy()
		end
		local naeeym2 = Instance.new("BillboardGui", Character)
		naeeym2.Size = UDim2.new(0, 500, 0, 70)
		naeeym2.StudsOffset = Vector3.new(0, 2, 0)
		naeeym2.Adornee = Head
		naeeym2.Name = "TalkingBillBoard"
		local tecks2 = Instance.new("TextLabel", naeeym2)
		tecks2.BackgroundTransparency = 1
		tecks2.BorderSizePixel = 0
		tecks2.Text = ""
		tecks2.Font = "Fantasy"
		tecks2.TextSize = 30
		tecks2.TextStrokeTransparency = 0
		tecks2.TextColor3 = color
		tecks2.TextStrokeColor3 = Color3.new(0, 0, 0)
		tecks2.Size = UDim2.new(1, 0, 0.5, 0)
		for i = 1.5, string.len(text) do
			CFuncs.Sound.Create("rbxassetid://149162327", Head, 5.25, 1)
			tecks2.Text = (text)
			swait()
		end
		wait(1)
		naeeym2:Destroy()
	end)
	chat()
end

function ch()
	for i = 0, 0.58, 0.1 / Animation_Speed do
			Swait()
local pos = Torso.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(math.random(0,360),math.random(0,360),math.random(0,360))
WACKYEFFECT({
	Time = 23,
	EffectType = "Sphere",
	Size = VT(50,50,50),
	Size2 = VT(0,0,0),
	Transparency = 0,
	Transparency2 = 1,
	CFrame = pos,
	MoveToPos = pos*CFrame.new(0,-math.random(75,125)/100,0).p,
	RotationX = 0,
	RotationY = 0,
	RotationZ = 0,
	Material = "Neon",
	Color = Color3.fromRGB(82, 124, 174),
	SoundID = nil,
	SoundPitch = nil,
	SoundVolume = nil
				})
		end
		--CFuncs["Sound"].Create("rbxassetid://153832545", char, 0.5, 1)
		CFuncs["Sound"].Create("rbxassetid://2648568095", Torso, 0.5, 1)
end

function Boop()
	ATTACK = true
	Rooted = false         
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	CreateSound("233856115",RootPart,8,1.2,false)
	local BULLET = CreatePart(3, Effects, "Neon", 0, 0, "Pearl", "Spheres", VT(7,7,10), true)
	local POS = CF(RightArm.Position)*ANGLES(RAD(0),RAD(MRANDOM(0,360)),RAD(0))*CF(0,0,MRANDOM(2,45))
	BULLET.CFrame = CF(POS.p,Mouse.Hit.p)
	BULLET.Color = C3(.25,.5,1)
	MakeForm(BULLET,"Ball")
	coroutine.resume(coroutine.create(function()
		for i =1, 200 do
			Swait()
				local HIT,POS,NORMAL = Raycast(BULLET.Position,BULLET.CFrame.lookVector,5,Character)
