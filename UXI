function ModeSw(name)
	for i=1,#Modes do
		if Modes[i].Name == name then
			CurrentM = Modes[i]
		end
	end
	text.Text = CurrentM.Name
	for _,v in next, wingModel:GetDescendants() do
	if(v:IsA'BasePart')then
		v.Color = CurrentM.Color
		elseif(v:IsA'Trail')then
		v.Color = ColorSequence.new(CurrentM.Color)
	end
	end
	text.TextColor3 = CurrentM.Color
	Song.SoundId = "rbxassetid://"..CurrentM.SongId
	Song.Pitch = CurrentM.Pitch
	Song.PlaybackSpeed = CurrentM.PlaybackSpd
	WingAnim = CurrentM.WingAnim
	Song.TimePosition = 0
	Hum.WalkSpeed=CurrentM.WS
end
UIS.InputBegan:connect(function(io,gpe)
	if(gpe or data.User~=data.Local)then return end
	if(io.KeyCode==Enum.KeyCode.One)then
