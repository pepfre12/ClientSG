function ModeSw(name)
	for i=1,#Modes do
		if Modes[i].Name == name then
			CurrentM = Modes[i]
		end
	end
	text.Text = CurrentM.Name
	for _,v in next, wingModel:GetDescendants() do
	if(v:IsA'BasePart')then
		v.Color = CurrentM.Color
		elseif(v:IsA'Trail')then
		v.Color = ColorSequence.new(CurrentM.Color)
LWP3W.C0=lwp3
		RWP1W.C0=rwp1
		RWP2W.C0=rwp2
		RWP3W.C0=rwp3
		NK.C0=head0
		RJ.C0=torso0
		RH.C0=rleg0
		RS.C0=rarm0
		LH.C0=lleg0
		LS.C0=larm0
		NK.C1=head1
		RJ.C1=torso1
		RH.C1=rleg1
		RS.C1=rarm1
		LH.C1=lleg1
		LS.C1=larm1
		if(sine-Sin>.8 or sine-Sin<-.8)then
		sine=Sin
		end
		if(hue-hu>.8 or hue-hu<-.8)then
		hue=hu
		end
		if(Wingsine-ssin>.8 or Wingsine-ssin<-.8)then
		Wingsine=ssin
		end
		local music = Song
		if(music and musD.pit)then
		music.Pitch = musD.pit
		end
		if(music and music.SoundId)then
			music.SoundId = musD.ID
		end
		CurrentM = txtt
end
