	LWP2W.C0 = LWP2W.C0:lerp(CF.N(4.25,-1,-1)*CF.A(M.R(10+15*M.C(Wingsine/32)),0,M.R(15+7.5*M.C(Wingsine/32))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(6.5,.5,-1)*CF.A(M.R(15+20*M.C(Wingsine/32)),0,M.R(30+9*M.C(Wingsine/32))),.2)
	
