local cen = CreateParta(m,1,1,"SmoothPlastic",BrickColor.random())
CreateWeld(cen,root,cen,0,12.5,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
local effar2 = Instance.new("ParticleEmitter",cen)
effar2.Texture = "rbxassetid://3959152627"
effar2.LightEmission = 1
effar2.Color = ColorSequence.new(Color3.new(1,0,0))
effar2.Rate = 50
effar2.Enabled = false
effar2.EmissionDirection = "Front"
effar2.Lifetime = NumberRange.new(2.5)
