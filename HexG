	partecle2.Transparency = NumberSequence.new(0,1)
	partecle2.Size = NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(0.1,25),NumberSequenceKeypoint.new(1,0)}
	partecle2.Acceleration = Vector3.new(0,5,0)
