	})
	groovy.TimePosition = 0
	groovy.SoundId = "rbxassetid://3043964557"
	local partecle2 = Instance.new("ParticleEmitter",Torso)
	partecle2.LightEmission = 9999
	partecle2.LightInfluence = 0
	partecle2.EmissionDirection = "Top"
	partecle2.Enabled = true
	partecle2.Texture = "http://www.roblox.com/asset/?id=1084970835"
