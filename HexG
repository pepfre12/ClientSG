	tweenserv:Create(yes,TweenInfo.new(3,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false),{Brightness = 0}):Play()
	ray:Destroy()
	partecle2:Destroy()
	spawn(function()
		for i=1,100 do
			for i=1,3 do
				local d = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*5),0)
				local e = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*10),0)
				local f = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*15),0)
				local g = Torso.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				tweeneff("Sphere",g,Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1),Vector3.new(3,99999999999,3),0,"Neon",0,{Transparency = 1},0.25,"Linear","Out",0,false)
				local a = Torso.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				local b = Torso.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				local speen = Torso.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				tweenmesh("Slash",a,MainColor,Vector3.new(0,0.1,0),0,"Neon",0,{Transparency = 1, CFrame = a*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180))), Size = Vector3.new(math.random(0,1000),0.1,math.random(0,1000))},0.25,"Quint","Out",0,false)
				tweenmesh("Slash",a,SecondColor,Vector3.new(0,0.1,0),0,"ForceField",0,{Transparency = 1, CFrame = a*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180))), Size = Vector3.new(math.random(0,1000),0.1,math.random(0,1000))},0.25,"Quint","Out",0,false)
				tweeneff("blo k",b,MainColor,Vector3.new(0.5,0.5,0.5),0,"Neon",0,{Transparency = 1, CFrame = b*CFrame.new(math.random(5,20),0,0), Size = Vector3.new(0,0,0)},0.25,"Linear","Out",0,false)
			end
			swait()
		end
		bodypos:Destroy()
	end)
	apotheosisform()
	attack = false
	attackanim = false
	wait(3)
	yes:Destroy()
end
function apotheosisform()
	MainColor = Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1)
	SecondColor = Color3.fromRGB(0,0,0)
	modechange({
		maincolor = MainColor,
		secondcolor = SecondColor,
		wingmat = "Neon",
		secondwingmat = "Neon",
		name = "APOTHEOSIS",
		speed = 500,
		jumppower = 100,
		font = "Gotham",
		theme = 3043964557,
		songinfo = "LunaticSounds - Dement ~After Legend~",
		pitch = 1
	})
	glitch = "APOTHEOSIS"
end
