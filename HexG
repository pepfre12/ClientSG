			end
		end]]
	end
end))
-- Final Loop
while true do
	swait()
	for i,v in pairs(char:GetChildren()) do
		if v.Name == "Armour" then
			v.Color = wings[2].Color.Color
		end
	end
	--replication
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		if v.Character then
			local a = v.PlayerGui
			if a:FindFirstChild(char.Name.."'s stuff") then
				if a:FindFirstChild(char.Name.."'s stuff").mode.Value ~= glitch then
					a:FindFirstChild(char.Name.."'s stuff").mode.Value = glitch
				end	
				if a:FindFirstChild(char.Name.."'s stuff").lwing1.Value ~= lwing1weld then
					a:FindFirstChild(char.Name.."'s stuff").lwing1.Value = lwing1weld
				end	
				if a:FindFirstChild(char.Name.."'s stuff").lwing2.Value ~= lwing2weld then
					a:FindFirstChild(char.Name.."'s stuff").lwing2.Value = lwing2weld
				end	
				if a:FindFirstChild(char.Name.."'s stuff").lwing3.Value ~= lwing3weld then
					a:FindFirstChild(char.Name.."'s stuff").lwing3.Value = lwing3weld
				end			
				if a:FindFirstChild(char.Name.."'s stuff").rwing1.Value ~= rwing1weld then
					a:FindFirstChild(char.Name.."'s stuff").rwing1.Value = rwing1weld
				end			
				if a:FindFirstChild(char.Name.."'s stuff").rwing2.Value ~= rwing2weld then
					a:FindFirstChild(char.Name.."'s stuff").rwing2.Value = rwing2weld
				end			
				if a:FindFirstChild(char.Name.."'s stuff").rwing3.Value ~= rwing3weld then
					a:FindFirstChild(char.Name.."'s stuff").rwing3.Value = rwing3weld
				end
				if a:FindFirstChild(char.Name.."'s stuff").core.Value ~= coreweld then
					a:FindFirstChild(char.Name.."'s stuff").core.Value = coreweld
				end					
				a:FindFirstChild(char.Name.."'s stuff").vis.Value = visualiser
			end
		end
	end
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		if v.Character then
			local a = v.PlayerGui
			if not a:FindFirstChild(char.Name.."'s stuff") then
				local wengrep = script.Replication.yes:Clone()
				wengrep.Parent = a
				wengrep.Disabled = false
				wengrep.Name = char.Name.."'s stuff"
			end
		end	
	end
	--feetplanting code
	local ForwardDirection = Hum.MoveDirection*root.CFrame.lookVector
	local SideDirection = Hum.MoveDirection*root.CFrame.rightVector
	
	local Vec = {
		X=SideDirection.X+SideDirection.Z,
		Z=ForwardDirection.X+ForwardDirection.Z
	};
	local Divide = 1
	if(Vec.Z<0)then
		Divide=math.clamp(-(1.25*Vec.Z),1,2)
	end
	Vec.Z = Vec.Z/Divide
	Vec.X = Vec.X/Divide
	
	--particle color
	emit2.Color = ColorSequence.new(MainColor)
	
	speen = speen + speed
	local torvel=(RootPart.Velocity*Vector3.new(1,0,1)).magnitude
	local velderp=RootPart.Velocity.y
	hitfloor,posfloor=rayCast(RootPart.Position,(CFrame.new(RootPart.Position,RootPart.Position - Vector3.new(0,1,0))).lookVector,4,char)
	--billboard stuf
	if glitch == "DISORIENTATION" then
		BGU.main.Rotation = math.random(-10,10)
		for i,v in pairs(BGU.main.visbars:GetChildren()) do
			if v.Name == "bottomleft" or v.Name == "bottomright" then
				v.Size = UDim2.fromScale(0.02,visualiser/math.random(2000,3000))
			else
				v.Size = UDim2.fromScale(0.02,-visualiser/math.random(2000,3000))
			end
		end
	else
