				LH.C0=clerp(LH.C0,cf(-1,-1 - 0.05 * math.cos(sine / 32),0)*angles(math.rad(-15),math.rad(-90),0)*angles(math.rad(-2),math.rad(0 - 1 * math.cos(sine / 56)),math.rad(0 + 2 * math.cos(sine / 32))),.1)
				RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,0 + 0.02 * math.cos(sine / 32),1.5)*angles(math.rad((Vec.Z)*40 - 2 * math.cos(sine / 32)),math.rad((-Vec.X)*20),math.rad(0 + 0 * math.cos(sine / 42))),.1)
				Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad((-Vec.Z)*30 - 2 * math.cos(sine / 37)),math.rad((-Vec.X)*1 + 2 * math.cos(sine / 58)),math.rad((Vec.X)*30 + 1 * math.cos(sine / 53))),.1)
				RW.C0=clerp(RW.C0,cf(1.5,0.5 + 0.025 * math.cos(sine / 45),0)*angles(math.rad(0),math.rad(0 - 2 * math.cos(sine / 58)),math.rad(20)),.1)
				LW.C0=clerp(LW.C0,cf(-1.5,0.5 + 0.025 * math.cos(sine / 45),0)*angles(math.rad(0),math.rad(0 - 3 * math.cos(sine / 59)),math.rad(-20)),.1)
			else		
				Anim="Run"
				RH.C0=clerp(RH.C0,cf(1,-1 - 0.4 * math.sin(sine / 6),-0.1 - 0.5 * math.cos(sine/6))*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-Vec.X*25 * math.cos(sine / 6)),math.rad(0-(Vec.X)*5 * math.cos(sine / 6)),math.rad((Vec.Z)*60 * math.cos(sine / 6))),.1)
				LH.C0=clerp(LH.C0,cf(-1,-1 + 0.4 * math.sin(sine / 6),-0.1 + 0.5 * math.cos(sine/6))*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-Vec.X*25 * math.cos(sine / 6)),math.rad(0-(Vec.X)*5 * math.cos(sine / 6)),math.rad((Vec.Z)*60 * math.cos(sine / 6))),.1)
				RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,-0.05,-0.05 - 0.05 * math.cos(sine / 3))*angles(math.rad((Vec.Z)*20 + 6 * math.cos(sine / 3)),math.rad((-Vec.X)*5 + root.RotVelocity.Y/1.5),math.rad(0 - root.RotVelocity.Y - 5 * math.cos(sine / 3))),.1)
				Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(5+(-Vec.Z)*10 - 3 * math.cos(sine / 3)),math.rad((-Vec.X)*1 + root.RotVelocity.Y/1.5),math.rad((Vec.X)*15 - hed.RotVelocity.Y*1.5 + 5 * math.cos(sine / 8))),.1)
				RW.C0=clerp(RW.C0,cf(1.5,0.5,0 + 0.25 * math.cos(sine / 6))*angles(math.rad((-Vec.Z)*80 * math.cos(sine / 6)),math.rad(0),math.rad((-Vec.X)*10 * math.cos(sine / 3))),.1)
				LW.C0=clerp(LW.C0,cf(-1.5,0.5,0 - 0.25 * math.cos(sine / 6))*angles(math.rad((Vec.Z)*80 * math.cos(sine / 6)),math.rad(0),math.rad((-Vec.X)*10 * math.cos(sine / 3))),.1)	
			end		
		end
	end
end
