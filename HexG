		partecle.RotSpeed = NumberRange.new(-100,100)
		partecle.Speed = NumberRange.new(400,500)
		partecle.SpreadAngle = Vector2.new(-180, 180)
		partecle.Rate = 10000000
		partecle.Drag = 1
		table.insert(particles,#particles+1,partecle)
	end
	for i= 1,20 do
		local a = Torso.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
		tweenmesh("Slash",a,MainColor,Vector3.new(0,0.1,0),0,"Neon",0,{Transparency = 1, CFrame = a*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180))), Size = Vector3.new(math.random(100,500),0.1,math.random(100,500))},3,"Quint","Out",0,false)
		tweenmesh("Slash",a,SecondColor,Vector3.new(0,0.1,0),0,"ForceField",0,{Transparency = 1, CFrame = a*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180))), Size = Vector3.new(math.random(100,500),0.1,math.random(100,500))},3,"Quint","Out",0,false)
	end
	attack = false
	coroutine.resume(coroutine.create(function()
		swait(25)
		partecle.Enabled = false
		for i,v in pairs(particles) do v.Enabled = false end
		swait(200)
		for i,v in pairs(particles) do v:Destroy() end
		partecle:Destroy()
	end))
end
--shake
function CamShake(intensity,length,style)
	if style == "global" then
		for i,v in pairs(workspace:GetChildren()) do
			if v:FindFirstChildWhichIsA("Humanoid") then
				local foreach = script.Shakescript:Clone()
				foreach.Parent = v
				foreach.Disabled = false
				foreach.Camshake:FireAllClients(intensity,length)
				game:GetService("Debris"):AddItem(foreach,5)
			end
		end
	elseif style == "local" then	
		local foreach = script.Shakescript:Clone()
		foreach.Parent = char
		foreach.Disabled = false
		foreach.Camshake:FireAllClients(intensity,length)
		game:GetService("Debris"):AddItem(foreach,5)
	end
end
--attacs
function ecboom()
	Hum.WalkSpeed = 0
	local sparkgui = Instance.new("BillboardGui",Torso)
	sparkgui.Size = UDim2.new(50,0,50,0)
	sparkgui.LightInfluence = 0
	sparkgui.AlwaysOnTop = true
	local spark = Instance.new("ImageLabel",sparkgui)
	spark.Image = "rbxassetid://5369718523"	
	spark.Size = UDim2.new(1,0,1,0)
	spark.BackgroundTransparency = 1
	spark.ImageColor3 = MainColor
	spark.BackgroundTransparency = 1
	coroutine.resume(coroutine.create(function()
		for i=1,100 do
			sparkgui.Size = UDim2.new(50-i,0,50-i,0)
			spark.ImageTransparency = i/100
			spark.Rotation = spark.Rotation + 5
			swait()
		end
		sparkgui:Destroy()
	end))
	attackanim = true
	for i=1,100 do
		swait()
		RootJoint.C0=clerp(RootJoint.C0,cf(0,0,0)*angles(math.rad(0.2),math.rad(-4.2),math.rad(-0.8))*RootCF,0.1)
		Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(-10),math.rad(4.2),math.rad(2.8)),0.1)
		RW.C0=clerp(RW.C0,cf(1.3,0.6,-0.4)*angles(math.rad(126),math.rad(-23.6),math.rad(-48.6)),0.1)
		LW.C0=clerp(LW.C0,cf(-1.3,0.8,-0.3)*angles(math.rad(149.9),math.rad(-2),math.rad(61.2)),0.1)
	end
	for i=1,250 do
		swait()
		RootJoint.C0=clerp(RootJoint.C0,cf(0,0,0)*angles(math.rad(0.2),math.rad(-4.2),math.rad(-0.8))*RootCF,0.1)
		Torso.Neck.C0=clerp(Torso.Neck.C0,cf(0,0,-0.3)*angles(math.rad(10.9),math.rad(4.7),math.rad(1.7))*necko,0.1)
		RW.C0=clerp(RW.C0,cf(1,0.8,0.4)*angles(math.rad(50.9),math.rad(-21.8),math.rad(89.2)),0.1)
		LW.C0=clerp(LW.C0,cf(-1.2,1,0)*angles(math.rad(-27.4),math.rad(26.2),math.rad(-49.7)),0.1)
	end
	attackanim = false
	Hum.WalkSpeed = storewalkspeed
end
function zap()
	attack = true
	attackanim = true
	Hum.WalkSpeed = 10
	local bodypos = Instance.new("BodyPosition",Torso)
	bodypos.MaxForce = Vector3.new(10000000,10000000,10000000)
	bodypos.Position = (root.CFrame * CFrame.new(0,50,0)).p
	Sound("Sound",1295446488,Torso,5,1,0)
	Sound("Sound",289315275,Torso,10,1,0)
	for i=20,1000,20 do
		RootJoint.C0 = clerp(RootJoint.C0,cf(0,0,0)*angles(math.rad(-i),math.rad(0.8),math.rad(0.6))*RootCF,1)
		LH.C0 = clerp(LH.C0,cf(-0.9,-0.5,-0.5)*angles(math.rad(-17.7),math.rad(0.3),math.rad(-5.1))*angles(math.rad(0),math.rad(-90),math.rad(0)),.1)
		RH.C0 = clerp(RH.C0,cf(0.9,-0.5,-0.5)*angles(math.rad(-10.5),math.rad(2.7),math.rad(7.2))*angles(math.rad(0),math.rad(90),math.rad(0)),.1)
		LW.C0 = clerp(LW.C0,cf(-1.4,0.5,-0.4)*angles(math.rad(106.3),math.rad(12.2),math.rad(44)),.1)
		RW.C0 = clerp(RW.C0,cf(1.2,0.5,-0.4)*angles(math.rad(102.7),math.rad(-22.5),math.rad(-58)),.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,cf(0.1,0.4,0.8)*angles(math.rad(-55.5),math.rad(-2),math.rad(4))*necko,.1)
		swait()
	end
	local place = CFrame.new(0,2,0)*rarm.CFrame
	local ray = Ray.new(place.p,(mousepos.p-place.p).unit*1000)
	local hit,pos,normal,material = workspace:FindPartOnRay(ray,char,false,true)
	local gap = (place.p - pos).magnitude
	local Distance = (root.CFrame.p - mousepos.p).magnitude
	local Difference = root.CFrame.Y - mousepos.p.Y
	local beam = Instance.new("Part",char)
	beam.Shape = "Cylinder"
	beam.Size = Vector3.new(gap, 20 + 3 * math.cos(sine/2),20 + 3 * math.cos(sine/2))
	beam.Color = wings[1].Color.Color
	beam.CFrame = CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0)
	beam.Material = "Neon"
	beam.Transparency = 0
	beam.Anchored = true
	beam.CanCollide = false
	local beam2 = Instance.new("Part",char)
	beam2.Shape = "Cylinder"
	beam2.Size = Vector3.new(gap, 25 + 3 * math.cos(sine/2),25 + 3 * math.cos(sine/2))
	beam2.Color = Color3.new(1,1,1)
	beam2.CFrame = CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0)
	beam2.Material = "Neon"
	beam2.Transparency = 0.5
	beam2.Anchored = true
	beam2.CanCollide = false
	local hitpart = Instance.new("Part",char)
	hitpart.Shape = "Ball"
	hitpart.Size = Vector3.new(5,5,5)
	hitpart.CFrame = CFrame.new(pos)
	hitpart.Transparency = 1
	hitpart.Anchored = true
	hitpart.CanCollide = false
	local particles = {}
	for i=1,3 do
		local partecle = Instance.new("ParticleEmitter",hitpart)
		partecle.LightEmission = 9999
		partecle.LightInfluence = 0
		partecle.EmissionDirection = "Top"
		partecle.Enabled = true
		partecle.Texture = "http://www.roblox.com/asset/?id=1084970835"
		partecle.Color = ColorSequence.new(MainColor)
		partecle.Transparency = NumberSequence.new(0,1)
		partecle.Size = NumberSequence.new(15,0)
		partecle.Acceleration = Vector3.new(0,1,0)
		partecle.Lifetime = NumberRange.new(0.3,0.7)
		partecle.Rotation = NumberRange.new(-180,180)
		partecle.RotSpeed = NumberRange.new(-100,100)
		partecle.Speed = NumberRange.new(400,500)
		partecle.SpreadAngle = Vector2.new(-180, 180)
		partecle.Rate = 10000000
		partecle.Drag = 1
		table.insert(particles,#particles+1,partecle)
	end
	Sound("Sound",1208650519,Torso,10,1,0)
	Sound("Sound",164881093,Torso,10,1,0)
	Sound("Sound",429123896,Torso,10,1,0)
	for i=1,300 do
		place = CFrame.new(0,2,0)*rarm.CFrame
		ray = Ray.new(place.p,(mousepos.p-place.p).unit*1000)
		hit,pos,normal,material = workspace:FindPartOnRay(ray,char,false,true)
		gap = (place.p - pos).magnitude
		Distance = (root.CFrame.p - mousepos.p).magnitude
		Difference = root.CFrame.Y - mousepos.p.Y
		RootJoint.C0 = clerp(RootJoint.C0,cf(0,0,0)*angles(math.rad(0),math.rad(79),math.rad(0))*RootCF*CFrame.Angles((math.atan(Difference / Distance) * 0.5), 0, (((hed.Position - mousepos.p).Unit):Cross(hed.CFrame.LookVector)).Y * 5),.1)
		LH.C0 = clerp(LH.C0,cf(-1,-0.9,-0.1)*angles(math.rad(-17.7),math.rad(0.3),math.rad(-5.1))*angles(math.rad(0),math.rad(-90),math.rad(0)),.1)
		RH.C0 = clerp(RH.C0,cf(1,-1,0)*angles(math.rad(-10.5),math.rad(2.7),math.rad(7.2))*angles(math.rad(0),math.rad(90),math.rad(0)),.1)
		LW.C0 = clerp(LW.C0,cf(-1.5,0.5,0.1)*angles(math.rad(11.8),math.rad(5.9),math.rad(-4.5)),.1)
		RW.C0 = clerp(RW.C0,cf(1.1,0.5,0.5)*angles(math.rad(87.6),math.rad(-8.9),math.rad(82.2)),.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,cf(0,0,-0.1)*angles(math.rad(3.7),math.rad(-76.8),math.rad(0))*necko,.1)
		--tweeneff("Cylinder",CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0),wings[1].Color.Color,Vector3.new(gap, 20 + 1 * math.cos(sine/2),20 + 1 * math.cos(sine/2)),0,"Neon",0,{Transparency = 1, Size = Vector3.new(gap,0,0)},0.1,"Linear","Out",0,false)
		--tweeneff("Cylinder",CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0),wings[2].Color.Color,Vector3.new(gap, 22 + 1 * math.cos(sine/2),22 + 1 * math.cos(sine/2)),0,"Neon",0.5,{Transparency = 1, Size = Vector3.new(gap,0,0)},0.1,"Linear","Out",0,false)
		for i,v in pairs(particles) do
			v.Color = ColorSequence.new(wings[1].Color.Color)
		end
		beam.Color = wings[1].Color.Color
		beam2.Color = Color3.new(1,1,1)
		beam2.Size = Vector3.new(gap, 25 + 3 * math.cos(sine/2),25 + 3 * math.cos(sine/2))
		beam2.CFrame = CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0)
		beam.Size = Vector3.new(gap, 20 + 3 * math.cos(sine/2),20 + 3 * math.cos(sine/2))
		beam.CFrame = CFrame.new(place.p, pos) * CFrame.new(0, 0, -gap/2) * CFrame.Angles(0,math.rad(90),0)
		tweeneff("Sphere",CFrame.new(pos),wings[1].Color.Color,Vector3.new(45,45,45),0,"Neon",0,{Transparency = 1, Size = Vector3.new(0,0,0)},0.25,"Linear","Out",0,false)
		tweeneff("Sphere",CFrame.new(pos),Color3.new(1,1,1),Vector3.new(47,47,47),0,"Neon",0,{Transparency = 1, Size = Vector3.new(0,0,0)},0.25,"Linear","Out",0,false)
		hitpart.CFrame = CFrame.new(pos)
		for i=1,5 do
			local a = CFrame.new(pos)*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
			tweeneff("Sphere",a,wings[1].Color.Color,Vector3.new(6,20,6),0,"Neon",0,{Transparency = 1, Size = Vector3.new(0,30,0), CFrame = a*CFrame.new(0,math.random(160,200),0)},0.25,"Linear","Out",0,false)
		end
		magidmg(pos,20,1000,2000,"Normal")
		swait()
	end
	for i,v in pairs(particles) do v:Destroy() end
	beam:Destroy()
	beam2:Destroy()
	bodypos:Destroy()
	attackanim = false
	attack = false
end
function bosschat(texttable)
	for i,v in pairs(game.Players:GetPlayers()) do
