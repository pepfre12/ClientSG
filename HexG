			MainColor = Color3.fromRGB(120,0,255)
			SecondColor = Color3.fromRGB(0,0,0)
			boom()
			Side = 1
			reloadwing("Wing")
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "ECCLESIASTIC",
				speed = 50,
				jumppower = 50,
				font = "Gotham",
				theme = 2606277947,
				songinfo = "Kurokotei - Chronostatis",
				pitch = 1
			})
			tweenrecolor({
				tweeninfo = TweenInfo.new(2,Enum.EasingStyle.Linear,Enum.EasingDirection.InOut,math.huge,true,0),
				maincolor = Color3.fromRGB(0,255,120),
				secondcolor = Color3.fromRGB(0,0,0)
			})
			glitch = "ECCLESIASTIC"
		elseif key == "e" and glitch ~= "DESIRE" and attack == false then
			MainColor = Color3.fromRGB(255, 152, 220)
			SecondColor = Color3.fromRGB(175, 221, 255)
			boom()
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "DESIRE",
				speed = 50,
				jumppower = 50,
				font = "Cartoon",
				theme = 4969887630,
				songinfo = "Meramipop - Secret Desire (Dyes Remix)",
				pitch = 1
			})
			tweenrecolor({
				tweeninfo = TweenInfo.new(0.5,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut,math.huge,true,0),
				maincolor = Color3.fromRGB(232, 186, 200),
				secondcolor = Color3.fromRGB(255, 255, 204)
			})
			glitch = "DESIRE"
		elseif key == "r" and glitch ~= "DESPAIR" and attack == false then
			MainColor = Color3.fromRGB(0, 0, 0)
			SecondColor = Color3.fromRGB(50, 50, 50)
			boom()
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "DESPAIR",
				speed = 40,
				jumppower = 50,
				font = "Garamond",
				theme = 4407184482,
				songinfo = "Kurokotei - Fracture Temporelle",
				pitch = 1
			})
			tweenrecolor({
				tweeninfo = TweenInfo.new(0.25,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut,math.huge,true,0),
				maincolor = Color3.fromRGB(0, 0, 0),
				secondcolor = Color3.fromRGB(255, 255, 255)
			})
			glitch = "DESPAIR"
		end
	end
end)
