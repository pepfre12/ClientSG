Player.Chatted:Connect(function(msg)
	print("player said "..string.sub(msg,1,8))
	if string.sub(msg,1,8) == "/e play " then
		visualiserid = string.sub(msg,9)
		print("player played "..string.sub(msg,9))
	end
end)
-- Start of Loops --
spawn(function()
	while true do
		swait()
		sine = sine + change
	end
end)
