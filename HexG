function APOTHEOSIS()
	attack = true
	attackanim = true
	Hum.WalkSpeed = 10
	local bodypos = Instance.new("BodyPosition",Torso)
	bodypos.MaxForce = Vector3.new(10000000,10000000,10000000)
	bodypos.Position = (root.CFrame * CFrame.new(0,200,0)).p
	Sound("Sound",1295446488,Torso,5,1,0)
	Sound("Sound",289315275,Torso,10,1,0)
	for i=20,1000,20 do
		RootJoint.C0 = clerp(RootJoint.C0,cf(0,0,0)*angles(math.rad(-i),math.rad(0.8),math.rad(0.6))*RootCF,1)
		LH.C0 = clerp(LH.C0,cf(-0.9,-0.5,-0.5)*angles(math.rad(-17.7),math.rad(0.3),math.rad(-5.1))*angles(math.rad(0),math.rad(-90),math.rad(0)),.1)
		RH.C0 = clerp(RH.C0,cf(0.9,-0.5,-0.5)*angles(math.rad(-10.5),math.rad(2.7),math.rad(7.2))*angles(math.rad(0),math.rad(90),math.rad(0)),.1)
		LW.C0 = clerp(LW.C0,cf(-1.4,0.5,-0.4)*angles(math.rad(106.3),math.rad(12.2),math.rad(44)),.1)
		RW.C0 = clerp(RW.C0,cf(1.2,0.5,-0.4)*angles(math.rad(102.7),math.rad(-22.5),math.rad(-58)),.1)
		Torso.Neck.C0 = clerp(Torso.Neck.C0,cf(0.1,0.4,0.8)*angles(math.rad(-55.5),math.rad(-2),math.rad(4))*necko,.1)
		swait()
	end
	modechange({
		maincolor = Color3.new(0,0,0),
		secondcolor = Color3.new(0.3,0.3,0.3),
		wingmat = "Neon",
		secondwingmat = "Neon",
