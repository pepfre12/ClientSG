	Sound("Sound",763716870,Torso.Parent,10,0.3,0)
	Sound("Sound",1208650519,Torso,10,1,0)
	Sound("Sound",164881093,Torso,10,1,0)
	Sound("Sound",429123896,Torso,10,1,0)
	glitch = nil
	coroutine.resume(coroutine.create(function()
		modechange({
			maincolor = Color3.new(1,1,1),
			secondcolor = Color3.new(1,1,1),
			wingmat = "Neon",
			secondwingmat = "Neon",
			name = "noob",
			speed = 75,
			jumppower = 75,
			font = "Fantasy",
			theme = 586732256,
			songinfo = "lol you died",
			pitch = 1
		})
		tweenrecolor({
			tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false),
			maincolor = Color3.fromRGB(0,0,0),
			secondcolor = Color3.fromRGB(0,0,0),
		})
		wait(1)
		BGU:Destroy()
	end))
	coroutine.resume(coroutine.create(function()
		tweenserv:Create(groovy,TweenInfo.new(0.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false),{Pitch = 0})
		wait(0.5)
		groovy:Destroy()
	end))
	for i,v in pairs({rarm,larm,rleg,lleg,Torso,hed}) do
		coroutine.resume(coroutine.create(function()
			v.Transparency = 1
			for i=1,5 do
				local partecle = Instance.new("ParticleEmitter",v)
				partecle.LightEmission = 9999
				partecle.LightInfluence = 0
				partecle.EmissionDirection = "Top"
				partecle.Enabled = true
				partecle.Texture = "http://www.roblox.com/asset/?id=5122583722"
				partecle.Color = ColorSequence.new(MainColor)
				partecle.Transparency = NumberSequence.new(0,1)
				partecle.Size = NumberSequence.new(0.5,0)
				partecle.Acceleration = Vector3.new(0,1,0)
				partecle.Lifetime = NumberRange.new(0.3,0.7)
				partecle.Rotation = NumberRange.new(-180,180)
				partecle.RotSpeed = NumberRange.new(-100,100)
				partecle.Speed = NumberRange.new(30,50)
				partecle.SpreadAngle = Vector2.new(-180, 180)
				partecle.Rate = 10000000
				partecle.Drag = 1
				swait(5)
				partecle.Enabled = false
			end
		end))
	end
end)
