--heavily delayed loop so no lag lol
coroutine.resume(coroutine.create(function()
	while true do
		swait(2)
		if glitch == "CHROMATIC" then
			MainColor = Color3.fromHSV(sine/60-math.floor(sine/60),0.5,1)
			SecondColor = Color3.fromHSV(sine/60-math.floor(sine/60),1,1)
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "CHROMATIC",
				speed = 500,
				jumppower = 50,
				font = "GothamBold",
				theme = 4926087847,
				songinfo = "Ebimayo - Goodfortune",
				pitch = 1
			})
		elseif glitch == "SERENITY" then
			MainColor = Color3.fromHSV(0.25+0.25*math.sin(math.tan(math.cos(sine/56))),0.45,1)
			SecondColor = Color3.fromRGB(255, 255, 255)
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "SERENITY",
				speed = 125,
				jumppower = 75,
				font = "SourceSansLight",
				theme = 1095119170,
				songinfo = "Bejeweled 2 - Final Destination",
				pitch = 1
			})
		elseif glitch == "DISORIENTATION" then
			local rnd = math.random(1,3)
			if rnd == 1 then
				MainColor = Color3.new(1, 0, 0)
			elseif rnd == 2 then
				MainColor = Color3.new(0, 1, 0)
			elseif rnd == 3 then
				MainColor = Color3.new(0, 0, 1)
			end
			SecondColor = Color3.new(0, 0, 0)
			local fonts = {"Legacy","Arial","ArialBold","SourceSans","SourceSansBold","SourceSansSemibold","SourceSansLight","SourceSansItalic","Bodoni","Garamond","Cartoon","Code","Highway","SciFi","Arcade","Fantasy","Antique","Gotham","GothamSemibold","GothamBold","GothamBlack"}
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "DISORIENTATION",
				speed = 1000,
				jumppower = 75,
				font = fonts[math.random(1,#fonts)],
				theme = 2032024538,
				songinfo = "Frums - Theyaremanycolors",
				pitch = 1
			})
			glitch = "DISORIENTATION"
		elseif glitch == "SYMPHONIC" then
			local visualisercolor = visualiser/500
			--print(visualiser/750)
			if visualisercolor > 1 then
				MainColor = Color3.fromHSV(sine/360-math.floor(sine/360),0.5,1)
				SecondColor = Color3.new(1,1,1)
			else
				MainColor = Color3.fromHSV(sine/360-math.floor(sine/360),0.5,visualisercolor)
				SecondColor = Color3.new(visualisercolor,visualisercolor,visualisercolor)
			end
			modechange({
				maincolor = MainColor,
				secondcolor = SecondColor,
				wingmat = "Neon",
				secondwingmat = "Neon",
				name = "SYMPHONIC",
				speed = 1000,
				jumppower = 75,
				font = "Arial",
				theme = visualiserid,
				songinfo = game:GetService("MarketplaceService"):GetProductInfo(visualiserid).Name,
				pitch = 1
			})
			glitch = "SYMPHONIC"
		elseif glitch == "APOTHEOSIS" then
			apotheosisform()
		end
		local function recolor(obj)
			for i,v in pairs(obj:GetDescendants()) do
				if v:IsA("ParticleEmitter") or v:IsA("Trail") then 
					if obj.Name == "Color" then
						v.Color = ColorSequence.new(wings[2].Color.Color)
					elseif obj.Name == "Frame" then
						v.Color = ColorSequence.new(wings[1].Color.Color)
					end
				end
			end
		end
		for i,v in pairs(wings) do
			recolor(v)
		end
		--[[for i,v in pairs(BGU.main:GetDescendants()) do
