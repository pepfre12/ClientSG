local TweenModeColor = {}
function tweenrecolor(InfoTable)
	--aidenferr fixed the tweens
	--for i,v in pairs(TweenModeColor)do v:Cancel()end TweenModeColor = {}
	local tweeninfo = InfoTable.tweeninfo
	local maincolor = InfoTable.maincolor
	local secondcolor = InfoTable.secondcolor
	local tweenserv = game:GetService("TweenService")
	local mode = InfoTable.mode
	
	local tweens = {{tweenserv:Create(BGU.main.Flare,tweeninfo,{ImageColor3 = maincolor}),
	tweenserv:Create(BGU.main.biggerhex,tweeninfo,{ImageColor3 = secondcolor}),
	tweenserv:Create(BGU.main.hex,tweeninfo,{ImageColor3 = maincolor}),
	tweenserv:Create(BGU.main.biggerleftline,tweeninfo,{ImageColor3 = secondcolor}),
	tweenserv:Create(BGU.main.biggerrightline,tweeninfo,{ImageColor3 = secondcolor}),
	tweenserv:Create(BGU.main.leftline,tweeninfo,{ImageColor3 = maincolor}),
	tweenserv:Create(BGU.main.rightline,tweeninfo,{ImageColor3 = maincolor}),
	tweenserv:Create(BGU.main.textthing,tweeninfo,{TextColor3 = maincolor, TextStrokeColor3 = secondcolor})}}
	for _,obj in pairs(wings) do
		table.insert(tweens,#tweens+1,{
			tweenserv:Create(obj.Color,tweeninfo,{Color = maincolor}),
			tweenserv:Create(obj.Frame,tweeninfo,{Color = secondcolor})})
	end
	for i,v in pairs(BGU.main.visbars:GetChildren()) do
		table.insert(tweens,#tweens+1,{
			tweenserv:Create(v,tweeninfo,{BackgroundColor3 = maincolor, BorderColor3 = secondcolor})})
	end
	local guimain = SG.main
	table.insert(tweens,#tweens+1,{
		tweenserv:Create(guimain.hexagonthing,tweeninfo,{ImageColor3 = maincolor}),
		tweenserv:Create(guimain.anotherhexagonthing,tweeninfo,{ImageColor3 = secondcolor}),
		tweenserv:Create(guimain.slidething,tweeninfo,{ImageColor3 = maincolor}),
		tweenserv:Create(guimain.anotherslidething,tweeninfo,{ImageColor3 = secondcolor}),
		tweenserv:Create(guimain.glow,tweeninfo,{ImageColor3 = maincolor}),
		tweenserv:Create(guimain.name,tweeninfo,{TextColor3 = maincolor}),
		tweenserv:Create(guimain.name,tweeninfo,{TextStrokeColor3 = secondcolor}),
		tweenserv:Create(guimain.textinfo,tweeninfo,{TextColor3 = maincolor}),
		tweenserv:Create(guimain.textinfo,tweeninfo,{TextStrokeColor3 = secondcolor}),
		tweenserv:Create(guimain["credit text thing"],tweeninfo,{BackgroundColor3 = maincolor}),
		tweenserv:Create(guimain["credit text thing"],tweeninfo,{BorderColor3 = secondcolor}),
		tweenserv:Create(guimain["credit text thing"].text,tweeninfo,{TextColor3 = maincolor}),
		tweenserv:Create(guimain["credit text thing"].text,tweeninfo,{TextStrokeColor3 = secondcolor}),
		tweenserv:Create(guimain.visbars,tweeninfo,{BackgroundColor3 = maincolor}),
		tweenserv:Create(guimain.visbars,tweeninfo,{BorderColor3 = secondcolor})})
	for _,v in pairs(guimain.visbars:GetChildren()) do
		if not v:IsA("UIGradient") then
			table.insert(tweens,#tweens+1,{
				tweenserv:Create(v,tweeninfo,{BackgroundColor3 = maincolor}),
				tweenserv:Create(v,tweeninfo,{BorderColor3 = secondcolor})})
		end
	end
	table.foreach(tweens,function(_,b)table.foreach(b,function(_,v)table.insert(TweenModeColor,#TweenModeColor+1,v)v:Play()end)end)
end
function modechange(InfoTable)
	for i,v in pairs(TweenModeColor)do v:Cancel()end TweenModeColor = {}
	local maincolor = InfoTable.maincolor
	local secondcolor = InfoTable.secondcolor
	local wingmat = InfoTable.wingmat
	local secondwingmat = InfoTable.secondwingmat
	local name = InfoTable.name
	local sped = InfoTable.speed
	local jemp = InfoTable.jumppower
	local font = InfoTable.font
	local ID = InfoTable.theme
	local songinfo = InfoTable.songinfo
	local PITCH = InfoTable.pitch
	local function recolor(obj)
		for i,v in pairs(obj:GetChildren()) do
			if v:IsA("MeshPart") then
				if v.Name == "Color" then
					if glitch == "DISORIENTATION" then
						local rnd = math.random(1,3)
						if rnd == 1 then
							v.Color = Color3.new(1,0,0)
						elseif rnd == 2 then
							v.Color = Color3.new(0,1,0)
						elseif rnd == 3 then
							v.Color = Color3.new(0,0,1)
						end
					else
						v.Color = maincolor
					end
					v.Material = wingmat
				elseif v.Name == "Frame" then
					if glitch == "DISORIENTATION" then
						local rnd = math.random(1,3)
						if rnd == 1 then
							v.Color = Color3.new(1,0,0)
						elseif rnd == 2 then
							v.Color = Color3.new(0,1,0)
						elseif rnd == 3 then
							v.Color = Color3.new(0,0,1)
						end
					else
						v.Color = secondcolor
					end
					v.Material = secondwingmat
				end
			end
		end
	end
	for i,v in pairs(wings) do
		recolor(v)
	end
	BGU.main.Flare.ImageColor3 = maincolor
	BGU.main.biggerhex.ImageColor3 = secondcolor
	BGU.main.hex.ImageColor3 = maincolor
	BGU.main.biggerleftline.ImageColor3 = secondcolor
	BGU.main.biggerrightline.ImageColor3 = secondcolor
	BGU.main.leftline.ImageColor3 = maincolor
	BGU.main.rightline.ImageColor3 = maincolor
	BGU.main.textthing.TextColor3 = maincolor
	BGU.main.textthing.TextStrokeColor3 = secondcolor
	BGU.main.textthing.Font = font
	BGU.main.textthing.Text = name
	for _,v in pairs(BGU.main.visbars:GetChildren()) do
		v.BackgroundColor3 = maincolor
		v.BorderColor3 = secondcolor
	end
	Hum.WalkSpeed = sped
	Hum.JumpPower = jemp
	storewalkspeed = sped
	groovy.SoundId = "rbxassetid://"..ID
	groovy.Pitch = PITCH
	local guimain = SG.main
	guimain.hexagonthing.ImageColor3 = maincolor
	guimain.anotherhexagonthing.ImageColor3 = secondcolor
	guimain.slidething.ImageColor3 = maincolor
	guimain.anotherslidething.ImageColor3 = secondcolor
	guimain.glow.ImageColor3 = maincolor
	guimain.name.Text = name
	guimain.name.TextColor3 = maincolor
	guimain.name.TextStrokeColor3 = secondcolor
	guimain.name.Font = font
	guimain.textinfo.Text = songinfo
	guimain.textinfo.TextColor3 = maincolor
	guimain.textinfo.TextStrokeColor3 = secondcolor
	guimain.textinfo.Font = font
	guimain["credit text thing"].BackgroundColor3 = maincolor
	guimain["credit text thing"].BorderColor3 = secondcolor
	guimain["credit text thing"].text.TextColor3 = maincolor
	guimain["credit text thing"].text.TextStrokeColor3 = secondcolor
	guimain["credit text thing"].text.Font = font
	guimain.visbars.BackgroundColor3 = maincolor
	guimain.visbars.BorderColor3 = secondcolor
	for _,v in pairs(guimain.visbars:GetChildren()) do
		if not v:IsA("UIGradient") then
			v.BackgroundColor3 = maincolor
			v.BorderColor3 = secondcolor
		end
	end
		--[[pcall(function()
		twen1:Cancel()
		twen2:Cancel()
		twen3:Cancel()
		twen4:Cancel()
		twen5:Cancel()
		twen6:Cancel()
		twen7:Cancel()
		twen8:Cancel()
