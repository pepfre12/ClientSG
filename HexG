		local bosgui = Instance.new("ScreenGui",plrgui)
		local fram = Instance.new("Frame",{Parent = bosgui, Size = UDim2.new(0, 0,0.1), BackgroundTransparency = 0, BackgroundColor3 = MainColor, BorderColor3 = SecondColor})
		
	end
end
