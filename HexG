--effec replication
function tweeneff(shape,pos,color,size,welded,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev)
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		if v.PlayerGui then
			local a = v.PlayerGui
			if a:FindFirstChild(char.Name.."'s stuff") then
				a:FindFirstChild(char.Name.."'s stuff").effec:FireClient(v,shape,pos,color,size,welded,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev,"tweeneff")
			end
		end
	end
end
function tweenmesh(shape,pos,color,size,hinged,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev)
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		if v.PlayerGui then
			local a = v.PlayerGui
			if a:FindFirstChild(char.Name.."'s stuff") then
				a:FindFirstChild(char.Name.."'s stuff").effec:FireClient(v,shape,pos,color,size,hinged,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev,"tweenmesh")
			end
		end
	end
end
function specialeff(shape,pos,color,size,hinged,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev)
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		if v.PlayerGui then
			local a = v.PlayerGui
			if a:FindFirstChild(char.Name.."'s stuff") then
				a:FindFirstChild(char.Name.."'s stuff").effec:FireClient(v,shape,pos,color,size,hinged,material,trans,prop,tweenlength,tweenstyle,tweendirection,tweenrep,tweenrev,"specialeff")
			end
		end
	end
end
-- Main Damage Function --
function DamageFunc(Humanoid,MinDmg,MaxDmg,Type)
	local Damage = math.random(MinDmg,MaxDmg)
	-- Failsafe --
	if Humanoid == nil then
		return
	end
	
	--some compatability with my script--
	local defenseboost = false
	local defenseboosted = Humanoid.Parent:findFirstChild("defenseboost")
	if defenseboosted ~= nil and defenseboosted.className == "BoolValue" and defenseboosted.Value == true then
		Damage = Damage/2
		Sound("Sound",240429289,Humanoid.Parent.Torso,7,4,0)
	end
	
	if Humanoid ~= nil then
		--local dmggui = script.dmggui:Clone()
		if Type == "Normal" then
			Humanoid.Health = Humanoid.Health - Damage
			--tex.Text = "-"..Damage
			--tex.TextColor3 = Color3.fromRGB(255,0,0)
		elseif Type == "Admin" then
			local DoDamage = true
			if DoDamage == true then
				Humanoid.Health = Humanoid.Health - Damage
				--tex.Text = "-"..Damage
				--tex.TextColor3 = Color3.fromRGB(255,0,0)
			end
		elseif Type == "Neg" then
			Humanoid.Health = Humanoid.Health + Damage
			--tex.Text = "+"..Damage
			--tex.TextColor3 = Color3.fromRGB(0,255,0)
		elseif Type == "Divide" then
			Humanoid.Health = Humanoid.Health / Damage
		end
		if Humanoid.Health == 0 and not Humanoid:FindFirstChild("Aliven't") then
			Humanoid.DisplayDistanceType = "None"
			local tag = Instance.new("BoolValue",Humanoid)
			tag.Name = "Aliven't"
			tag.Value = true
			Sound("Sound",763716870,Humanoid.Parent,1,0.3,0)
			
			local humparts = {
				Humanoid.Parent.Head,
				Humanoid.Parent.Torso,
				Humanoid.Parent["Right Arm"],
				Humanoid.Parent["Left Arm"],
				Humanoid.Parent["Right Leg"],
				Humanoid.Parent["Left Leg"],
			}
			
			
			for i,v in pairs(humparts) do
				coroutine.resume(coroutine.create(function()
					local bp = Instance.new("BodyPosition", v)
					bp.P = 3000
					bp.D = 1500
					bp.maxForce = Vector3.new(9999999999999, 9999999999999, 9999999999999)
					bp.position = v.Position + Vector3.new(math.random(-4,4),10 + math.random(-4,4),math.random(-4,4))
					v.Material = Enum.Material.Neon
					v.Anchored = false
					v.CanCollide = false
					v.Color = MainColor
					v.Transparency = 1
					local partecle = Instance.new("ParticleEmitter",v)
					partecle.LightEmission = 9999
					partecle.LightInfluence = 0
					partecle.EmissionDirection = "Top"
					partecle.Enabled = true
					partecle.Texture = "http://www.roblox.com/asset/?id=5122583722"
					partecle.Color = ColorSequence.new(MainColor)
					partecle.Transparency = NumberSequence.new(0,1)
					partecle.Size = NumberSequence.new(3,0)
					partecle.Acceleration = Vector3.new(0,1,0)
					partecle.Lifetime = NumberRange.new(0.3,0.7)
					partecle.Rotation = NumberRange.new(-180,180)
					partecle.RotSpeed = NumberRange.new(-100,100)
					partecle.Speed = NumberRange.new(30,50)
					partecle.SpreadAngle = Vector2.new(-180, 180)
					partecle.Rate = 10000000
					partecle.Drag = 1
					swait(10)
					partecle.Enabled = false
					swait(50)
					v:BreakJoints()
				end))
			end
		end
	end
end
-- Magic Damage --
function magidmg(Pos,Range,MinDmg,MaxDmg,Type)
	for i,v in pairs(game.Workspace:GetDescendants()) do
		if v.ClassName == "Humanoid" then
			if v.Parent:FindFirstChild("Torso") and v.Parent ~= script.Parent then
				local dist = (Pos - v.Parent.Torso.Position).Magnitude
				if dist <= Range then
					DamageFunc(v,MinDmg,MaxDmg,Type)	
				end
			end
		end
	end
end

--weng
local lwing1,lwing1weld = nil
local lwing2,lwing2weld = nil
local lwing3,lwing3weld = nil
local rwing1,rwing1weld = nil
local rwing2,rwing2weld = nil
local rwing3,rwing3weld = nil
local core,coreweld = nil
local wings = {}
local motorfolder = Instance.new("Folder",char)
function reloadwing(newwing)
	pcall(function()
		lwing1:Destroy()
		lwing2:Destroy()
		lwing3:Destroy()
		rwing1:Destroy()
		rwing2:Destroy()
		rwing3:Destroy()
		lwing1weld:Destroy()
		lwing2weld:Destroy()
		lwing3weld:Destroy()
		rwing1weld:Destroy()
		rwing2weld:Destroy()
		rwing3weld:Destroy()
		coreweld:Destroy()
		core:Destroy()
	end)
	WingStyle = newwing
	local function createwing(wingtype)
		local wing = script[newwing]:Clone()
		wing.Parent = char
		wing.Handle.Anchored = false
		local wingweld = Instance.new("Motor6D",wing.Handle)
		wingweld.Part0 = Torso
		wingweld.Part1 = wing.Handle
		wing.Name = wingtype
		return wing,wingweld
	end
	core = script.Core:Clone()
	core.Handle.Anchored = false
	core.Parent = char
	core.Name = "Color"
	coreweld = Instance.new("Motor6D",motorfolder)
	coreweld.Part0 = Torso
	coreweld.Part1 = core.Handle
	lwing1,lwing1weld = createwing("Frame")
	lwing2,lwing2weld = createwing("Color")
	lwing3,lwing3weld = createwing("Color")
	rwing1,rwing1weld = createwing("Frame")
	rwing2,rwing2weld = createwing("Color")
	rwing3,rwing3weld = createwing("Color")
	wings = {
		lwing1,
		lwing2,
		lwing3,
		rwing1,
		rwing2,
		rwing3,
		core
	}
end
reloadwing("Wing")
