function salvate()
	if FindTorso(mousepos.p,5) ~= nil then
		attack = true
		local attacked = FindTorso(mousepos.p,5)
		local part = Instance.new("Part",char)
		part.Transparency = 1
		part.Anchored = true
		part.CanCollide = false
		part.CFrame = attacked.CFrame
		part.Size = Vector3.new(5,5,5)
		Instance.new("WeldConstraint",part,{Part0 = attacked, Part1 = part})
		local partic = Instance.new("ParticleEmitter",part)
		partic.Texture = "rbxassetid://1084969997"
		partic.LightEmission = 1
		partic.LightInfluence = 0
		partic.Color = ColorSequence.new(MainColor,SecondColor)
		partic.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(0.3,0.5),NumberSequenceKeypoint.new(1,1)}
		partic.Size = NumberSequence.new{NumberSequenceKeypoint.new(0,30),NumberSequenceKeypoint.new(1,0)}
		partic.LockedToPart = true
		partic.Lifetime = NumberRange.new(0.3)
		partic.Rate = 100
		partic.Rotation = NumberRange.new(-180,180)
		partic.RotSpeed = NumberRange.new(360)
		partic.Speed = NumberRange.new(0)
		partic.SpreadAngle = Vector2.new(0,0)
		local partic2 = Instance.new("ParticleEmitter",part)
		partic2.Texture = "rbxassetid://243728104"
		partic2.LightEmission = 1
		partic2.LightInfluence = 0
		partic2.Color = ColorSequence.new(MainColor,SecondColor)
		partic2.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(0.3,0.5),NumberSequenceKeypoint.new(1,1)}
		partic2.Size = NumberSequence.new{NumberSequenceKeypoint.new(0,30),NumberSequenceKeypoint.new(1,0)}
		partic2.LockedToPart = true
		partic2.Lifetime = NumberRange.new(0.3)
		partic2.Rate = 100
		partic2.Rotation = NumberRange.new(-180,180)
		partic2.RotSpeed = NumberRange.new(360)
		partic2.Speed = NumberRange.new(0)
		partic2.SpreadAngle = Vector2.new(0,0)
		Sound("Sound",3582130077,attacked,10,1,0)
		coroutine.resume(coroutine.create(function()
			attackanim = true
			for i=0,75 do
				RootJoint.C0 = clerp(RootJoint.C0,cf(-0.5,3.7,0.2)*angles(math.rad(14),math.rad(38.5),math.rad(-0.3))*RootCF,.1)
				LH.C0 = clerp(LH.C0,cf(-1,-1,0)*angles(math.rad(-17.7),math.rad(0.3),math.rad(-5.1))*angles(math.rad(0),math.rad(-90),math.rad(0)),.1)
				RH.C0 = clerp(RH.C0,cf(1,-0.8,-0.4)*angles(math.rad(-10.5),math.rad(2.7),math.rad(7.2))*angles(math.rad(0),math.rad(90),math.rad(0)),.1)
				LW.C0 = clerp(LW.C0,cf(-1.4,0.8,0.2)*angles(math.rad(2.9),math.rad(27.9),math.rad(-33.3)),.1)
				RW.C0 = clerp(RW.C0,cf(1.3,0.6,-0.3)*angles(math.rad(-18.9),math.rad(48.9),math.rad(98.2)),.1)
				Torso.Neck.C0 = clerp(Torso.Neck.C0,cf(0.1,0.1,0.5)*angles(math.rad(-28.2),math.rad(-24.3),math.rad(5))*necko,.1)
				swait()
			end
			attackanim = false
		end))
		swait(50)
		Sound("CFrameSound",221920821,char,10,1,0,attacked.CFrame)
		Sound("CFrameSound",163680447,char,10,1,0,attacked.CFrame)
		Sound("CFrameSound",824687369,char,10,1,0,attacked.CFrame)
		Sound("CFrameSound",847061203,char,10,1,0,attacked.CFrame)
		pcall(function()
			attacked.Parent:BreakJoints()
		end)
		tweeneff("Sphere",attacked.CFrame,MainColor,Vector3.new(0,0,0),0,"Neon",0,{Transparency = 1, Size = Vector3.new(100,100,100)},0.5,"Quint","Out",0,false)
		tweeneff("Sphere",attacked.CFrame,MainColor,Vector3.new(1,1,1),0,"Neon",0,{Transparency = 1, Size = Vector3.new(101,101,101)},0.5,"Quint","Out",0,false)
		for i= 1,10 do
			for i,v in pairs({MainColor,SecondColor}) do
				local speen = attacked.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				local speen2 = attacked.CFrame*CFrame.Angles(math.rad(math.random(-180,180)),math.rad(math.random(-180,180)),math.rad(math.random(-180,180)))
				tweenmesh("Wind",speen,v,Vector3.new(0,.1,0),0,"Neon",0,{Transparency = 1, CFrame = speen*CFrame.Angles(0,math.rad(10000),0), Size = Vector3.new(math.random(70,200),0,math.random(70,200))},math.random(3,4),"Quint","Out",0,false)
				tweeneff("Sphere",speen2,v,Vector3.new(4,0,4),0,"Neon",0,{Transparency = 1, Size = Vector3.new(0,999,0)},1,"Linear","Out",0,false)
			end
		end
		attack = false
		swait(50)
		partic.Enabled = false
		partic2.Enabled = false
		swait(50)
		part:Destroy()
	end
end
