	partecle2.Rotation = NumberRange.new(0)
	partecle2.RotSpeed = NumberRange.new(-100,100)
	partecle2.Speed = NumberRange.new(0,1700)
	partecle2.SpreadAngle = Vector2.new(-180, 180)
	partecle2.Rate = 1000
	partecle2.Drag = 7
	local ray = Instance.new("ParticleEmitter",Torso)
	ray.ZOffset = -1
	ray.Texture = "rbxassetid://1053548563"
	ray.LightEmission = 1
	ray.LightInfluence = 0
	ray.Color = ColorSequence.new(MainColor,SecondColor)
	ray.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,1)}
	ray.Size = NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,300)}
	ray.LockedToPart = false
	ray.Lifetime = NumberRange.new(0.25,0.5)
	ray.Rate = 1600
	ray.Rotation = NumberRange.new(-180,180)
	ray.RotSpeed = NumberRange.new(0)
	ray.Speed = NumberRange.new(0)
	ray.SpreadAngle = Vector2.new(0,0)
	ray.Drag = 5
	spawn(function()
		for i=1,250/4,.75/2 do
			Sound("Sound",1368583274,Torso,1,1,0)
			Sound("Sound",1368637781,Torso,1,1,0)
			Sound("Sound",1368605755,Torso,1,1,0)
			Sound("Sound",763718160,Torso,1,1,0)
			Sound("Sound",782353443,Torso,1,1,0)
			swait(4)
		end
	end)
	for i=1,250/4,.75 do
		partecle2.Color = ColorSequence.new(Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1))
		ray.Color = ColorSequence.new(Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1))
		local a = Torso.CFrame*CFrame.Angles(math.rad(sine*1),math.rad(sine*2),math.rad(sine*3))
		local b = Torso.CFrame*CFrame.Angles(math.rad(sine*2),math.rad(sine*4),math.rad(sine*6))
		local c = Torso.CFrame*CFrame.Angles(math.rad(sine*4),math.rad(sine*8),math.rad(sine*12))
		local d = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*5),0)
		local e = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*10),0)
		local f = Torso.CFrame*CFrame.Angles(math.rad(90 + math.random(-5,5)),math.rad(sine*15),0)
		tweeneff("Sphere",Torso.CFrame,Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1),Vector3.new(i,i,i),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*1.5,i*1.5,i*1.5)},0.1,"Linear","Out",0,false)
		tweenmesh("ball",a,Color3.new(0,0,0),Vector3.new(i*1.5,i*1.5,i*1.5),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*1.75,i*1.75,i*1.75), CFrame = a*CFrame.Angles(math.rad(0),math.rad(360),math.rad(0))},0.1,"Linear","Out",0,false)
		tweenmesh("moreball",b,Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1),Vector3.new(i*1.75,i*1.75,i*1.75),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*2,i*2,i*2), CFrame = b*CFrame.Angles(0,math.rad(720),0)},0.1,"Linear","Out",0,false)
		tweenmesh("evenmoreball",c,Color3.new(0,0,0),Vector3.new(i*2,i*2,i*2),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*2.25,i*2.25,i*2.25), CFrame = c*CFrame.Angles(0,math.rad(1080),0)},0.1,"Linear","Out",0,false)
		tweenmesh("Swirl",d,Color3.fromHSV(sine/180-math.floor(sine/180),0.5,1),Vector3.new(i*3,20,i*3),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*3.25,20,i*3.25), CFrame = d*CFrame.Angles(0,math.rad(360),0)},0.1,"Linear","Out",0,false)
		tweenmesh("Wind",e,Color3.new(0,0,0),Vector3.new(i*3.25,20,i*3.25),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*3.5,20,i*3.5),CFrame = e*CFrame.Angles(0,math.rad(720),0)},0.1,"Linear","Out",0,false)
		tweenmesh("Wind2",f,Color3.new(0,0,0),Vector3.new(i*3.5,20,i*3.5),0,"Neon",0,{Transparency = 1, Size = Vector3.new(i*3.75,20,i*3.75), CFrame = f*CFrame.Angles(0,math.rad(1080),0)},0.1,"Linear","Out",0,false)
		swait(2)
	end
